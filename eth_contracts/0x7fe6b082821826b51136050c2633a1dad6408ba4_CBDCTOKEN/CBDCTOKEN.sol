    uint256 public constant initSupply = 10000000;
    address public constant _dev = 0x376acE34EC188F1dd50210d4B081E12f8772B7EA;
    address owner;
    
    constructor(string _name, string _symbol, uint8 _decimals) public ERC20Detailed(_name, _symbol, _decimals){
