// * SPDX-License-Identifier: MIT

pragma solidity ^0.8.17;








contract DistributeAddress is Ownable {
    using SafeMath for uint256;

    address public executionAddress;
    address public stakingnAddress;
    address public buyBackAddress;
    address public advisorAddress;
    address public devAddress;
    address public partnerAddress;

    uint256 public distributePercentOne = 50;
    uint256 public distributePercentTwo = 20;
    uint256 public distributePercentThree = 10;
    uint256 public distributePercentFour = 10;
    uint256 public distributePercentFive = 5;
    uint256 public distributePercentSix = 5;

    receive() external payable {
        distributeDividends();
    }

    event SentBNB(address usr, uint256 amount);

    function distributeDividends() private {
        uint256 initialBalance = address(this).balance;

        if (initialBalance > 0 ) {
            uint256 execution = initialBalance.mul(distributePercentOne).div(100);
            uint256 staking = initialBalance.mul(distributePercentTwo).div(100);
            uint256 buyBack = initialBalance.mul(distributePercentThree).div(100);
            uint256 advisor = initialBalance.mul(distributePercentFour).div(100);
            uint256 dev =  initialBalance.mul(distributePercentFive).div(100);
            uint256 partner = initialBalance.mul(distributePercentSix).div(100);

            (bool sent, ) = address(executionAddress).call{value: execution}("");
            if(sent) {
                emit SentBNB(address(executionAddress), execution);
            
            }
            (sent,) = address(stakingnAddress).call{value: staking}("");
            if(sent) {
                emit SentBNB(address(stakingnAddress), staking);
            
            }
            (sent,) = address(buyBackAddress).call{value: buyBack}("");
            if(sent) {
                emit SentBNB(address(buyBackAddress), buyBack);
            
            }
            (sent,) = address(advisorAddress).call{value: advisor}("");
            if(sent) {
                emit SentBNB(address(advisorAddress), advisor);
            
            }
            (sent,) = address(devAddress).call{value: dev}("");
            if(sent) {
                emit SentBNB(address(devAddress), dev);
            
            }
                       (sent,) = address(partnerAddress).call{value: partner}("");
            if(sent) {
                emit SentBNB(address(partnerAddress), partner);
            
            }
        }
     }

     function setPercentDistribute(uint256 _distributePercentOne, uint256 _distributePercentTwo, 
     uint256 _distributePercentThree, uint256 _distributePercentFour, uint256 _distributePercentFive,
     uint256 _distributePercentSix) external onlyOwner {
        distributePercentOne = _distributePercentOne;
        distributePercentTwo = _distributePercentTwo;
        distributePercentThree = _distributePercentThree;
        distributePercentFour = _distributePercentFour;
        distributePercentFive = _distributePercentFive;
        distributePercentSix = _distributePercentSix;
     } 
     function addAddressDividends(address _executionAddress, address _stakingnAddress, address _buyBackAddress,
     address _advisorAddress, address _devAddress, address _partnerAddress) external onlyOwner  {
         executionAddress = _executionAddress;
         stakingnAddress = _stakingnAddress;
         buyBackAddress = _buyBackAddress;
         advisorAddress = _advisorAddress;
         devAddress = _devAddress;
         partnerAddress = _partnerAddress;
     }
    function getBNB(address admin)external onlyOwner {
        uint256 balance = address(this).balance;
        if(balance > 0) {
            (bool success, ) = admin.call{ value: balance }("");
            require(success, "Address: unable to send value, recipient may have reverted");
        }
    }
}