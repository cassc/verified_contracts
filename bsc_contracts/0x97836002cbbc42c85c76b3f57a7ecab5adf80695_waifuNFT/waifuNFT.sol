        }
        return owned[msg.sender];
    }

    function saleSwitch() public onlyOwner returns (bool) {
        saleStart = !saleStart;
        return saleStart;
    }

    function clearStuckBalance() public payable onlyOwner {
        payable(owner).call{value: address(this).balance};
    }

    function checkOwner(address _user)
        external
        view
        returns (uint256[] memory)
    {
        require(_user != address(0), "Invalid address!");
        return owned[_user];
    }

    function nftLinks(address _user) external view returns (string[] memory) {
        require(_user != address(0), "Invalid address!");
        return metadata[_user];
    }

    function changeOwnership(address _newOwner)
        public
        onlyOwner
        returns (address)
    {
        require(_newOwner != address(0), "Invalid address!");
        require(_newOwner != owner, "Same Owner!");

        owner = _newOwner;
        return owner;
    }

    function changeBaseUrl(string memory _newBase)
        public
        onlyOwner
        returns (string memory)
    {
        baseUrl = _newBase;
        return baseUrl;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        string memory baseURI = baseUrl;
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, tokenId.toString(), ".json"))
                : "";
    }
}