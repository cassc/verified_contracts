/**
 * @title NFT
 */
abstract contract NFT721 is ERC721, INFT, Comn {
    uint256 internal _nftAmount; // nft ID
    uint256 internal _burnAmount; // nft brun amount
    mapping(address => uint256[]) internal _owneds; // user's pet
    mapping(uint256 => uint256) internal _ownedIndexs; // pledge ID => index of owneds
    mapping(address => uint256) internal _nonces; // approve nonce
    mapping(uint256 => bool) internal _lockeds; // nft lock state
    mapping(address => uint256) public allowMintSizes; // allow the master mint batchsize

    /**
     * @dev generate NFT ID
     * nfttype(8)+id(56)
     * hero 1  |  arm 2  |  passcard 3
     */
    function _newID(uint8 btype) internal returns (uint256) {
        return (uint256(btype) << 56) + (++_nftAmount);
    }

    /**
     * @dev get the maximum number of NFT
     */
    function amount() public view virtual returns (uint256) {
        return _nftAmount;
    }

    /**
     * @dev get all NFT of the user
     */
    function allsOf(address account)
        public
        view
        virtual
        returns (uint256[] memory)
    {
        return _owneds[account];
    }

    /**
     * @dev approve for contract
     */
    function permit(
        address to,
        uint256 tokenId,
        bytes memory signature
    ) public override {
        address owner = ERC721.ownerOf(tokenId);
        // signature check
        bytes32 digest = ECDSA.toEthSignedMessageHash(
            bytes(permitMsg(to, tokenId))
        );
        address recoveredSigner = ECDSA.recover(digest, signature);
        require(recoveredSigner == owner, "signature error");

        _nonces[owner] += 1;
        _approve(to, tokenId);
    }

    /**
     * @dev get approve signature message
     */
    function permitMsg(address to, uint256 tokenId)
        public
        view
        returns (string memory)
    {
        address owner = ERC721.ownerOf(tokenId);
        return
            string(
                abi.encodePacked(
                    "Approve ",
                    symbol(),
                    " ",
                    Strings.toString(tokenId),
                    " to ",
                    addressToString(to),
                    " with ",
                    Strings.toString(_nonces[owner] + 1)
                )
            );
    }

    /**
     * @dev address to string
     */
    function addressToString(address _address)
        public
        pure
        returns (string memory)
    {
        bytes32 _bytes = bytes32(uint256(uint160(_address)));
        bytes memory HEX = "0123456789abcdef";
        bytes memory _string = new bytes(42);
        _string[0] = "0";
        _string[1] = "x";
        for (uint256 i = 0; i < 20; i++) {
            _string[2 + i * 2] = HEX[uint8(_bytes[i + 12] >> 4)];
            _string[3 + i * 2] = HEX[uint8(_bytes[i + 12] & 0x0f)];
        }
        return string(_string);
    }

    /**
     * @dev get approve nonce
     */
    function approveNonce(address account) public view returns (uint256) {
        return _nonces[account] + 1;
    }

    /**
     * @dev See {IERC721Enumerable-totalSupply}.
     */
    function totalSupply() public view virtual returns (uint256) {
        return _nftAmount - _burnAmount;
    }

    /**
     * @dev set the max size mint this for a address
     */
    function allowMintFor(address account, uint256 _amount)
        public
        virtual
        onlyAuditor
    {
        bytes memory bkdata = abi.encodeWithSignature(
            "_allowMintFor(address,uint256)",
            account,
            _amount
        );
        string memory descr = string(
            abi.encodePacked(
                symbol(),
                " allow mint ",
                Strings.toString(_amount),
                " for ",
                addressToString(account)
            )
        );
        Admin.audit(bkdata, descr);
    }

    /**
     * @dev set the max size mint this for a address
     */
    function _allowMintFor(address account, uint256 _amount)
        public
        virtual
        onlyAdmin
    {
        allowMintSizes[account] += _amount;
    }

    /**
     * cash the balance
     */
    function withdraw(
        address token,
        uint256 _amount,
        address account
    ) public virtual onlyAuditor {
        bytes memory bkdata = abi.encodeWithSignature(
            "_withdraw(address,uint256,address)",
            token,
            _amount,
            account
        );
        string memory descr = string(
            abi.encodePacked(
                symbol(),
                " withdraw ",
                Strings.toString(_amount / (10**ICoin(token).decimals())),
                " to ",
                addressToString(account)
            )
        );
        Admin.audit(bkdata, descr);
    }

    /**
     * cash the balance
     */
    function _withdraw(
        address token,
        uint256 _amount,
        address account
    ) public virtual onlyAdmin returns (bool) {
        if (token == address(0)) {
            payable(account).transfer(_amount);
        } else {
            ICoin(token).transfer(account, _amount);
        }
        return true;
    }

    /**
     * @dev See {ERC721-_beforeTokenTransfer}.
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
        if (tokenId != 0 && from != to) {
            require(batchSize == 1, "consecutive transfers not supported");
            require(_lockeds[tokenId] == false);
            if (from != address(0)) {
                arrayDel(_owneds[from], _ownedIndexs[tokenId]);
                delete _ownedIndexs[tokenId];
            }

            if (to != address(0)) {
                _owneds[to].push(tokenId);
                _ownedIndexs[tokenId] = _owneds[to].length - 1;
            } else {
                _burnAmount += 1;
            }
        }
    }

    /**
     * @dev random a integer 0 - max
     * must different seed in a call
     */
    function _rand(uint256 max, uint256 seed) internal view returns (uint256) {
        if (max == 0) {
            return 0;
        }
        uint256 _value = uint256(
            keccak256(
                abi.encodePacked(seed, _nftAmount, msg.sender, block.timestamp)
            )
        );
        return _value % (max + 1);
    }

    /**
     * delete an array element
     * must store
     */
    function arrayDel(uint256[] storage _arr, uint256 index) internal {
        if (index < _arr.length - 1) {
            _arr[index] = _arr[_arr.length - 1];
        }
        _arr.pop();
    }
}




/**
 * @title clash of nft passcard
 * proxy at bscmain 0x5b936dCb112eeD8EBAE436d212342eFeE19E1859
 * proxy at bsctest 0x8865BC838E5AA1116dDE809647E3486B7164b420
 */
contract PasscardNFT is NFT721, IPasscard {
    mapping(uint256 => Passcard) internal _frames; // nft medias
    mapping(uint256 => uint256) internal _ucToMaps; // nftid map to offline id, nftid => toid
    mapping(uint256 => uint256) internal _ucFromMaps; // nftid map from offline id,  fromid => nftid
    mapping(address => uint256) public prices; // mint one need token price, token => price
    uint256 fourStartMintedSize = 0; // the four start passcard haven minted size, must < 2000

    event UpStar(uint256 nftID, uint8 star, uint256[] otherNFT);  //  update star event

    constructor() ERC721("", "") {}

    /**
     * @dev See {IERC721Metadata-name}.
     */
    function name() public view virtual override returns (string memory) {
        return "ClashOfNFT Passcard";
    }

    /**
     * @dev See {IERC721Metadata-symbol}.
     */
    function symbol() public view virtual override returns (string memory) {
        return "PASSCARD";
    }

    /**
     * @dev mint one must price
     */
    function setPrice(address token, uint256 price) public onlyMaster {
        prices[token] = price;
    }

    /**
     * @dev mint arm for account with star
     */
    function _mintFor(address account, Passcard memory frame)
        private
        returns (uint256)
    {
        require(frame.star > 0 && frame.star < 5);
        if (frame.star == 4) {
            require(fourStartMintedSize < 2000, "Four Star over 2000");
            fourStartMintedSize += 1;
        }
        uint256 id = _newID(3);
        _mint(account, id);
        _frames[id] = frame;
        return id;
    }

    /**
     * @dev mint with erc20
     * Requires erc20 token
     */
    function mintForMe(
        uint8 star,
        address token,
        bytes memory signature
    ) public payable {
        require(token != address(0), "Invalid token");
        uint256 price = mintCoin(star, token);
        ICoin coin = ICoin(token);
        coin.permit(msg.sender, address(this), price, signature);
        coin.transferFrom(msg.sender, address(this), price);
        _mintFor(
            msg.sender,
            Passcard({star: star, mintTime: uint64(block.timestamp)})
        );
    }

    /**
     * @dev mint with native or erc20 token
     * erc20 will approve it before
     */
    function mintForMe(uint8 star, address token) public payable {
        uint256 price = mintCoin(star, token);
        if (token == address(0)) {
            require(msg.value == price, "Invalid price");
        } else {
            ICoin(token).transferFrom(msg.sender, address(this), price);
        }
        _mintFor(
            msg.sender,
            Passcard({star: star, mintTime: uint64(block.timestamp)})
        );
    }

    /**
     * @dev mint a passcard for player
     */
    function mintForMe(bytes memory signature, uint256 ucID, uint8 star) public virtual returns(uint256) {
        require(_ucFromMaps[ucID] == 0, "Already");
        bytes32 digest = keccak256(abi.encode(address(this), msg.sender, keccak256("mintForMe(uint256,uint8)"), ucID, star));
        address recoveredSigner = ECDSA.recover(digest, signature);
        require(recoveredSigner == Conf.broker());

        uint256 nftID = _mintFor(msg.sender, Passcard({star: star, mintTime: uint64(block.timestamp)}));

        _ucFromMaps[ucID] = nftID;
        _ucToMaps[nftID] = ucID;
        return nftID;
    }

    /**
     * @dev Mint nft to account
     */
    function mintTo(
        address account,
        uint8 star,
        uint8 _amount
    ) public onlyAuditor {
        require(allowMintSizes[account] >= _amount, "Allow error");
        allowMintSizes[account] -= _amount;
        for (uint256 i = 0; i < _amount; i++) {
            _mintFor(
                account,
                Passcard({star: star, mintTime: uint64(block.timestamp)})
            );
        }
    }

    /**
     * @dev can upgrade star level & get the con needed
     */
    function mintCoin(uint8 star, address token) public view returns (uint256) {
        require(star < 5 && star > 0, "Invalid star");
        require(prices[token] > 0, "Invalid token");

        uint16[4] memory _prices = [uint16(10), 20, 50, 100];
        uint256 price = _prices[star - 1] * prices[token];

        return price;
    }

    /**
     * @dev Update grade the passcard star
     */
    function upStar(uint256 nftID, uint256[] memory otherID) public {
        require(_frames[nftID].star == 2, "Invalid star");
        require(otherID.length > 0, "Least two NFT");
        uint256 rate = 0;
        uint256 len = otherID.length;
        uint256 seed = nftID;
        for (uint256 i = 0; i < len; i++) {
            uint256 id = otherID[i];
            require(_frames[id].star == 1, "Invalid star");
            _burn(otherID[i]);
            rate += 10;
            nftID += id;
        }
        if (_rand(99, seed) < rate) {
            _frames[nftID].star = 3;
        }

        emit UpStar(nftID, 3, otherID);
    }

    /**
     * @dev get meta info
     */
    function infoOf(uint256 nftID)
        public
        view
        virtual
        override
        returns (Passcard memory)
    {
        return _frames[nftID];
    }

    /**
     * @dev get star level
     */
    function starOf(uint256 nftID) public view virtual returns (uint256) {
        return _frames[nftID].star;
    }

    /**
     * @dev NFT URI address
     */
    function _baseURI() internal pure virtual override returns (string memory) {
        return "https://api.clashofnft.io/passcard/item/";
    }
}