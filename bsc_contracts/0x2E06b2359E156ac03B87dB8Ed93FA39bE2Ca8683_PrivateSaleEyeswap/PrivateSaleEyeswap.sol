contract PrivateSaleEyeswap {
    uint256 public slot = 0;
    uint256 public max_slot = 10;
    uint256 public price = 1500 ether;
    address development_1 = 0xDe371956D1d879b8fbd963bAe64D4291a3bb44CE; // model future upgrades and bot server setup and so on
    address development_2 = 0xdab8970B41D70B8Ae5ff88D3Bf2281ae06C4B775; // Coin signals and Telegram bot setup and guide
    uint256 percentage = 75;
    address public TokenAddress;
    IERC20 public BUSD;

    constructor() {
        TokenAddress = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56; // BUSD-MainNet
        BUSD = IERC20(TokenAddress);
    }
    struct user_whitelist_address {
        address addr;
        string telegram;
        bool purchase;
    }

    mapping(address => user_whitelist_address) public whitelist;


    function buySlot(uint256 _amtx,string memory telegram) public {
        require(!whitelist[msg.sender].purchase);
        require(_amtx == price);
        require(max_slot>=slot, "Private-Sale Over");
        uint256 dev1 = PercentageViewer(_amtx);
        uint256 remindPer = SafeMath.sub(_amtx,dev1);
        BUSD.transferFrom(msg.sender,development_1,dev1);
        BUSD.transferFrom(msg.sender,development_2,remindPer);
        whitelist[msg.sender] = user_whitelist_address(msg.sender,telegram,true);
        slot = slot + 1;
    }

    function PercentageViewer(uint256 _amtx) public view returns(uint256) {
        uint256 value1 = SafeMath.mul(_amtx,percentage);
        return SafeMath.div(value1,100);
    }

}