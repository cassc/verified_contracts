        AssetStakerStorage storage _storage = assetStakerStorage();

        require(
            slot == ANY_SLOT ||
                (slot <= _storage.slotSize &&
                    !_storage.stakedSlots[msg.sender].contains(slot)),
            "Incorrect slot"
        );

        uint256 _slot = slot == ANY_SLOT ? _findSlot() : slot;
        _storage.slots[msg.sender][_slot] = AssetStake(
            block.timestamp,
            assetId,
            bonus
        );
        _storage.stakedSlots[msg.sender].add(_slot);
