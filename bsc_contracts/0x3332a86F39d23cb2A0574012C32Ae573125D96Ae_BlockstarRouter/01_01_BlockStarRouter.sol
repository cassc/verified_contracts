contract BlockstarRouter is IBlockstarRouter02,OwnableUpgradeable {
    using SafeMath for uint256;
    address payable public recoveryWallet;
    address payable public opsWallet;
    address payable public reflectionsWallet;
    address payable public advisoryWallet;
    address payable public liquidityWallet;
    uint256 public _nextResetTimestamp;
    uint256 public _volume;
    uint256 public normalFees;
    uint256 public backdraftFees;
    
    struct Fee {
        uint16 recoveryFee;
        uint16 opsFee;
        uint16 reflectionsFee;
        uint16 advisoryFee;
        uint16 liquidityFee;
    }

    Fee public fee;

    Fee public backdraftFee;

    struct UserSellAmount {
        uint256 amount;
        uint256 lastUpdate;
    }

    uint256 private _advisoryFee;
    uint256 private _recoveryFee;
    uint256 private _opsFee;
    uint256 private _reflectionsFee;
    uint256 private _liquidityFee;
    mapping(address => bool) public _isBlackListed;
    mapping(address => bool) private _isExcludedFromFee;
    mapping(address => bool) private _isExcluded;
    address[] private _excluded;
    mapping(address => UserSellAmount) public userSellVolume;
    uint256 public resetDuration;
    uint256 public _previousResetTimestamp;
    uint256 public _previousVolume;
    uint256 public backdraftVolumePercent;
    uint256 public antiBotBlocks;

    address public override factory;
    address public override WETH;
    address public tokenAddess;
    bool public paused;

    modifier ensure(uint256 deadline) {
        require(deadline >= block.timestamp, "BlockStarRouter: EXPIRED");
        _;
    }

    receive() external payable {
        assert(msg.sender == WETH); // only accept ETH via fallback from the WETH contract
    }

    constructor(address _factory, address _WETH) public {
        __Ownable_init();
        factory = _factory;
        WETH = _WETH;

        tokenAddess = 0x4033F69f486debe311D804E116a29DD90054C7D0;
        resetDuration = 86400;
        backdraftVolumePercent = 5;
        recoveryWallet = payable(0xf57f48F545dcA5ca4353A3CBBADB7a24C32BFd0B);
        opsWallet = payable(0x79CA9c41c94f8c78240Aa361D1028e21EC6e8E3B);
        reflectionsWallet = payable(0xB2434bf5176b80fB43254dD02fE438069CA6315D);
        advisoryWallet = payable(0xa85cA31006c8C6ACC5F5E06e4a8E55569510d43b);
        liquidityWallet = payable(0xe0598B2D87Dbf90496420317379d70F7fe7e4Fb6);
        normalFees = 1000;
        backdraftFees = 3000;
       
        fee.recoveryFee = 20;
        fee.opsFee = 20;
        fee.reflectionsFee = 40;
        fee.advisoryFee = 10;
        fee.liquidityFee = 10;

        backdraftFee.recoveryFee = 20;
        backdraftFee.opsFee = 20;
        backdraftFee.reflectionsFee = 40;
        backdraftFee.advisoryFee = 10;
        backdraftFee.liquidityFee = 10;

        _nextResetTimestamp = block.timestamp + resetDuration;
         _previousVolume = 500000000000000000000000000;
    }

    // **** ADD LIQUIDITY ****
    function _addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin
    ) internal virtual returns (uint amountA, uint amountB) {
        // create the pair if it doesn't exist yet
        if (IBlockstarFactory(factory).getPair(tokenA, tokenB) == address(0)) {
            IBlockstarFactory(factory).createPair(tokenA, tokenB);
        }
        (uint reserveA, uint reserveB) = BlockstarLibrary.getReserves(factory, tokenA, tokenB);
        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountADesired, amountBDesired);
        } else {
            uint amountBOptimal = BlockstarLibrary.quote(amountADesired, reserveA, reserveB);
            if (amountBOptimal <= amountBDesired) {
                require(amountBOptimal >= amountBMin, 'BlockstarRouter: INSUFFICIENT_B_AMOUNT');
                (amountA, amountB) = (amountADesired, amountBOptimal);
            } else {
                uint amountAOptimal = BlockstarLibrary.quote(amountBDesired, reserveB, reserveA);
                assert(amountAOptimal <= amountADesired);
                require(amountAOptimal >= amountAMin, 'BlockstarRouter: INSUFFICIENT_A_AMOUNT');
                (amountA, amountB) = (amountAOptimal, amountBDesired);
            }
        }
    }
    function addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external virtual override ensure(deadline) returns (uint amountA, uint amountB, uint liquidity) {
        (amountA, amountB) = _addLiquidity(tokenA, tokenB, amountADesired, amountBDesired, amountAMin, amountBMin);
        address pair = BlockstarLibrary.pairFor(factory, tokenA, tokenB);
        TransferHelper.safeTransferFrom(tokenA, msg.sender, pair, amountA);
        TransferHelper.safeTransferFrom(tokenB, msg.sender, pair, amountB);
        liquidity = IBlockstarPair(pair).mint(to);
    }
    function addLiquidityETH(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external virtual override payable ensure(deadline) returns (uint amountToken, uint amountETH, uint liquidity) {
        (amountToken, amountETH) = _addLiquidity(
            token,
            WETH,
            amountTokenDesired,
            msg.value,
            amountTokenMin,
            amountETHMin
        );
        address pair = BlockstarLibrary.pairFor(factory, token, WETH);
        TransferHelper.safeTransferFrom(token, msg.sender, pair, amountToken);
        IWETH(WETH).deposit{value: amountETH}();
        assert(IWETH(WETH).transfer(pair, amountETH));
        liquidity = IBlockstarPair(pair).mint(to);
        // refund dust eth, if any
        if (msg.value > amountETH) TransferHelper.safeTransferETH(msg.sender, msg.value - amountETH);
    }

    // **** REMOVE LIQUIDITY ****
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountA, uint amountB) {
        address pair = BlockstarLibrary.pairFor(factory, tokenA, tokenB);
        IBlockstarPair(pair).transferFrom(msg.sender, pair, liquidity); // send liquidity to pair
        (uint amount0, uint amount1) = IBlockstarPair(pair).burn(to);
        (address token0,) = BlockstarLibrary.sortTokens(tokenA, tokenB);
        (amountA, amountB) = tokenA == token0 ? (amount0, amount1) : (amount1, amount0);
        require(amountA >= amountAMin, 'BlockstarRouter: INSUFFICIENT_A_AMOUNT');
        require(amountB >= amountBMin, 'BlockstarRouter: INSUFFICIENT_B_AMOUNT');
    }
    function removeLiquidityETH(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountToken, uint amountETH) {
        (amountToken, amountETH) = removeLiquidity(
            token,
            WETH,
            liquidity,
            amountTokenMin,
            amountETHMin,
            address(this),
            deadline
        );
        TransferHelper.safeTransfer(token, to, amountToken);
        IWETH(WETH).withdraw(amountETH);
        TransferHelper.safeTransferETH(to, amountETH);
    }
    function removeLiquidityWithPermit(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountA, uint amountB) {
        address pair = BlockstarLibrary.pairFor(factory, tokenA, tokenB);
        uint value = approveMax ? uint(-1) : liquidity;
        IBlockstarPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        (amountA, amountB) = removeLiquidity(tokenA, tokenB, liquidity, amountAMin, amountBMin, to, deadline);
    }
    function removeLiquidityETHWithPermit(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountToken, uint amountETH) {
        address pair = BlockstarLibrary.pairFor(factory, token, WETH);
        uint value = approveMax ? uint(-1) : liquidity;
        IBlockstarPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        (amountToken, amountETH) = removeLiquidityETH(token, liquidity, amountTokenMin, amountETHMin, to, deadline);
    }

    // **** REMOVE LIQUIDITY (supporting fee-on-transfer tokens) ****
    function removeLiquidityETHSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountETH) {
        (, amountETH) = removeLiquidity(
            token,
            WETH,
            liquidity,
            amountTokenMin,
            amountETHMin,
            address(this),
            deadline
        );
        TransferHelper.safeTransfer(token, to, IERC20Upgradeable(token).balanceOf(address(this)));
        IWETH(WETH).withdraw(amountETH);
        TransferHelper.safeTransferETH(to, amountETH);
    }
    function removeLiquidityETHWithPermitSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountETH) {
        address pair = BlockstarLibrary.pairFor(factory, token, WETH);
        uint value = approveMax ? uint(-1) : liquidity;
        IBlockstarPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        amountETH = removeLiquidityETHSupportingFeeOnTransferTokens(
            token, liquidity, amountTokenMin, amountETHMin, to, deadline
        );
    }

    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(uint[] memory amounts, address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = BlockstarLibrary.sortTokens(input, output);
            uint amountOut = amounts[i + 1];
            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOut) : (amountOut, uint(0));
            address to = i < path.length - 2 ? BlockstarLibrary.pairFor(factory, output, path[i + 2]) : _to;
            IBlockstarPair(BlockstarLibrary.pairFor(factory, input, output)).swap(
                amount0Out, amount1Out, to, new bytes(0)
            );
        }
    }
    
    function swapExactETHForTokens(uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        payable
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(!paused , "Trading not enabled!!");
        require(path[0] == WETH, "BlockstarRouter: INVALID_PATH");
        require(path[1] == tokenAddess, "BlockstarRouter: INVALID_PATH");

        require(
            !_isBlackListed[msg.sender] && !_isBlackListed[to],
            "Account is blacklisted"
        );
        uint256 tFees = 0;
        bool takeFee = true;
        // address[] calldata cpath = [WETH,tokenAddess];
        if (_isExcludedFromFee[msg.sender]) {
            takeFee = false;
        }

        if (takeFee) {
            tFees = normalFees;
            setFee();
        }
        uint256 amount = msg.value;
        uint256 amountOutFees = amountOutMin.mul(tFees).div(10000);
        uint256 amountOuts = amountOutMin.sub(amountOutFees);
        uint256 fees = amount.mul(tFees).div(10000);
        uint256 afterFees = amount.sub(fees);

        _takeFees(fees);

        amounts = BlockstarLibrary.getAmountsOut(factory, afterFees, path);
        require(
            amounts[amounts.length - 1] >= amountOuts,
            "BlockstarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        IWETH(WETH).deposit{value: amounts[0]}();
        assert(
            IWETH(WETH).transfer(
                BlockstarLibrary.pairFor(factory, WETH, tokenAddess),
                amounts[0]
            )
        );
        _swap(amounts, path, to);

        IBlockstarRouterBuyBack(reflectionsWallet).buyBack();
    }
    
    function swapExactTokensForETH(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(!paused , "Trading not enabled!!");
        require(path[path.length - 1] == WETH, "BlockstarRouter: INVALID_PATH");
        require(path[0] == tokenAddess, "BlockstarRouter: INVALID_PATH");
        require(
            !_isBlackListed[msg.sender] && !_isBlackListed[to],
            "Account is blacklisted"
        );
        uint256 tFees = normalFees;
        bool takeFee = true;
        

        if (_isExcludedFromFee[msg.sender]) {
            takeFee = false;
            tFees = 0;
            setFee();
        }

        if (takeFee) {
            setFee();
            tFees = normalFees;
            if (_nextResetTimestamp >= block.timestamp) {
                _volume += amountIn;

                userSellVolume[msg.sender].amount = userSellVolume[msg.sender]
                    .lastUpdate <= _nextResetTimestamp - resetDuration
                    ? amountIn
                    : userSellVolume[msg.sender].amount + amountIn;
            } else {
                do {
                    _previousResetTimestamp = _nextResetTimestamp;
                    _nextResetTimestamp += resetDuration;
                } while (_nextResetTimestamp < block.timestamp);

                _previousVolume = _volume;
                _volume = amountIn;
                userSellVolume[msg.sender].amount = amountIn;
            }
            userSellVolume[msg.sender].lastUpdate = block.timestamp;
            if (
                userSellVolume[msg.sender].amount >
                (_previousVolume * backdraftVolumePercent) / 100
            ) {
                setBackDraftFee();
                tFees = backdraftFees;
            }
        }

        amounts = BlockstarLibrary.getAmountsOut(factory, amountIn, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "BlockstarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            BlockstarLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, address(this));
        IWETH(WETH).withdraw(amounts[amounts.length - 1]);

        uint256 amount = amounts[amounts.length - 1];
        uint256 fees = amount.mul(tFees).div(10000);
        uint256 afterFees = amount.sub(fees);
        
        _takeFees(fees);
        TransferHelper.safeTransferETH(to, afterFees);
        IBlockstarRouterBuyBack(reflectionsWallet).buyBack();
    }

     function swapExactETHBuyBack(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[0] == WETH, "BlockstarRouter: INVALID_PATH");
        require(path[1] == tokenAddess, "BlockstarRouter: INVALID_PATH");
        require(msg.sender == reflectionsWallet, "Not Allowed Trade!!!");

        amounts = BlockstarLibrary.getAmountsOut(factory, msg.value, path);
        require(
            amounts[amounts.length - 1] >= amountOutMin,
            "BlockstarRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        IWETH(WETH).deposit{value: amounts[0]}();
        assert(
            IWETH(WETH).transfer(
                BlockstarLibrary.pairFor(factory, path[0], path[1]),
                amounts[0]
            )
        );
        _swap(amounts, path, to);
    }
    
    
