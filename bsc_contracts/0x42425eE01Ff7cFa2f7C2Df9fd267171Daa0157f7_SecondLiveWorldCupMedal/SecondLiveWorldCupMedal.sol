// File: contracts/worldcup/SecondLiveWorldCupMedal.sol

pragma solidity ^0.8.0;








contract SecondLiveWorldCupMedal is ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    
    bool private initialized;

    uint256 public startTime;
    uint256 public endTime;

    address public babt;
    IGalxe public galxe;
    uint256 public galxeCid; // 8350
    ISecondLiveNFT public secondLive;

    // oatId -> bool
    mapping(uint256 => bool) public galxeIdClaimed;
    // user -> bool
    mapping(address => bool) public claimed;

    bytes32 public merkleRoot;

    event GalxeClaim(address sender, uint256 oatId);
    event Claim(address sender, uint256 oatId);
    event UpdateWhiteList(bytes32 merkleRoot);
    event UpdateTime(uint256 startTime, uint256 endTime);

    function initialize(
        address _owner,
        uint256 _startTime,
        uint256 _endTime,
        ISecondLiveNFT _secondLive,
        address _babt,
        IGalxe _galxe,
        uint256 _galxeCid,
        bytes32 _merkleRoot) external {
        require(!initialized, "initialize: Already initialized!");
        _transferOwnership(_owner);
        
        startTime = _startTime;
        endTime = _endTime;

        secondLive = _secondLive;
        babt = _babt;
        galxe = _galxe;
        galxeCid = _galxeCid;
        merkleRoot = _merkleRoot;
        
        initialized = true;
    }

    function babtCanClaim(address sender) public view returns (bool){
        return IERC721(babt).balanceOf(sender)>0?true:false;
    }

    function galexCanClaim(address sender, uint256 oatId) public view returns (bool){
        uint256 cid = galxe.cid(oatId);
        if (cid == galxeCid) {
            address owner_ = galxe.ownerOf(oatId);
            if (sender == owner_) {
                if (galxeIdClaimed[oatId]) {
                    return false;
                } else {
                    return true;
                }
            } else {
                return false;
            }
        }else {
            return false;
        }
    }

    function merkleCanClaim(
        address sender,
        uint256 addressId,
        bytes32[] memory merkleProof) public view returns (bool){
        bytes32 node = keccak256(abi.encodePacked(addressId, sender));
        return MerkleProof.verify(merkleProof, merkleRoot, node);
    }

    function claim(
        uint256 oatId,
        uint256 addressId,
        bytes32[] memory merkleProof)
    external nonReentrant {
        require(block.timestamp >= startTime, "not start!");
        require(endTime >= block.timestamp, "have end!");
        require(!claimed[msg.sender], "have claimed!");
        
        bool babtClaim = babtCanClaim(msg.sender);
        bool galexClaim = galexCanClaim(msg.sender, oatId);
        if(galexClaim) {
            galxeIdClaimed[oatId] = true;
            emit GalxeClaim(msg.sender, oatId);
        }
        bool merkleClaim = merkleCanClaim(msg.sender, addressId, merkleProof);
        require((babtClaim || galexClaim || merkleClaim), "Ineligible");

        claimed[msg.sender] = true;
        
        bytes memory _data = '0x0';
        ISecondLiveNFT.Attribute memory attribute;
        attribute.rule = 6;
        attribute.format = 115;
        attribute.extra = 125;
        attribute.quality = 0;
        secondLive.mint(msg.sender, 125, 1, _data, attribute);

        emit Claim(msg.sender, oatId);

    }

    function updateMerkleRoot(bytes32 _merkleRoot) external onlyOwner{
        merkleRoot = _merkleRoot;
        emit UpdateWhiteList(_merkleRoot);
    }

    function updateTime(
        uint256 _startTime,
        uint256 _endTime) external onlyOwner{
        
        startTime = _startTime;
        endTime = _endTime;
        emit UpdateTime(_startTime, _endTime);
    }
}