contract Tlv is ERC20, ERC20Burnable, Pausable, Ownable {

    address payable public _marketingAddress =
        payable(0xf669Cfc5f6dd1ffdd1A526B1F2d641C697a447Fa);
    address payable public _reserveAddress =
        payable(0xFF821FCEB0Ad8Ea1e2a84696C2Bd5793e3a25A8D);
    address payable public _burnAddress      = payable(0x000000000000000000000000000000000000dEaD);

    mapping (address => bool) public _isExcludedFromAntiWhale;
    mapping (address => bool) public _isExcludedFromTax;
    mapping (address => bool) public _isExcludedFromMaxTxAmount;
    uint256 public _maxBalance = 50000 * 10**decimals();
    uint256 public _maxBuyAmount = 50000 * 10**decimals();
    uint256 public _maxSellAmount = 50000 * 10**decimals();
    uint256 public _buyTax = 0;
    uint256 public _sellTax = 10;
    uint256 public _sellReservePercent = 5;
    uint256 public _sellMarketingPercent = 5;
    uint256 public _buyReservePercent = 0;
    uint256 public _buyMarketingPercent = 0;
    bool public _isLimited;

    IPancakeRouter02 public pancakeRouter;
    address public pancakePair;

    address public constant USDT = 0x55d398326f99059fF775485246999027B3197955; //change this

    constructor() ERC20("Tycoon Las Vegas", "TLV") {
        IPancakeRouter02 _pancakeRouter = IPancakeRouter02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E
        );
        // IPancakeRouter02 _pancakeRouter = IPancakeRouter02(
        //     0xD99D1c33F9fC3444f8101754aBC46c52416550D1
        // );
        pancakePair = IPancakeFactory(_pancakeRouter.factory()).createPair(
            address(this),
            USDT
        );
        pancakeRouter = _pancakeRouter;
        _mint(msg.sender, 30000000 * 10**decimals());

        _isExcludedFromAntiWhale[owner()]                   = true;
        _isExcludedFromAntiWhale[address(this)]             = true;
        _isExcludedFromAntiWhale[pancakePair]               = true;
        _isExcludedFromAntiWhale[address(_pancakeRouter)]   = true;
        _isExcludedFromAntiWhale[_burnAddress]              = true;
        _isExcludedFromAntiWhale[_marketingAddress]         = true;
        _isExcludedFromAntiWhale[_reserveAddress]           = true;

        _isExcludedFromMaxTxAmount[owner()]                 = true;
        _isExcludedFromMaxTxAmount[address(this)]           = true;
        _isExcludedFromMaxTxAmount[pancakePair]             = true;
        _isExcludedFromMaxTxAmount[address(_pancakeRouter)] = true;
        _isExcludedFromMaxTxAmount[_burnAddress]            = true;
        _isExcludedFromMaxTxAmount[_marketingAddress]       = true;
        _isExcludedFromMaxTxAmount[_reserveAddress]         = true;

        _isExcludedFromTax[owner()]             = true;
        _isExcludedFromTax[address(this)]       = true;
        _isExcludedFromTax[_marketingAddress]   = true;
        _isExcludedFromTax[_reserveAddress]     = true;
        
        _isLimited = true;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function mint(address to, uint256 amount) public onlyOwner {
        _mint(to, amount);
    }

    function setMaxBalance(uint256 amount) public onlyOwner {
        _maxBalance = amount * 10**decimals();
    }

    function setMaxBuyAmount(uint256 amount) public onlyOwner {
        _maxBuyAmount = amount * 10**decimals();
    }

    function setMaxSellAmount(uint256 amount) public onlyOwner {
        _maxSellAmount = amount * 10**decimals();
    }

    function setBuyTax(uint256 amount) public onlyOwner {
        _buyTax = amount;
    }

    function setSellTax(uint256 amount) public onlyOwner {
        _sellTax = amount;
    }

    function setSellReservePercent(uint256 amount) public onlyOwner {
        _sellReservePercent = amount;
    }

    function setSellMarketingPercent(uint256 amount) public onlyOwner {
        _sellMarketingPercent = amount;
    }

    function setBuyReservePercent(uint256 amount) public onlyOwner {
        _buyReservePercent = amount;
    }

    function setBuyMarketingPercent(uint256 amount) public onlyOwner {
        _buyMarketingPercent = amount;
    }

    function setMarketingAddress(address payable marketingAddress)
        public
        onlyOwner
    {
        _marketingAddress = marketingAddress;
    }

    function setReserveAddress(address payable reserveAddress)
        public
        onlyOwner
    {
        _reserveAddress = reserveAddress;
    }

    function setIsLimited(bool isLimited) public onlyOwner {
        _isLimited = isLimited;
    }

    function excludeFromAntiWhale(address account) public onlyOwner {
        _isExcludedFromAntiWhale[account] = true;
    }
    
    function includeInAntiWhale(address account) public onlyOwner {
        _isExcludedFromAntiWhale[account] = false;
    }

    function excludeFromTax(address account) public onlyOwner {
        _isExcludedFromTax[account] = true;
    }
    
    function includeInTax(address account) public onlyOwner {
        _isExcludedFromTax[account] = false;
    }

    function excludeFromMaxTxAmount(address account) public onlyOwner {
        _isExcludedFromMaxTxAmount[account] = true;
    }
    
    function includeInMaxTxAmount(address account) public onlyOwner {
        _isExcludedFromMaxTxAmount[account] = false;
    }

    receive() external payable {}

    function recoverBNB() public onlyOwner {
        address payable recipient = payable(_msgSender());
        if (address(this).balance > 0)
            recipient.transfer(address(this).balance);
    }

    //New Pancakeswap router version?
    //No problem, just change it!
    function setRouterAddress(address newRouter) public onlyOwner {
        IPancakeRouter02 _newPancakeRouter = IPancakeRouter02(newRouter);
        // pancakePair = IPancakeFactory(_newPancakeRouter.factory()).createPair(address(this), _newPancakeRouter.WETH());
        address getPair = IPancakeFactory(_newPancakeRouter.factory()).getPair(
            address(this),
            USDT
        );
        if (getPair == address(0)) {
            pancakePair = IPancakeFactory(_newPancakeRouter.factory())
                .createPair(address(this), USDT);
        } else {
            pancakePair = getPair;
        }
        pancakeRouter = _newPancakeRouter;
    }

    function withdraw(
        IERC20 token,
        address recipient,
        uint256 amount
    ) public onlyOwner {
        require(
            token.balanceOf(address(this)) >= amount,
            "Withdraw: Insufficient balance"
        );
        token.transfer(recipient, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        uint256 fromBalance = balanceOf(from);
        require(
            fromBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );
        if (_isLimited) {
            require(
                _isExcludedFromAntiWhale[to] || balanceOf(to) + amount <= _maxBalance,
                "Max tokens limit for this account reached. Try lower amount"
            );
            if (from == pancakePair) {
                require(
                    _isExcludedFromMaxTxAmount[to] || amount <= _maxBuyAmount,
                    "Buy amount exceeds the maximum amount"
                );
            } else if (to == pancakePair) {
                require(
                    _isExcludedFromMaxTxAmount[from] || amount <= _maxSellAmount,
                    "Sell amount exceeds the maximum amount"
                );
            }
        }

        if (from == pancakePair) {
          if (_buyTax > 0 && !_isExcludedFromTax[from] && !_isExcludedFromTax[to]) {
            uint256 reserveAmount;
            uint256 marketingAmount;
            if (_buyReservePercent > 0) {
              reserveAmount = amount * _buyReservePercent / 100;
            }
            if (_buyMarketingPercent > 0) {
              marketingAmount = amount * _buyMarketingPercent / 100;
            }
            if (reserveAmount > 0) {
              super._transfer(from, _reserveAddress, reserveAmount);
              amount -= reserveAmount;
            }
            if (marketingAmount > 0) {
              super._transfer(from, _marketingAddress, marketingAmount);
              amount -= marketingAmount;
            }
          }
        } else if (to == pancakePair) {
          if (_sellTax > 0 && !_isExcludedFromTax[from] && !_isExcludedFromTax[to]) {
            uint256 reserveAmount;
            uint256 marketingAmount;
            if (_sellReservePercent > 0) {
              reserveAmount = amount * _sellReservePercent / 100;
            }
            if (_sellMarketingPercent > 0) {
              marketingAmount = amount * _sellMarketingPercent / 100;
            }
            if (reserveAmount > 0) {
              super._transfer(from, _reserveAddress, reserveAmount);
              amount -= reserveAmount;
            }
            if (marketingAmount > 0) {
              super._transfer(from, _marketingAddress, marketingAmount);
              amount -= marketingAmount;
            }
          }
        }
        super._transfer(from, to, amount);
        // emit Transfer(from, to, amount);
        _afterTokenTransfer(from, to, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }
}