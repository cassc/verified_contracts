    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}
abstract contract ERC20Burnable is Context, ERC20 {

    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    function burnFrom(address account, uint256 amount) public virtual {
        _spendAllowance(account, _msgSender(), amount);
        _burn(account, amount);
    }
}

contract GameToken is ERC20Burnable, AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor(
        string memory name_,
        string memory symbol_,
        uint256 amount_,
        address wallet_
    ) ERC20 (name_, symbol_) {
        _mint(wallet_, amount_ * (10 ** decimals()));
    }

    function mint(address to_, uint256 amount_) public virtual {
        require(hasRole(MINTER_ROLE, _msgSender()), "GameToken: must have minter role to mint");
        _mint(to_, amount_);
    }

}