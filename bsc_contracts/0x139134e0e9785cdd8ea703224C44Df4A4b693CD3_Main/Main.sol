// File: contracts/Main.sol


pragma solidity ^0.8.0;


contract Main {
    uint256 public interval=864000;
    uint256 private all=10000;
    uint256 private part=5000;
    bool public initialized=false;
    address payable public owner;
    mapping (address => uint256) public machineMetrics;
    mapping (address => uint256) public newMachines;
    mapping (address => uint256) public startTime;
    mapping (address => address) public referrals;
    uint256 public marketMachines;

    using EnumerableSet for EnumerableSet.AddressSet;
    EnumerableSet.AddressSet private buyers;

    constructor() {
        owner=payable(msg.sender);
    }
    function runMachines(address ref) public{
        require(initialized, "uninitialized");
        if(ref == msg.sender) {
            ref = address(0);
        }
        if(referrals[msg.sender]==address(0) && referrals[msg.sender]!=msg.sender){
            referrals[msg.sender]=ref;
        }
        uint256 rewards=getMyRewards(msg.sender);
        uint256 newMiners=SafeMath.div(rewards,interval);
        machineMetrics[msg.sender]=SafeMath.add(machineMetrics[msg.sender],newMiners);
        newMachines[msg.sender]=0;
        startTime[msg.sender]=block.number;
        
        newMachines[referrals[msg.sender]]=SafeMath.add(newMachines[referrals[msg.sender]],SafeMath.div(rewards,10));

        marketMachines=SafeMath.add(marketMachines,SafeMath.div(rewards,5));
    }
    function getRewards() public{
        require(initialized, "uninitialized");
        uint256 rewards=getMyRewards(msg.sender);
        uint256 rewardValue=calculateRewardsSell(rewards);
        uint256 fee=devFee(rewardValue);
        newMachines[msg.sender]=0;
        startTime[msg.sender]=block.number;
        marketMachines=SafeMath.add(marketMachines,rewards);
        owner.transfer(fee);
        payable(msg.sender).transfer(SafeMath.sub(rewardValue,fee));
    }
    function buyMachines(address ref) public payable{
        require(initialized, "uninitialized");
        uint256 machinesBought=calculateMachineBuy(msg.value,SafeMath.sub(address(this).balance,msg.value));
        machinesBought=SafeMath.sub(machinesBought,devFee(machinesBought));
        uint256 fee=devFee(msg.value);
        owner.transfer(fee);
        newMachines[msg.sender]=SafeMath.add(newMachines[msg.sender],machinesBought);
        buyers.add(msg.sender);
        runMachines(ref);
    }
    
    function calculateTrade(uint256 rt,uint256 rs, uint256 bs) public view returns(uint256){
        return SafeMath.div(SafeMath.mul(all,bs),SafeMath.add(part,SafeMath.div(SafeMath.add(SafeMath.mul(all,rs),SafeMath.mul(part,rt)),rt)));
    }
    function calculateRewardsSell(uint256 amount) public view returns(uint256){
        return calculateTrade(amount,marketMachines,address(this).balance);
    }
    function calculateMachineBuy(uint256 eth,uint256 contractBalance) public view returns(uint256){
        return calculateTrade(eth,contractBalance,marketMachines);
    }
    
    function devFee(uint256 amount) public pure returns(uint256){
        return SafeMath.div(SafeMath.mul(amount,5),100);
    }
    function initFactory() public payable{
        require(marketMachines==0, "already initialized");
        initialized=true;
        marketMachines=86400000000;
    }
    function getBalance() public view returns(uint256){
        return address(this).balance;
    }
    function getAPR() public view returns(uint256){
        return 100*(365*28800)/interval;
    }
    function getScale() external view returns (uint256) {
        return buyers.length();
    }
    function rewardInfo(address user) external view returns (uint256) {
        if (getBalance() == 0) return 0;
        uint256 rewards=getMyRewards(user);
        if (rewards == 0) return 0;
        return calculateRewardsSell(rewards);
    }
    function assets() external pure returns(address) {
        return 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
    }
    
    function getMyMachineMetrics(address user) public view returns(uint256){
        return machineMetrics[user];
    }
    function getMyRewards(address user) public view returns(uint256){
        return SafeMath.add(newMachines[user],getRewardsSinceLast(user));
    }
    function getRewardsSinceLast(address adr) public view returns(uint256){
        uint256 secondsPassed=min(interval,SafeMath.sub(block.number,startTime[adr]));
        return SafeMath.mul(secondsPassed,machineMetrics[adr]);
    }
    function min(uint256 a, uint256 b) private pure returns (uint256) {
        return a < b ? a : b;
    }
}

