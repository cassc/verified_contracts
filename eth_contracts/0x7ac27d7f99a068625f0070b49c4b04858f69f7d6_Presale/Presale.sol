        // 1 wei of eth = 378,787,878,787,878 / 5735196 = 66046195 wei pmdg per wei of eth
        //so if function receives weiAmount = 1,000,000,000,000,000,000 wei of Eth
        // we just multiply value of 1 wei of eth to weiAmount and gets the number of tokens to be transferred in pmdg
        // 66046195 * 1,000,000,000,000,000,000 = 66046195000000000000000000 wei of pmdg = 66,046,195 pmdg
        uint256 PMDGperEth = _rate.div(oneWeiUsd);
        uint256 PMDGamount = PMDGperEth * weiAmount;
        return PMDGamount;
    }

    function _getTokenFromUsdc(
        uint256 amount //1* 10**6
    ) internal view returns (uint256 tokenAmount) {
        //1 usdc = 10**6
        // 10**6 = 99990716 wei of usd
        // 1 wei usd = 10 ** 6 / 99990716
        uint256 usdcPriceInUSD = uint256(getLatestPriceUsdc());

        // uint256 oneWeiUsd = 10 ** 6 / usdcPriceInUSD; // would be zero
        // 378,787,878,787,878 wei pmdg  = 10 ** 6 / 99990716
        // then 1 wei of usdc
        // 1 wei of usdc = 378,787,878,787,878  * (99990716 / 10**6)
        uint256 PMDGperUSDC = _rate * (usdcPriceInUSD / 10 ** 6);
        // 1 wei of usdt = 37,875,271,212,121,133
        // so if function receives 10**6 wei usdt as weiAmount
        // value of 1 wei of usdt  * weiAMount = 37,875,271,212,121,133 * 10**6 = 37875271212121133000000 wei pmdg = 37875.271212121133 PMDG
        uint256 PMDGamount = PMDGperUSDC * amount;
        return PMDGamount;
    }

    function _getTokenFromUsdt(
        uint256 amount //1* 10**6
    ) internal view returns (uint256 tokenAmount) {
        //1 usdt = 10**6
        // 10**6 = 99990716 wei of usd
        // 1 wei usd = 10 ** 6 / 99990716
        uint256 usdtPriceInUSD = uint256(getLatestPriceUsdt());
        // uint256 usdtPriceInUSD = 99990000;
        // uint256 oneWeiUsd = 10 ** 6 / usdtPriceInUSD; // would be zero
        // 378,787,878,787,878 wei pmdg  = 10 ** 6 / 99990716
        // then 1 wei of usdt
        // 1 wei of usdt = 378,787,878,787,878  * (99990716 / 10**6)
        uint256 PMDGperUSDT = _rate * (usdtPriceInUSD / 10 ** 6);
        // 1 wei of usdt = 37,875,271,212,121,133
        // so if function receives 10**6 wei usdt as weiAmount
        // value of 1 wei of usdt  * weiAMount = 37,875,271,212,121,133 * 10**6 = 37875271212121133000000 wei pmdg = 37875.271212121133 PMDG
        uint256 PMDGamount = PMDGperUSDT * amount;
        return PMDGamount;
    }

    /**
     * @dev fallback function ***DO NOT OVERRIDE***
     * Note that other contracts will transfer funds with a base gas stipend
     * of 2300, which is not enough to call buyTokens. Consider calling
     * buyTokens directly when purchasing tokens from a contract.
     */

    receive() external payable {
        // undo change
        if (_finalized) {
            revert("Pre-Sale is closed");
        } else {
            buyTokensFromEth(_msgSender());
        }
    }

