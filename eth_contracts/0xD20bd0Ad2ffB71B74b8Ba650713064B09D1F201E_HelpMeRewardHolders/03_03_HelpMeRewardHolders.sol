contract HelpMeRewardHolders is Ownable {
    uint256[] public winners;
    
    uint256 public winnerAmount = 5;
    uint256 public NDebuggers = 855;

    /*
     * @notice This is the function that determines the new group of holders for royalty rewards
     */
    function raffleWinners() public onlyOwner {
        while (winners.length > 0) {
            winners.pop();
        }

        for (uint256 trial = 0; trial < winnerAmount; trial++) {
            winners.push(uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, trial))) % NDebuggers + 1);
        }
    }    

    function updateWinnerAmount(uint256 _amount) public onlyOwner {
        winnerAmount = _amount;
    }

    /*
     * @notice Check the new royalty receivers here! If you hold any of these HMDT NFTs, then congrats!
     */
    function viewWinners() public view returns (uint256[] memory) {
        return winners;
    }


}