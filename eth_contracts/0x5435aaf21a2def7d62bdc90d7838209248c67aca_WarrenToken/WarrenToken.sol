    function findOnePercent(uint256 _value) private view returns (uint256)  {
        uint256 roundValue = SafeMath.ceil(_value, basePercent);
        uint256 onePercent = SafeMath.div(SafeMath.mul(roundValue, basePercent), 10000);
        return onePercent;
    }

