contract EthCoreData is Pausable, ReentrancyGuard, WhitelistedRole {
    using SafeMath for uint256;
    using Address for address;
    //custodian
    address[] public custodians;
    address[] public custodians_cold;
    // operations
    mapping(string => bool) public finished_tx;
    // implementation slot
    bytes32 constant IMPLEMENTATION_SLOT = 0x360894a13ba1a3210667c828492db98dca3e2076cc3735a920a3ca505d382bbc;
    address public impl_proposal;
    uint256 public admin_nonce;
    // mintable
    bool public mintable;

    event MemberChange(uint256 indexed nonce);
    event Constructed(address indexed addr);
    event Upgraded(address indexed implementation);

    function _onlyWhitelistAdmin() internal view {
        require(
            isWhitelistAdmin(msg.sender),
            "caller does not have the WhitelistAdmin role"
        );
    }

    function _onlyWhitelisted() internal view {
        require(
            isWhitelisted(msg.sender),
            "caller does not have the Whitelisted role"
        );
    }

    /* ===== Public utility functions ===== */
    function custodianCount() public view returns (uint256) {
        return custodians.length;
    }

    function isCustodian(address addr) public view returns (bool) {
        for (uint256 i = 0; i < custodians.length; ++i) {
            if (custodians[i] == addr) return true;
        }
        return false;
    }

    /* ===== Modifiers =====*/
    modifier onlyCustodian() {
        require(isCustodian(msg.sender), "sender not custodian");
        _;
    }

    modifier isMintable() {
        require(mintable, "mint disabled");
        _;
    }

    /* ===== Member ===== */
    function addMembers(
        address[] memory add_addr,
        address[] memory add_addr_cold
    ) internal {
        for (uint256 i = 0; i < add_addr.length; ++i) {
            custodians.push(add_addr[i]);
            custodians_cold.push(add_addr_cold[i]);
        }
    }

    function removeHot(uint256 index) internal {
        require(index < custodians.length, "bad index");
        custodians[index] = custodians[custodians.length - 1];
        custodians.pop();
    }

    function removeCold(uint256 index) internal {
        require(index < custodians_cold.length, "bad index");
        custodians_cold[index] = custodians_cold[custodians_cold.length - 1];
        custodians_cold.pop();
    }

    function stringEqual(string memory s1, string memory s2)
        public
        pure
        returns (bool)
    {
        return
            keccak256(abi.encodePacked(s1)) == keccak256(abi.encodePacked(s2));
    }

    /* ===== set implmentation ===== */
    // get current implementation contract address
    function implementation() external view returns (address impl) {
        bytes32 slot = IMPLEMENTATION_SLOT;
        assembly {
            impl := sload(slot)
        }
    }

    // store new Impl contract address to fixed slot
    function setImplementation(address newImplementation) internal {
        require(Address.isContract(newImplementation), "non-contract address");
        bytes32 slot = IMPLEMENTATION_SLOT;
        assembly {
            sstore(slot, newImplementation)
        }
    }
}


