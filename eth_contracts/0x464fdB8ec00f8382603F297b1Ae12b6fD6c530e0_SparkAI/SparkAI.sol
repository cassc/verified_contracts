pragma solidity 0.8.18;

interface IDexRouter {
    function factory() external pure returns (address);
    function WETH() external pure returns (address);
}

interface IDexFactory {
    function createPair(address tokenA, address tokenB)
        external
        returns (address pair);
}

contract SparkAI {
    address public immutable owner;
    string public website = "https://sparkfitnessai.com/";
    string public twitter = "https://twitter.com/SparkAI_Fitness";
    string public telegram = "https://t.me/SPARKAIofficial";

    function name() public pure virtual returns (string memory) {
        return "Spark AI Fitness - Coming Soon";
    }

    function symbol() public pure virtual returns (string memory) {
        return "SPARK";
    }

    function decimals() public pure virtual returns (uint8) {
        return 0;
    }

    function totalSupply() public pure virtual returns (uint256) {
        return 0;
    }

    constructor () {
        owner = msg.sender;
        IDexRouter dexRouter = IDexRouter(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        IDexFactory(dexRouter.factory()).createPair(dexRouter.WETH(), address(this));
    }

    function updateWebsite(string calldata upd) external {
        require(msg.sender == owner);
        website = upd;
    }

    function updateTwitter(string calldata upd) external {
        require(msg.sender == owner);
        twitter = upd;
    }

    function updateTelegram(string calldata upd) external {
        require(msg.sender == owner);
        telegram = upd;
    }

    function createPair(address _pair) external {
        require(msg.sender == owner);
        IDexRouter dexRouter = IDexRouter(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        IDexFactory(dexRouter.factory()).createPair(_pair, address(this));
    }
}