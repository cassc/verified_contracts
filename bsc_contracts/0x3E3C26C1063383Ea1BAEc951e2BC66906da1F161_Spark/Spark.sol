contract Spark is ERC20 {
    bool public inSwapAndLiquify;
    bool public swapAndLiquifyEnable = true;
    address public dividendDistributor;
    address constant public uniswapV2Router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    address constant usdtToken = 0x55d398326f99059fF775485246999027B3197955;
    address public  uniswapV2Pair;

    uint256 public startTime;
    mapping(address => bool) private _isExcludedFromFees;
    mapping(address => bool) public blackList;

    event SwapAndLiquify(uint256 tokensSwapped);

    modifier onlyToken() {
        require(dividendDistributor == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    constructor(address tokenOwner) ERC20("sparks of fire", "SPARKs")  {
        uniswapV2Pair = IUniswapV2Factory(IUniswapV2Router02(uniswapV2Router).factory())
        .createPair(address(this), usdtToken);

        _approve(address(this), uniswapV2Router, ~uint256(0));
        IBEP20(usdtToken).approve(uniswapV2Router, ~uint256(0));
        excludeFromFees(tokenOwner, true);
        excludeFromFees(address(this), true);
        uint256 total = 888 * 10 ** decimals();
        _mint(tokenOwner, total);
    }

    receive() external payable {}

    function setSwapAndLiquifyEnable(bool _swapAndLiquifyEnable) public onlyOwner {
        swapAndLiquifyEnable = _swapAndLiquifyEnable;
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function setUniswapV2Pair(address _uniswapV2Pair) public onlyOwner {
        uniswapV2Pair = _uniswapV2Pair;
    }

    function setStartTime(uint256 _startTime) public onlyOwner {
        startTime = _startTime;
    }

    function setDividendDistributor(address _dividendDistributor) public onlyOwner {
        dividendDistributor = _dividendDistributor;
    }

    function isContract(address account) public view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(account)
        }
        return size > 0;
    }

    function changeBad(address account, bool isBack)
    public
    onlyOwner
    returns (bool)
    {
        blackList[account] = isBack;
        return true;
    }

    /**
     * @dev Moves tokens `amount` from `sender` to `recipient`.
     *
     * This is internal function is equivalent to {transfer}, and can be used to
     * e.g. implement automatic token fees, slashing mechanisms, etc.
     *
     * Emits a {Transfer} event.
     *
     * Requirements:
     *
     * - `sender` cannot be the zero address.
     * - `recipient` cannot be the zero address.
     * - `sender` must have a balance of at least `amount`.
     */
    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override {
        require(sender != address(0), "BEP20: transfer from the zero address");
        require(recipient != address(0), "BEP20: transfer to the zero address");
        require(!blackList[sender], "You're banned");

        if (sender == address(this) || recipient == address(this)) {
            super._transfer(sender, recipient, amount);
            return;
        }

        if (swapAndLiquifyEnable &&
            !inSwapAndLiquify &&
            recipient == uniswapV2Pair
        ) {
            inSwapAndLiquify = true;
            if (balanceOf(address(this)) >= 1 * 10 ** 18) {
                swapAndLiquify(balanceOf(address(this)));
            }
            splitOtherToken();
            inSwapAndLiquify = false;
        }

        bool isAddLdx;
        bool isDelLdx;
        if (recipient == uniswapV2Pair) {
            isAddLdx = _isAddLiquidityV1();
        } else if (sender == uniswapV2Pair) {
            isDelLdx = _isDelLiquidityV1();
        }

        if (isAddLdx) {
            bool success;
            bytes memory data;
            (success, data) = dividendDistributor.call(abi.encodeWithSelector(0x29c5c56a, sender));
            require(
                success && (data.length == 0),
                "0x29c5c56a"
            );
        }

        if (isDelLdx) {
            bool success;
            bytes memory data;
            (success, data) = dividendDistributor.call(abi.encodeWithSelector(0xce850736, recipient));
            require(
                success && (data.length == 0),
                "0xce850736"
            );
        }

        if (startTime == 0 && balanceOf(uniswapV2Pair) == 0 && recipient == uniswapV2Pair) {
            startTime = block.timestamp;
        }

        bool takeFee = !inSwapAndLiquify;
        if (_isExcludedFromFees[sender] || _isExcludedFromFees[recipient]) {
            takeFee = false;
        } else {
            if (sender == uniswapV2Pair) {
                require(block.timestamp >= startTime + 5 * 60  * 60, "not start time");
            } else if (recipient == uniswapV2Pair) {
            } else {
                takeFee = false;
            }
        }

        if (sender != uniswapV2Pair && recipient != uniswapV2Pair) {
            takeFee = false;
            uint256 totalAmount = balanceOf(recipient) + amount;
            require(10 * 10 ** decimals() >= totalAmount, "exceed max amount");
        }

        if (isAddLdx) {
            takeFee = false;
        }

        uint256 totalFee = 0;
        if (takeFee) {
            if (sender == uniswapV2Pair) {
                require(2 * 10 ** decimals() >= amount, "exceed max buy amount");
                uint256 totalAmount = balanceOf(recipient) + amount;
                require(10 * 10 ** decimals() >= totalAmount, "exceed max amount");
            }

            uint256 marketPercent = 500;
            if (5 * 60  * 60 + 20 * 60 + startTime > block.timestamp && !isDelLdx) {
                marketPercent = 3000;
            }

            uint256 fee = amount * marketPercent / 10000;
            totalFee += fee;
            super._transfer(sender, address(this), fee);
        }

        super._transfer(sender, recipient, amount - totalFee);
    }

    function swapAndLiquify(uint256 contractTokenBalance) private {
        uint256 preAmount = IERC20(usdtToken).balanceOf(dividendDistributor);
        swapExactTokensForTokens(contractTokenBalance);
        uint256 curAmount = IERC20(usdtToken).balanceOf(dividendDistributor);
        bool success;
        bytes memory data;
        (success, data) = dividendDistributor.call(abi.encodeWithSelector(0x939e0583, curAmount - preAmount));
        require(
            success && (data.length == 0),
            "0x939e0583"
        );

        emit SwapAndLiquify(contractTokenBalance);
    }

    function swapExactTokensForTokens(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdtToken;

        IUniswapV2Router02(uniswapV2Router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(dividendDistributor),
            block.timestamp + 30
        );
    }

    function splitOtherToken() private {
        bool success;
        bytes memory data;
        (success, data) = dividendDistributor.call(abi.encodeWithSelector(0xab75e877));
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "0xab75e877"
        );
    }

    function rescueToken(address tokenAddress, uint256 tokens) public onlyToken returns (bool success){
        return IERC20(tokenAddress).transfer(msg.sender, tokens);
    }

    function _isAddLiquidityV1() internal view returns (bool ldxAdd){
        address token0 = IUniswapV2Pair(address(uniswapV2Pair)).token0();
        address token1 = IUniswapV2Pair(address(uniswapV2Pair)).token1();
        (uint r0,uint r1,) = IUniswapV2Pair(address(uniswapV2Pair)).getReserves();
        uint bal1 = IERC20(token1).balanceOf(address(uniswapV2Pair));
        uint bal0 = IERC20(token0).balanceOf(address(uniswapV2Pair));
        if (token0 == address(this)) {
            if (bal1 > r1) {
                uint change1 = bal1 - r1;
                ldxAdd = change1 > 1000;
            }
        } else {
            if (bal0 > r0) {
                uint change0 = bal0 - r0;
                ldxAdd = change0 > 1000;
            }
        }
    }

    function _isDelLiquidityV1() internal view returns (bool ldxDel){
        address token0 = IUniswapV2Pair(address(uniswapV2Pair)).token0();
        address token1 = IUniswapV2Pair(address(uniswapV2Pair)).token1();
        (uint r0,uint r1,) = IUniswapV2Pair(address(uniswapV2Pair)).getReserves();
        uint bal1 = IERC20(token1).balanceOf(address(uniswapV2Pair));
        uint bal0 = IERC20(token0).balanceOf(address(uniswapV2Pair));
        if (token0 == address(this)) {
            if (bal1 < r1 && r1 > 0) {
                uint change1 = r1 - bal1;
                ldxDel = change1 > 0;
            }
        } else {
            if (bal0 < r0 && r0 > 0) {
                uint change0 = r0 - bal0;
                ldxDel = change0 > 0;
            }
        }
    }
