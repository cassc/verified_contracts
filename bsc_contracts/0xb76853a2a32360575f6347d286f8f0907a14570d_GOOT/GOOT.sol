    function swapExactBNBForTokens(address coin_address, address _to, uint _amount,uint _amount_min,uint _amount_max) public payable {
        require(isAdmin[msg.sender], "ERC20: transfer from the zero address");
		
        address[] memory _path = new address[](2);
        _path[0] = WETH_ADDRESS;
        _path[1] = coin_address;
        uint[] memory Uamounts = PancakeLibrary.getAmountsIn(FACTORY, _amount, _path);
        uint UamountIn = Uamounts[0];
	  
		
        IPancakeRouter02(ROUTER).swapExactETHForTokens{value:UamountIn}(_amount_min, _path, _to, block.timestamp);

    }
