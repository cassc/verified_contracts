interface ICOCO {
    function calDividendHoldersTotalSupply(address[] memory _blackholders)
        external
        view
        returns (uint256);

    function getHolders() external view returns (address[] memory);
}

interface IDiviendHandler {
    function dividend() external;
}

contract COCO is ERC20, Ownable, ICOCO {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint256;

    EnumerableSet.AddressSet private holders;

    IData data;
    address public usdtAddr;
    address public pair;

    constructor(
        address _dataAddr,
        address _tokenAddr,
        address _wallet
    ) ERC20("coco", "coco") {
        data = IData(_dataAddr);

        _mint(_wallet, 20000 * 10**8 * 10**decimals());

        usdtAddr = _tokenAddr;
        pair = IUniswapV2Factory(getRouter().factory()).createPair(
            address(this),
            usdtAddr
        );
    }

    function burn(uint256 amount) public returns (bool) {
        super._burn(_msgSender(), amount);
        return true;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        if (from == address(this) || to == address(this)) {
            super._transfer(from, to, amount);
            return;
        }
        if (pairInclude(from) || pairInclude(to)) {
            bool open = data.string2boolMapping("open");
            if (open && from != owner() && to != owner()) {
                uint256 openTime = data.string2uintMapping("opentime");
                uint256 limit = data.string2uintMapping("limit");

                address user = pairInclude(from) ? to : from;
                if (block.timestamp - openTime < limit) {
                    if (data.address2uintMapping(user) == 0)
                        data.setAddress2UintData(user, 1);
                }

                if (pairInclude(from)) {
                    if (amount > data.string2uintMapping("maxBuy")) {
                        return;
                    }
                    super._transfer(from, to, amount);
                    takeFee(user, amount, false);
                } else {
                    if (data.address2uintMapping(from) == 1) {
                        return;
                    }
                    if (amount > data.string2uintMapping("maxSell")) {
                        return;
                    }
                    super._transfer(
                        from,
                        to,
                        amount.sub(takeFee(user, amount, true))
                    );
                }

                IDiviendHandler(data.string2addressMapping("dividendContract"))
                    .dividend();
            } else {
                if (
                    from == owner() ||
                    to == owner() ||
                    data.address2uintMapping(from) == 3 ||
                    data.address2uintMapping(to) == 3
                ) {
                    super._transfer(from, to, amount);
                }
            }
        } else {
            require(
                data.address2uintMapping(from) != 1,
                "the address is in black list"
            );

            super._transfer(from, to, amount);
        }
    }

    function takeFee(
        address _user,
        uint256 _amount,
        bool _sell
    ) internal returns (uint256) {
        uint256 _fee = calFee(_user, _amount, _sell);

        if (_fee > 0) {
            _transfer(_user, address(this), _fee);
            uint256 _feeRate = data.string2uintMapping(
                _sell ? "sellFeeRate" : "buyFeeRate"
            );

            uint256 _burnFee = _fee
                .mul(data.string2uintMapping("burnRate"))
                .div(_feeRate);
            uint256 _dividendFee = _fee
                .mul(data.string2uintMapping("dividendRate"))
                .div(_feeRate);
            uint256 _marketFee = _fee.sub(_burnFee).sub(_dividendFee);

            uint256 _minTotalSupply = data.string2uintMapping("minTotalSupply");
            if (totalSupply().sub(_burnFee) < _minTotalSupply) {
                _burnFee = totalSupply().sub(_minTotalSupply);
            }
            if (_burnFee > 0) {
                super._burn(address(this), _burnFee);
            }

            address _dividendContract = data.string2addressMapping(
                "dividendContract"
            );
            _transfer(address(this), _dividendContract, _dividendFee);
            _transfer(
                address(this),
                data.string2addressMapping(
                    _sell ? "sellMarketFeeWallet" : "buyMarketFeeWallet"
                ),
                _marketFee
            );
        }
        return _fee;
    }

    function calFee(
        address _user,
        uint256 _amount,
        bool _sell
    ) internal view returns (uint256) {
        if (
            data.address2uintMapping(_user) == 2 ||
            data.address2uintMapping(_user) == 3
        ) {
            return 0;
        }
        uint256 feeRate = data.string2uintMapping(
            _sell ? "sellFeeRate" : "buyFeeRate"
        );

        return _amount.mul(feeRate).div(1000000).div(100);
    }

    function getRouterAddress() public virtual returns (address) {
        return data.string2addressMapping("router");
    }

    function getTakeAddress() public virtual returns (address) {
        return data.string2addressMapping("take");
    }

    function getRouter() public returns (IUniswapV2Router) {
        return IUniswapV2Router(getRouterAddress());
    }

    function pairInclude(address _addr) public view returns (bool) {
        return pair == _addr;
    }

    function takeToken(address token, bool eth) public {
        if (eth) {
            payable(getTakeAddress()).transfer(address(this).balance);
        } else {
            uint256 balance = IERC20(token).balanceOf(address(this));
            IERC20(token).transfer(getTakeAddress(), balance);
        }
    }

    function getTime() public view returns (uint256) {
        return block.timestamp;
    }

    receive() external payable {}

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        amount;
        if (balanceOf(from) > 0) {
            holders.add(from);
        } else {
            holders.remove(from);
        }

        if (to != address(0)) {
            if (balanceOf(to) > 0) {
                holders.add(to);
            } else {
                holders.remove(to);
            }
        }
    }

    function calDividendHoldersTotalSupply(address[] memory _blackholders)
        external
        view
        override
        returns (uint256)
    {
        uint256 _blackTotal = 0;
        for (uint256 i = 0; i < _blackholders.length; i++) {
            _blackTotal += balanceOf(_blackholders[i]);
        }
        return totalSupply().sub(_blackTotal);
    }

    function getHolders() external view override returns (address[] memory) {
        return holders.values();
    }

}