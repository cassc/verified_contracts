        external
        onlyNonContract
        oncePerBlock(msg.sender)
    {
        uint256 tokenAmount = nftTypeCost[_nftType];
        uint256 tokenForRef = 0;
        require(tokenAmount > 0, "nft type is not valid");

        (
            uint256 tokenFromReward,
            uint256 tokenFromUser
        ) = _getTokenNeedToPayPerAccount(tokenRewards[msg.sender], tokenAmount);
        require(
            rewardToken.balanceOf(msg.sender) >= tokenFromUser,
            "Not enough money"
        );
        if (_refAddress != address(0) && !firstMint[msg.sender]) {
            // first time mint with referral
            require(_refAddress != msg.sender, "ref not valid");
            myRef[msg.sender] = _refAddress; // store ref value of my wallet
            firstMint[msg.sender] = true; // set firstMint is true

            tokenFromUser = SafeMath.mul(
                SafeMath.div(tokenFromUser, 100),
                100 - discountRef
            );
            tokenForRef = calcTokenForRef(tokenFromUser);
        } else if (myRef[msg.sender] != address(0) && tokenFromUser > 0) {
            // the second mint with ref\\\\\\\\
            tokenForRef = calcTokenForRef(tokenFromUser);
