    function createOneNFT(
        address _to,
        string memory _breed,
        Sex _sex,
        int256[] calldata _IDs
    ) external;

    function createLegendaryItem(
        address _to,
        uint256 _legendaryIndex,
        string memory _breed,
        Sex _sex
    ) external;

    function totalSupply() external view returns (uint256);

    function tokenURI(uint256 _petId) external view returns (string memory);

    function ownerOf(uint256 _petId) external view returns (address);

    function attributesOnPet(uint256 _petId, address _attributeAddress) external view returns (int256);

    function getAttributesOnPet(uint256 _petId) external view returns (int256[] memory);

    function attributeChangingFee() external returns (uint256);

    function changeAttributeOnPet(
        uint256 _petId,
        address _attributeAddress,
        int256 _attributeIdToAdd
    ) external payable;
}


