        require(_storage.stakedSlots[owner].contains(slot), "Incorrect slot");

        AssetStake storage _assetStake = _storage.slots[owner][slot];

        require(_assetStake.timestamp < block.timestamp, "Nothing to claim");

        return _calculateClaim(_assetStake.timestamp, _assetStake.bonus);
