contract TokenERC20 is Ownable, ERC20 {
    using SafeMath for uint256;

    uint256 public ecosystemFee = 0;
    uint256 public savingFee = 5;
    uint256 public constant feeDenominator = 100;

    address public ecosystem = 0xCF3DB80904CC189631dE20E027cB743A22bdA78c;
    address public rewardPool = 0x08adbaA6A215affd711F532ec219299ba1E5b9B7;

    mapping(address => bool) public liquidityProvider;

    event FeeRateChanged(uint256 indexed _ecosystemFeeRate, uint256 indexed _savingFeeRate);

    constructor(
        string memory name,
        string memory symbol,
        uint8 decimals
    ) ERC20(name, symbol, decimals) Ownable() {}

    function setTransferFeeRate(
        uint256 _ecosystemFeeRate,
        uint256 _savingFeeRate
    ) public onlyOwner {
        require(_ecosystemFeeRate <= 5, "Not more than 5%");
        require(_savingFeeRate <= 5, "Not more than 5%");
        ecosystemFee = _ecosystemFeeRate;
        savingFee = _savingFeeRate;
        emit FeeRateChanged(_ecosystemFeeRate, _savingFeeRate);
    }

    function setLiquidityProvider(address _liquidityProvider) public onlyOwner {
        require(_liquidityProvider != address(0));
        liquidityProvider[_liquidityProvider] = true;
    }

    function sweepBNB(address _to) public onlyOwner {
        require(_to != address(0));
        payable(_to).transfer(address(this).balance);
    }

    function sweepToken(address _token, address _to) public onlyOwner {
        require(_token != address(0));
        require(_to != address(0));
        require(IERC20(_token).transfer(_to, IERC20(_token).balanceOf(address(this))), "Transfer failed");
    }
}

contract FootballChain is TokenERC20 {
    using SafeMath for uint256;

    uint256 public constant DECIMALS = 18;

    IUniswapV2Router02 public uniswapV2Router;

    uint256 public constant maxSupply = 100 * 10**6 * 10**18;

    address public uniswapV2Pair;

    bool inSwap = false;

    modifier swapping() {
        inSwap = true;
        _;
        inSwap = false;
    }

    constructor() TokenERC20("FootballChain", "FBC", uint8(DECIMALS)) {
        _mint(_msgSender(), maxSupply);

        uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(
            address(this),
            uniswapV2Router.WETH()
        );
    }

    function burn(uint256 amount) public {
        _burn(_msgSender(), amount);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {

        if (inSwap) {
            super._transfer(sender, recipient, amount);
            return;
        }

        uint256 _amount = shouldTakeFee(sender, recipient)
            ? takeFee(sender, amount)
            : amount;

        super._transfer(sender, recipient, _amount);
    }

    function shouldTakeFee(address from, address to)
        internal
        view
        returns (bool)
    {
        return
            (from != address(this) && to != address(this)) &&
            (uniswapV2Pair == from || uniswapV2Pair == to) &&
            !liquidityProvider[from];
    }

    function takeFee(address sender, uint256 _amount)
        internal
        swapping
        returns (uint256)
    {
        bool isBuy = uniswapV2Pair == sender;

        if (isBuy) {
            uint256 ecosystemAmount = _amount.mul(ecosystemFee).div(feeDenominator);
            super._transfer(sender, ecosystem, ecosystemAmount);
            return _amount.sub(ecosystemAmount);
        } else {
            uint256 savingAmount = _amount.mul(savingFee).div(
                feeDenominator
            );
            super._transfer(sender, rewardPool, savingAmount);
            return _amount.sub(savingAmount);
        }
    }

    // receivable token
    receive() external payable {}
}