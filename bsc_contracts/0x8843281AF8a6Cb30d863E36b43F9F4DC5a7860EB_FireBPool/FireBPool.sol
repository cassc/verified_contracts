contract FireBPool is IFireBPool, AccessControl {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    uint256 public constant PRECISION = 1000;
    address public constant HOLE = address(0x000000000000000000000000000000000000dEaD);

    bytes32 public constant ROLE_ADMIN = keccak256("ROLE_ADMIN");
    bytes32 public constant ROLE_APOOL = keccak256("ROLE_APOOL");
    bytes32 public constant ROLE_ROBOT = keccak256("ROLE_ROBOT");

    address public feeTo;
    IFireUsdtRobot public robot;

    address public tokenFdt;
    address public tokenUsdt;
    address public aPool;
    address public aRouter;

    mapping(address => uint256) private balances;
    
    uint256 private reserveFdt;
    uint256 private reserveUsdt;
    uint256 private blockTimestampLast;

    event Deposited(address indexed _account, uint256 _amount);
    event Withdrawn(address indexed _account, uint256 _amount);

    constructor(
        address _fdt, 
        address _usdt, 
        address _aRouter,
        address _feeTo
    ) {
        _setupRole(ROLE_ADMIN, _msgSender());

        _setRoleAdmin(ROLE_APOOL, ROLE_ADMIN);
        _setRoleAdmin(ROLE_ROBOT, ROLE_ADMIN);

        _setupRole(ROLE_APOOL, _fdt);
        _setupRole(ROLE_ROBOT, _msgSender());

        tokenFdt = _fdt;
        tokenUsdt = _usdt;

        aRouter = _aRouter;
        address _factory = IUniswapV2Router02(_aRouter).factory();
        aPool = IUniswapV2Factory(_factory).getPair(_fdt, _usdt);

        feeTo = _feeTo;
    }

    function setRobot(address _robot) public onlyRole(ROLE_ADMIN) {
        robot = IFireUsdtRobot(_robot);
        _setupRole(ROLE_ROBOT, _robot);
    }

    function balanceOf(address _account) override external view returns (uint256) {
        return balances[_account];
    }

    function deposit(address _account, uint256 _amountFdt) override external onlyRole(ROLE_APOOL) {
        balances[_account] = balances[_account] + _amountFdt;
        emit Deposited(_account, _amountFdt);
    }

    function withdraw(address _account, uint256 _amountFdt) override public onlyRole(ROLE_APOOL) {
        if (balances[_account] <= 0 || balances[_account] < _amountFdt) {
            return;
        }

        uint256 _bPoolUsdtBalance = IERC20(tokenUsdt).balanceOf(address(this));
        if (_bPoolUsdtBalance <= 0) {
            return;
        }

        if (reserveFdt <= 0 || reserveUsdt <= 0) {
            return;
        }

        uint256 _amountUsdt = _amountFdt.mul(reserveUsdt).div(reserveFdt);
        if (_amountUsdt > _bPoolUsdtBalance) {
            _amountUsdt = _bPoolUsdtBalance;
        }

        uint256 _amountFee = _amountUsdt.mul(50).div(PRECISION);
        IERC20(tokenUsdt).transfer(feeTo, _amountFee);

        IERC20(tokenFdt).transfer(HOLE, _amountFdt);
        IERC20(tokenUsdt).transfer(_account, _amountUsdt.sub(_amountFee));

        reserveFdt = reserveFdt.sub(_amountFdt);
        reserveUsdt = reserveUsdt.sub(_amountUsdt);

        balances[_account] = balances[_account].sub(_amountFdt);

        emit Withdrawn(_account, _amountFdt);
    }

    function withdrawAll() external {
        address _account = _msgSender();
        uint256 _amountFdt = balances[_msgSender()];

        if (balances[_account] <= 0 || balances[_account] < _amountFdt) {
            return;
        }

        uint256 _bPoolUsdtBalance = IERC20(tokenUsdt).balanceOf(address(this));
        if (_bPoolUsdtBalance <= 0) {
            return;
        }

        if (reserveFdt <= 0 || reserveUsdt <= 0) {
            return;
        }

        uint256 _amountUsdt = _amountFdt.mul(reserveUsdt).div(reserveFdt);
        if (_amountUsdt > _bPoolUsdtBalance) {
            _amountUsdt = _bPoolUsdtBalance;
        }

        uint256 _amountFee = _amountUsdt.mul(50).div(PRECISION);
        IERC20(tokenUsdt).transfer(feeTo, _amountFee);

        IERC20(tokenFdt).transfer(HOLE, _amountFdt);
        IERC20(tokenUsdt).transfer(_account, _amountUsdt.sub(_amountFee));

        reserveFdt = reserveFdt.sub(_amountFdt);
        reserveUsdt = reserveUsdt.sub(_amountUsdt);

        balances[_account] = balances[_account].sub(_amountFdt);

        emit Withdrawn(_account, _amountFdt);
    }

    function sync() override public onlyRole(ROLE_ROBOT) {
        reserveFdt = IERC20(tokenFdt).balanceOf(address(this));
        reserveUsdt = IERC20(tokenUsdt).balanceOf(address(this));
        blockTimestampLast = block.timestamp;
    }

    function getReserves() override external view 
        returns (
            uint256 _fdtAmount,
            uint256 _usdtAmount,
            uint256 _blockTimestampLast
        )
    {
        return (reserveFdt, reserveUsdt, blockTimestampLast);
    }
}