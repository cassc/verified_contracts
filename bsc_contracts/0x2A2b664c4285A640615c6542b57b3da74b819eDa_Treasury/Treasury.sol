     * @notice with this function, evreyone can purchase with paying the price.
     * @dev in case the deposit method is sixp(0) or busd(2), the token must be approved for this smart contract before this function is called. In order to pay with bnb(1).
     * @param _tokenId the ID of the farm.
     */
    function Buy(uint256 _tokenId) external payable {
        SixPackMiningFarm SixpackMiningFarmContract = SixPackMiningFarm(
            miningFarmAddress
        );
        require(
            SixpackMiningFarmContract.ownerOf(_tokenId) == address(this),
            "Treasury::Buy: It is not possible to buy this farm"
        );

        SixPackMiningFarm.FarmType size = SixpackMiningFarmContract.getFarmType(
            _tokenId
        );
        if (DepositMethod == DepositMethods.BNB) {
            depositEther(size);
        } else {
            depositERC20(size);
        }
        SixpackMiningFarmContract.transferFrom(
            address(this),
            msg.sender,
            _tokenId
        );
        emit tokenSold(_tokenId, msg.sender);
    }

    function depositEther(SixPackMiningFarm.FarmType size) private {
        price storage priceOfFarm = farmPrice[size];
        require(
            msg.value >= priceOfFarm.bnb,
            "Treasuty::DepositEther: Deposit tokens are less than the price of NFT"
        );
        (bool sentToPlatform, ) = upgradeAndEarningAddress.call{
            value: msg.value
        }("");
        require(sentToPlatform, "Treasuty::DepositEther: Failed to send Ether");
    }

    function depositERC20(SixPackMiningFarm.FarmType size) private {
        uint256 farmPricBaseOnToken = 0;
        address payableToken = address(0);

        price storage priceOfFarm = farmPrice[size];
        if (DepositMethod == DepositMethods.SIXP) {
            farmPricBaseOnToken = priceOfFarm.sixp;
            payableToken = tokenContractAddress.sixp;
        }
        if (DepositMethod == DepositMethods.BUSD) {
            farmPricBaseOnToken = priceOfFarm.busd;
            payableToken = tokenContractAddress.busd;
        }
        require(farmPricBaseOnToken > 0, "Treasuty::depositERC20: Price error");
        IERC20 ERC20TOKEN = IERC20(address(payableToken));

        uint256 allowance = ERC20TOKEN.allowance(msg.sender, address(this));
        uint256 balance = ERC20TOKEN.balanceOf(msg.sender);

        require(
            allowance >= farmPricBaseOnToken,
            "Treasuty::depositERC20: Aprrove token"
        );

        require(
            balance >= farmPricBaseOnToken,
            "Treasuty::depositERC20: Balancne error"
        );

        ERC20TOKEN.transferFrom(
            msg.sender,
            upgradeAndEarningAddress,
            farmPricBaseOnToken
        );
    }

    /**
     * @notice with this function, the admin can set sixp and busd addresses.
     * @dev this method shoud be called immediately after depolying contract.
     * @param sixp sixp token address.
     * @param busd busd token address.
     */
    function setTokenContractAddress(address sixp, address busd)
        external
        onlyAdmin
        returns (bool)
    {
        tokenContractAddress = tokenAddress({sixp: sixp, busd: busd});
        emit tokenContractChanged(sixp, busd);

        return true;
    }

    /**
     * @notice  by calling this function, the sixp token gives addresses rewards based on the value of minted sixp and addresses' royalties.
     * @dev by calling this function, a function from sixp smart contract will be called, and sixp tokens will be minted. In average, this function will be called evrey 24 hours.
     */
    function mintForTreasury() external {
        require(mintPermission, "Treasury::mintForTreasury: Permission error");
        IERC20 ERC20TOKEN = IERC20(address(tokenContractAddress.sixp));
        uint256 _mintPerDay = ERC20TOKEN.mintPerDay();

        ERC20TOKEN.mintForTreasury();
        _rewardSharing(_mintPerDay);
        emit mintdSixpForTreasury();
    }

    function _rewardSharing(uint256 _mintPerDay) private {
        IERC20 ERC20TOKEN = IERC20(address(tokenContractAddress.sixp));
        uint256 upgradeAndEarningAmount = (upgradeAndEarningRoyalty *
            _mintPerDay) / 100;
        uint256 repairAndMaintenanceAmount = (repairAndMaintenanceRoyalty *
            _mintPerDay) / 100;
        uint256 AGPAndWellnessProgramAmount = (AGPAndWellnessProgramRoyalty *
            _mintPerDay) / 100;
        ERC20TOKEN.transfer(upgradeAndEarningAddress, upgradeAndEarningAmount);
        ERC20TOKEN.transfer(
            repairAndMaintenanceAddress,
            repairAndMaintenanceAmount
        );
        ERC20TOKEN.transfer(
            AGPAndWellnessProgramAddress,
            AGPAndWellnessProgramAmount
        );
        emit rewardShared(
            upgradeAndEarningAmount,
            repairAndMaintenanceAmount,
            AGPAndWellnessProgramAmount
        );
    }

    /**
     * @notice  by calling this function, the admin can set RepairAndMaintenance address for dividing sixp tokens.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _repairAndMaintenanceAddress RepairAndMaintenance address.
     */
    function setRepairAndMaintenanceAddress(
        address _repairAndMaintenanceAddress
    ) external onlyAdmin returns (bool) {
        repairAndMaintenanceAddress = _repairAndMaintenanceAddress;
        emit repairAndMaintenanceAddressChanged(repairAndMaintenanceAddress);

        return true;
    }

    /**
     * @notice  by calling this function, the admin can set upgradeAndEarning address for dividing sixp tokens.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _upgradeAndEarningAddress upgradeAndEarning address.
     */
    function setUpgradeAndEarningAddress(address _upgradeAndEarningAddress)
        external
        onlyAdmin
        returns (bool)
    {
        upgradeAndEarningAddress = _upgradeAndEarningAddress;
        emit upgradeAndEarningAddressChanged(upgradeAndEarningAddress);
        return true;
    }

    /**
     * @notice  by calling this function, the admin can set AGPAndWellnessProgram address for dividing sixp tokens.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _AGPAndWellnessProgramAddress AGPAndWellnessProgram address.
     */
    function setAGPAndWellnessProgramAddress(
        address _AGPAndWellnessProgramAddress
    ) external onlyAdmin returns (bool) {
        AGPAndWellnessProgramAddress = _AGPAndWellnessProgramAddress;
        emit AGPAndWellnessProgramAddressChanged(AGPAndWellnessProgramAddress);

        return true;
    }

    /**
     * @notice  by calling this function, the admin can set governance address.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _governanceAddress governance address.
     */
    function setGovernanceAddress(address _governanceAddress)
        external
        onlyAdmin
        returns (bool)
    {
        _setGovernanceAddress(_governanceAddress);
        return true;
    }

    function _setGovernanceAddress(address _governanceAddress)
        private
        returns (bool)
    {
        governanceAddress = _governanceAddress;
        GovernanceContract = Governance(address(governanceAddress));

        emit governanceAddressChanged(governanceAddress);

        return true;
    }

    /**
     * @notice  this function is for changing the permission of mintForTreasury .
     * @dev for changing permission, the admin can create a proposal in governance smart contract, and for this proposal voting starts. In case this proposal succeeds, this function can be called.
     * @param permission true or false.
     */
    function setMintPermission(bool permission) external onlyGovernance {
        mintPermission = permission;
        emit mintForTreasuryPermissionChanged(permission);
    }

    /**
     * @notice  by calling this function, the admin can set miningFarm address.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _miningFarmAddress address of miningFarm.
     */
    function setMiningFarmAddress(address _miningFarmAddress)
        external
        onlyAdmin
    {
        _setMiningFarmAddress(_miningFarmAddress);
    }

    function _setMiningFarmAddress(address _miningFarmAddress) private {
        miningFarmAddress = _miningFarmAddress;
    }

    /**
     * @notice  this function is for changing the percentages of royalties .
     * @dev for changing percentages of royalties, the admin can create a proposal in governance smart contract, and for this proposal voting starts. In case this proposal succeeds, this function can be called.
     * @param _upgradeAndEarningRoyalty the royalty of upgradeAndEarning address .
     * @param _repairAndMaintenanceRoyalty the royalty of epairAndMaintenance address.
     * @param _AGPAndWellnessProgramRoyalty the royalty of AGPAndWellnessProgram address.
     */
    function updateRoyalty(
        uint256 _upgradeAndEarningRoyalty,
        uint256 _repairAndMaintenanceRoyalty,
        uint256 _AGPAndWellnessProgramRoyalty
    ) external onlyGovernance {
        require(
            _upgradeAndEarningRoyalty +
                _repairAndMaintenanceRoyalty +
                _AGPAndWellnessProgramRoyalty ==
                100,
            "Treasuty::updateRoyalty: The sum of 3 numbers is not equal to 100"
        );

        upgradeAndEarningRoyalty = _upgradeAndEarningRoyalty;
        repairAndMaintenanceRoyalty = _repairAndMaintenanceRoyalty;
        AGPAndWellnessProgramRoyalty = _AGPAndWellnessProgramRoyalty;
        emit royaltyUpdated(
            _upgradeAndEarningRoyalty,
            _repairAndMaintenanceRoyalty,
            _AGPAndWellnessProgramRoyalty
        );
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }
}