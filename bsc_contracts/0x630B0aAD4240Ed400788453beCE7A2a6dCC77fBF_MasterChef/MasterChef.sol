    function relMap(address user) external view returns (address);
    function userInfo(uint256 poolId,address user)external returns(uint256,uint256);
}


// MasterChef is the master of WB3. He can make WB3 and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to a governance smart contract once WB3 is sufficiently
// distributed and the community can show to govern itself.
//
// Have fun reading it. Hopefully it's bug-free. God bless.
contract MasterChef is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    address public WETH;

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        //
        // We do some fancy math here. Basically, any point in time, the amount of USDTs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken; // Address of LP token contract.
        uint256 lastRewardBlock; // Last block number that USDTs distribution occurs.
        uint256 accPerShare; // Accumulated USDTs per share, times 1e12. See below.
    }

    // The USDT TOKEN!
    IBEP20 public usdt;
    // Dev address.
    address public devaddr;
    // USDT tokens created per block.
    uint256 public usdtPerBlock;

    uint256 public feeBNB = 2 * 1e15;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    
    address public topRel = 0x7777777777777777777777777777777777777777;
    mapping(address=>address) public relMap;
    mapping(address=>bool) public blackList;

    // The block number when USDT mining starts.
    uint256 public startBlock;

    address public profitAddress;

    address public oldMasterChef = 0xE98B86F52f06D575580d4ac85CdF4ab0d4dB104F;

    uint256 private blockInteval = 86400;//86400
    mapping(address=>bool) public ifSynced;

    event Deposit(address indexed user,address indexed refAddress, uint256 amount);
    event Withdraw(address indexed user,  uint256 amount);
    event WithdrawProfit(address indexed user,  uint256 amount);
    

    constructor(
        address _WETH,
        IBEP20 _usdt,
        IBEP20 _wb3,
        address _profitAddress,
        address _devaddr,
        uint256 _usdtPerDay,
        uint256 _startDay//0点的时间戳
    ) public {
        WETH = _WETH;
        usdt = _usdt;
        profitAddress = _profitAddress;
        devaddr = _devaddr;
        usdtPerBlock = _usdtPerDay;
        startBlock = (_startDay+28800)/blockInteval;// (1664294400+28800)/blockInteval = 19263 = startBlock 1day 1Block

        // staking pool
        poolInfo.push(PoolInfo({lpToken: _wb3, lastRewardBlock: startBlock, accPerShare: 0}));

    }

    function setUsdtProfitAddress(address _profitAddress) public onlyOwner {
        profitAddress = _profitAddress;
    }

    function setUsdtPerBlock(uint256 _usdtPerBlock) public onlyOwner {
        updatePool();
        usdtPerBlock = _usdtPerBlock;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public pure returns (uint256) {
        return _to.sub(_from);
    }

    function addBlackList(address _address,bool _value) external onlyOwner {
        blackList[_address] = _value;
    }

    // View function to see pending USDTs on frontend.
    function pendingUSDT(address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][_user];
        uint256 accPerShare = pool.accPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        uint256 day = (block.timestamp+28800)/blockInteval;
        if (day > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, day);
            uint256 usdtReward = multiplier.mul(usdtPerBlock);
            accPerShare = accPerShare.add(usdtReward.mul(1e12).div(lpSupply));
        }
        return user.amount.mul(accPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool() public {
        PoolInfo storage pool = poolInfo[0];
        uint256 day = (block.timestamp+28800)/blockInteval;
        if (day <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            pool.lastRewardBlock = day;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, day);
        uint256 usdtReward = multiplier.mul(usdtPerBlock);
        pool.accPerShare = pool.accPerShare.add(usdtReward.mul(1e12).div(lpSupply));
        pool.lastRewardBlock = day;
    }

    // Deposit LP tokens to MasterChef for USDT allocation.
    function deposit(uint256 _amount,address refAddress) public payable{
        require(msg.value == feeBNB, "deposit: feeBNB error");
        if(relMap[msg.sender] == address(0)){
            if(refAddress != topRel){
                require(refAddress != address(0) 
                    && refAddress!=msg.sender 
                    && relMap[refAddress]!=address(0), "deposit: refAddress error");
            }
            relMap[msg.sender] = topRel;
        }else{
            refAddress = relMap[msg.sender];
        }
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];
        updatePool();
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accPerShare).div(1e12).sub(user.rewardDebt);
            if (pending > 0) {
                safeUSDTTransfer(msg.sender, pending);
            }
        }
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            user.amount = user.amount.add(_amount);
        }
        user.rewardDebt = user.amount.mul(pool.accPerShare).div(1e12);
        emit Deposit(msg.sender,refAddress, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _amount) public payable{
        require(!blackList[msg.sender],"in blackList");
        require(msg.value == feeBNB, "withdraw: feeBNB error");
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");

        updatePool();
        uint256 pending = user.amount.mul(pool.accPerShare).div(1e12).sub(user.rewardDebt);
        if (pending > 0) {
            safeUSDTTransfer(msg.sender, pending);
        }
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accPerShare).div(1e12);
        emit Withdraw(msg.sender, _amount);
    }

    function withdrawProfit() public{
        require(!blackList[msg.sender],"in blackList");
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];
        
        updatePool();
        uint256 pending = user.amount.mul(pool.accPerShare).div(1e12).sub(user.rewardDebt);
        require(pending > 0, "withdrawProfit: no profit");
        safeUSDTTransfer(msg.sender, pending);
        user.rewardDebt = user.amount.mul(pool.accPerShare).div(1e12);
        emit WithdrawProfit(msg.sender, pending);
    }


    // Safe usdt transfer function, just in case if rounding error causes pool to not have enough USDTs.
    function safeUSDTTransfer(address _to, uint256 _amount) internal {
        // require(usdt.balanceOf(address(this)) >= _amount, "withdraw: contract doesnt have enough balance transfer");
        usdt.safeTransferFrom(profitAddress,_to, _amount);
    }

    function withdrawToken(address tokenAddress_, uint256 amount) external onlyOwner {
        IBEP20(tokenAddress_).safeTransfer(msg.sender, amount);
    }

    function withdrawBNB(uint256 amount) external onlyOwner {
        require(amount >= 0, "withdraw: amount not good");
        (bool success,) = msg.sender.call{value : amount}(new bytes(0));
        require(success, 'TransferHelper::safeTransferETH: ETH transfer failed');
    }

    // Update dev address by the previous dev.
    function dev(address _devaddr) public {
        require(msg.sender == devaddr, "dev: wut?");
        devaddr = _devaddr;
    }


    receive() external payable {
        assert(msg.sender == WETH); // only accept ETH via fallback from the WETH contract
    }

    function syncFromOld(address user_) external onlyOwner{
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][user_];
        require(!ifSynced[user_],"synced");
        address refAddress = IMasterChef(oldMasterChef).relMap(user_);
        require(refAddress!=address(0),"refAddress error");
        (uint256 depositAmount,) = IMasterChef(oldMasterChef).userInfo(0,user_);
        require(depositAmount>=0,"depositAmount error");
        updatePool();
        pool.lpToken.safeTransferFrom(address(msg.sender), address(this), depositAmount);
        user.amount = depositAmount;
        user.rewardDebt = user.amount.mul(pool.accPerShare).div(1e12);
        ifSynced[user_] = true;
        relMap[user_] = refAddress;
        emit Deposit(user_,refAddress, depositAmount);
    }

}