    function allowance(address owner, address spender) external view returns (uint256);
    function approve(address spender, uint256 amount) external returns (bool);
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
}

interface IFqswap {
    function totalSupply() external view returns (uint256);

    function balanceOf(address account) external view returns (uint256);

    function transfer(address recipient, uint256 amount) external returns (bool);

    function allowance(address owner, address spender) external view returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);

    function migrate(address account, uint256 amount) external;


    function isMigrationStarted() external view returns (bool);
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
}


interface IMigration {
    function migrate(uint256 amount) external returns (uint256);

    function toggleMigrate(bool _isLocked) external returns (bool);
}

contract Migration is IMigration, Initializable, ContextUpgradeable, OwnableUpgradeable {
  using SafeMathUpgradeable for uint256;

  uint256 private consolidation;
  bool private _isLocked;
  bool private isMigrated;
  IERC20Upgradeable public FqswapV1;
  IFqswap public FqswapV2;

  function initialize(address _fqswapV1, address _fqswapV2) public initializer {
    __Context_init_unchained();
    __Ownable_init_unchained();
    __Fqswap_migration_init_unchained(_fqswapV1, _fqswapV2);
  }

  function __Fqswap_migration_init_unchained(address _fqswapV1, address _fqswapV2) internal initializer {
    FqswapV1 = IERC20Upgradeable(_fqswapV1);
    FqswapV2 = IFqswap(_fqswapV2);
    _isLocked = false;
    consolidation = 1000;
  }


  function setFqswapV2Address(address _fqswapV2) external onlyOwner() {
    require(_fqswapV2 != address(0), "Migration: Invalid address");
    FqswapV2 = IFqswap(_fqswapV2);
  }

  function setFqswapV1Address(address _fqswapV1) external onlyOwner() {
    require(_fqswapV1 != address(0), "Migration: Invalid address");
    FqswapV1 = IERC20Upgradeable(_fqswapV1);
  }

  function migrate(uint256 _amount) external override returns (uint256) {
    require(FqswapV2.isMigrationStarted(), "Migration: Migration is not started yet");
    require(!_isLocked, "Migration: Locked by admin");
    require(_amount > 0, "Migration: Amount is too low");
    require(_amount >= consolidation, "Migration: Amount is too low");
    uint256 _balance = FqswapV1.balanceOf(_msgSender());
    require(_balance >= _amount, "Migration: Insufficient balance");

    require(FqswapV1.transferFrom(_msgSender(), address(FqswapV2), _amount), 'Migration: Transfer from old version failed');
    FqswapV2.migrate(_msgSender(), _amount.div(consolidation));

    isMigrated = true;
    return _amount.div(consolidation);
  }

  function toggleMigrate(bool isLocked) external override onlyOwner() returns (bool) {
    _isLocked = isLocked;
    return _isLocked;
  }

  function setConsolidation(uint256 _consolidation) external onlyOwner() {
    require(_consolidation > 0, "Migration: Consolidation should be 1 at least");
    require(!isMigrated, "Migration: Migrations was started");
    consolidation = _consolidation;
  }

  function isLocked() external view returns (bool) {
    return _isLocked;
  }
}