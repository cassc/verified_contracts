        Character.CharacterStruct memory char = character.get(charID);
        character.drainStamina(charID, staminaCostFight);

        // calculate SP
        uint64 _remainSP = uint64(
            100 - character.getDurabilityPoints(charID).mul(2)
        ); // minus 2% per day

        _performCalculate(
            charID,
            char.nftType,
            char.rarity,
            _remainSP,
