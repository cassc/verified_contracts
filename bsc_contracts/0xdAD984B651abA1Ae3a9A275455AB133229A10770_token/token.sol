contract token is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    
    string private _name = 'AOC';
    string private _symbol = 'AOC';
    uint8 private _decimals = 18;
    uint256 private _totalSupply = 3888 * 10 **18;
    
    uint256 public _devFee = 5;
    uint256 public _inviteFee = 5;

    mapping (address => uint256) private _balances;
    
    mapping (address => address) public inviteInfo;
    mapping (address => bool) private _isExcludedFromFee;

    mapping (address => mapping (address => uint256)) private _allowed;
    address public _ownerAddress = address(0x639a3b465F27b4F35542F6E088EE26c730D1F5C4);
    address public _devAddress = address(0x8704e323FAa50ab9496f07b16bdD7d1385e57f46);
    
    constructor () public {
        _balances[_ownerAddress] = _totalSupply;
        _isExcludedFromFee[_ownerAddress] = true;
        _isExcludedFromFee[_devAddress] = true;
        emit Transfer(address(0),_ownerAddress, _totalSupply);
    }
    
    function _transfer(address from, address to, uint256 value) internal {
        require(value <= _balances[from]);
        require(to != address(0));
        if (inviteInfo[to] == address(0) && !isContract(from) && !isContract(to)) {
            inviteInfo[to] = from;
        }
        uint256 nowValue = value;
        if (!_isExcludedFromFee[from] && !_isExcludedFromFee[to]) {
            uint256 devValue = value.mul(_devFee).div(100);
            uint256 inviteValue = value.mul(_inviteFee).div(100);
            nowValue = value.sub(devValue).sub(inviteValue);
            _balances[_devAddress] = _balances[_devAddress].add(devValue);
            sendInviteValue(from,to,inviteValue);
        }
        _balances[from] = _balances[from].sub(value);
        _balances[to] = _balances[to].add(nowValue);
        emit Transfer(from, to, nowValue);
    }
    
    function sendInviteValue(address from,address to, uint256 inviteValue) private {
        address sjAddress = inviteInfo[to];
        if (!isContract(from)) {
            sjAddress = inviteInfo[from];
        } 
        uint256 inviteOneValue = inviteValue.div(5);
        for (uint256 i; i < 5; i++) {
            if (sjAddress != address(0)) {
                _balances[sjAddress] = _balances[sjAddress].add(inviteOneValue);
                emit Transfer(from, sjAddress, inviteOneValue);
                sjAddress = inviteInfo[sjAddress];
            } else {
                break;
            }
        }
    }
    
    function setIsExcludedFromFee(address account,bool _bool) public onlyOwner{
        _isExcludedFromFee[account] = _bool;
    }
    
    function setFee(uint256 dFee,uint256 iFee) public onlyOwner{
        _devFee = dFee;
        _inviteFee = iFee;
    }
    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}
    /**
     * @return the name of the token.
     */
    function name() public view returns (string memory) {
        return _name;
    }

    /**
     * @return the symbol of the token.
     */
    function symbol() public view returns (string memory) {
        return _symbol;
    }

    /**
     * @return the number of decimals of the token.
     */
    function decimals() public view returns (uint8) {
        return _decimals;
    }

    /**
    * @dev Total number of tokens in existence
    */
    function totalSupply() public override view returns (uint256) {
        return _totalSupply;
    }

