pragma solidity ^0.8.9;

contract Governance {
    using SafeMath for uint256;
    struct Proposal {
        /// @notice Unique id for looking up a proposal
        uint256 id;
        /// @notice Creator of the proposal
        address proposer;
        /// @notice The timestamp that the proposal will be available for execution, set once the vote succeeds
        address target;
        /// @notice The ordered list of function signatures to be called
        string signature;
        /// @notice The ordered list of calldata to be passed to each call
        bytes callData;
        /// @notice The block timestamp at which voting begins: holders must delegate their votes prior to this timestamp
        uint256 startTime;
        /// @notice The block timestamp at which voting ends: votes must be cast prior to this timestamp
        uint256 endTime;
        /// @notice Current number of votes in favor of this proposal
        uint256 forVotes;
        /// @notice Flag marking whether the proposal has been canceled
        bool canceled;
        /// @notice Flag marking whether the proposal has been executed
        bool executed;
        /// @notice Receipts of ballots for the entire set of voters
        mapping(uint256 => address) receipts;
        /// @notice  description to be called
        string description;
    }
    /// @notice Possible states that a proposal may be in
    enum ProposalState {
        Active,
        Canceled,
        Defeated,
        Succeeded,
        Executed
    }
    /**
     * @notice the period for a proposal to be valid for voters.
     */
    uint256 public votingPeriod = 24 * 60 * 60;
    /// @notice The total number of proposals
    uint256 public proposalCount;
    /// @notice The official record of all proposals ever proposed
    mapping(uint256 => Proposal) public proposals;
    /// @notice mining farm smart contract address.
    address public miningFarmAddress;
    SixPackMiningFarm SixpackMiningFarmContract;
    /**
     * @dev minimum cumulative vote that a proposal could perceive as a winner
     */
    uint256 public proposalThreshold = 31;
    /// @notice This modifier checks that only admin (The owner of form 1 to 6) is allowed to call the function
    modifier onlyAdmin() {
        bool hasPermission = false;
        for (uint256 tokenId = 1; tokenId <= 6; tokenId++) {
            if (SixpackMiningFarmContract.ownerOf(tokenId) == msg.sender) {
                hasPermission = true;
                break;
            }
        }
        require(hasPermission, "Only Admin");
        _;
    }
    /// @notice An event will be emitted when a vote is casted on a proposal
    event VoteCast(
        address voter,
        uint256 proposalId,
        uint256 votes,
        uint256[] voterFarms
    ); /// @notice An event will be emitted when mining farm collection address changes
    event changeFarmCollection(address newFarmCollection);
    /// @notice An event will be emitted when proposalThreshold is modified
    event NewProposalThreshold(uint256 proposalThreshold);
    /// @notice An event will be emitted when votingPeriod is modified
    event NewVotingPeriod(uint256 votingPeriod);
    /// @notice An event will be emitted when a proposal is canceled
    event ProposalCanceled(uint256 proposalId);
    /// @notice An event will be emitted when a new proposal is created
    event ProposalCreated(
        uint256 id,
        address proposer,
        address targets,
        string signatures,
        bytes callData,
        uint256 startTime,
        uint256 endTime,
        string description
    );
    /// @notice An event will be emitted when a proposal is executed
    event ProposalExecuted(uint256 id);

    constructor(address _miningFarmAddress) {
        _setFarmCollection(_miningFarmAddress);
    }

    /**
     * @notice  by calling this function, the admin can set miningFarm address.
     * @dev this method shoud be called immediately after depolying contract.
     * @param _miningFarmAddress address of miningFarm.
     */
    function setFarmCollection(address _miningFarmAddress)
        external
        onlyAdmin
        returns (bool)
    {
        _setFarmCollection(_miningFarmAddress);
        return true;
    }

    function _setFarmCollection(address _miningFarmAddress)
        private
        returns (bool)
    {
        miningFarmAddress = _miningFarmAddress;
        SixpackMiningFarmContract = SixPackMiningFarm(
            address(miningFarmAddress)
        );
        emit changeFarmCollection(miningFarmAddress);
        return true;
    }

    /**
     * @notice change proposalThreshold value, only admin can change this value
     * @param _proposalThreshold new amount of proposalThreshold
     */
    function setProposalThreshold(uint256 _proposalThreshold)
        external
        onlyAdmin
        returns (bool)
    {
        proposalThreshold = _proposalThreshold;
        emit NewProposalThreshold(proposalThreshold);

        return true;
    }

    /**
     * @notice change votingPeriod value, only admin can change this value
     * @param _votingPeriod new amount of votingPeriod
     */
    function setVotingPeriod(uint256 _votingPeriod)
        external
        onlyAdmin
        returns (bool)
    {
        votingPeriod = _votingPeriod;
        emit NewVotingPeriod(votingPeriod);

        return true;
    }

    /**
     * @notice create a proposal
     * @param target contract address
     * @param signature function signature in the contract
     * @param callData function parameters value
     * @param description optional description for this proposal
     */
    function createProposal(
        address target,
        string calldata signature,
        bytes calldata callData,
        string calldata description
    ) external onlyAdmin {
        uint256 startTime = block.timestamp;
        uint256 endTime = startTime.add(votingPeriod);

        proposalCount++;
        uint256 proposalId = proposalCount;

        Proposal storage newProposal = proposals[proposalId];
        require(
            newProposal.id == 0,
            "Governance::Propose: ProposalID collsion"
        );
        newProposal.id = proposalId;
        newProposal.proposer = msg.sender;
        newProposal.target = target;
        newProposal.signature = signature;
        newProposal.callData = callData;
        newProposal.startTime = startTime;
        newProposal.endTime = endTime;
        newProposal.forVotes = 0;
        newProposal.canceled = false;
        newProposal.executed = false;
        newProposal.description = description;

        emit ProposalCreated(
            newProposal.id,
            newProposal.proposer,
            newProposal.target,
            newProposal.signature,
            newProposal.callData,
            newProposal.startTime,
            newProposal.endTime,
            newProposal.description
        );
    }

    /**
     * @notice cast vote for a proposal
     * @param proposalId id of proposal
     * @param myFarms list of farms id
     */
    function castVote(uint256[] calldata myFarms, uint256 proposalId) external {
        for (uint256 i = 0; i < myFarms.length; i++) {
            uint256 farmId = myFarms[i];
            require(
                SixpackMiningFarmContract.ownerOf(farmId) == msg.sender,
                string.concat(
                    "Governance::CastVote: Owner error of farmId ",
                    Strings.toString(farmId)
                )
            );
            require(
                SixpackMiningFarmContract.getFarmType(farmId) ==
                    SixPackMiningFarm.FarmType.TURBO,
                string.concat(
                    "Governance::CastVote: Size error of farmId ",
                    Strings.toString(farmId)
                )
            );
        }

        return _castVote(myFarms, proposalId);
    }

    function _castVote(uint256[] calldata myFarms, uint256 proposalId) private {
        require(
            state(proposalId) == ProposalState.Active,
            "Governance::_castVote: voting is not active"
        );

        Proposal storage proposal = proposals[proposalId];
        for (uint256 i = 0; i < myFarms.length; i++) {
            uint256 farmId = myFarms[i];
            require(
                proposal.receipts[farmId] == address(0),
                string.concat(
                    "Governance::_castVote: A vote has already been registered with this farmId ",
                    Strings.toString(farmId)
                )
            );
            proposal.receipts[farmId] = msg.sender;
        }
        proposal.forVotes = proposal.forVotes.add(myFarms.length);

        emit VoteCast(msg.sender, proposalId, proposal.forVotes, myFarms);
    }

    /**
     * @notice get status of a proposal
     * @dev order of proposal lifecycle and conditions
     * @param proposalId id of proposal
     */
    function state(uint256 proposalId) public view returns (ProposalState) {
        require(
            proposalCount >= proposalId && proposalId > 0,
            "Governance::State: invalid proposal id"
        );

        Proposal storage proposal = proposals[proposalId];

        if (proposal.executed) {
            return ProposalState.Executed;
        } else if (proposal.canceled) {
            return ProposalState.Canceled;
        } else if (block.timestamp <= proposal.endTime) {
            return ProposalState.Active;
        } else if (proposal.forVotes < proposalThreshold) {
            return ProposalState.Defeated;
        }
        return ProposalState.Succeeded;
    }

    /**
     * @notice execute a succeeded proposal
     * @dev the proposal status must be Succeeded
     * @param proposalId id of proposal
     */
    function executeProposal(uint256 proposalId)
        external
        returns (bytes memory)
    {
        require(
            state(proposalId) == ProposalState.Succeeded,
            "Governance::_castVote: proposal is not Succeed"
        );
        Proposal storage proposal = proposals[proposalId];
        proposal.executed = true;

        bytes memory callData = abi.encodePacked(
            bytes4(keccak256(bytes(proposal.signature))),
            proposal.callData
        );
        (bool success, bytes memory returnData) = proposal.target.call(
            callData
        );
        require(
            success,
            "Governance::executeProposal: Transaction execution reverted."
        );
        emit ProposalExecuted(proposalId);
        return returnData;
    }

    /**
     * @notice cancel a proposal
     * @dev the proposal status must not be Executed, and caller must be admin
     * @param proposalId id of proposal
     */
    function cancelProposal(uint256 proposalId) external onlyAdmin {
        require(
            state(proposalId) != ProposalState.Executed,
            "Governance::cancel: cannot cancel executed proposal"
        );
        Proposal storage proposal = proposals[proposalId];
        proposal.canceled = true;
        emit ProposalCanceled(proposalId);
    }
}

