contract MsgWrap is SafeOwnable {
    using SafeMath for uint256;

    address public tokenAddress;
    address public usdtAddress;

    constructor(address _usdt) {
        
        usdtAddress = _usdt;
 
    }

    function setToken(address _token) external onlyOwner {
        tokenAddress = _token;
    }

    function setUsdt(address _usdt) external onlyOwner {
        usdtAddress = _usdt;
    }

    function getToken() public view returns (address) {
        return tokenAddress;
    }

    function getUsdt() public view returns (address) {
        return usdtAddress;
    }


    function withdrawToToken(address _token) external {
        uint256 tokenBalance = IERC20(_token).balanceOf(address(this));
        if (tokenBalance == 0) {
            return;
        }
        IERC20(_token).transfer(tokenAddress, tokenBalance);
    }

    function withdraw(address _token) public onlyOwner {
        uint256 tokenBalance = IERC20(_token).balanceOf(address(this));
        if (tokenBalance == 0) {
            return;
        }
        IERC20(_token).transfer(msg.sender, tokenBalance);
    }
}