// File: PiBridgeMinerNft.sol


pragma solidity ^0.8.3;












contract PiBridgeMinerNft is
    Context,
    AccessControlEnumerable,
    ERC721Enumerable,
    ERC721Burnable,
    Pausable,
    Ownable,
    ReentrancyGuard
{
    using SafeMath for uint256;
    using SafeERC20 for ERC20;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdTracker;

    bool public isPause;
    string private _baseTokenURI = "";
    address public paymentToken;
    uint256 public baseRate = 10000;
    address public devWallet;
    address public nftAirDrop;

    mapping(uint256 => uint256) public nftTypes;
    mapping(uint256 => uint256) public nftTypePriceMap;
    mapping(uint256 => uint256) public nftTypeMaxSupplyMap;
    mapping(uint256 => uint256) public nftTypeUpgradeFeeMap;

    mapping(uint256 => uint256) public nftRefMap; // 0: no ref, > 1: ref, 2: 1ref, 3: 2ref, 4: 3ref,...
    mapping(uint256 => uint256) public nftRefBonusPercentLv1Map; // set for default bonus
    mapping(uint256 => uint256) public nftRefBonusPercentLv2Map; // set for default bonus
    mapping(uint256 => uint256) public nftRefCustomBonusPercentLv1Map; // set for custom user bonus
    mapping(uint256 => uint256) public nftRefCustomBonusPercentLv2Map; // set for custom user bonus
    mapping(uint256 => uint256) public nftRefBonusMap; 
    
    mapping(uint256 => uint256) public nftParentMap;

    mapping(address => bool) public blackListMap;

    // swap nft AirDrop
    uint256 public nftAirDropType;
    uint256 public nftAirDropFee;

    event nftMinted(uint256 nftId, uint256 nftType);
    event nftUpgraded(uint256 nftId, uint256 nftType);
    event nftProccessComission(uint256 nftId, uint256 nftType, uint256 comission);
    event nftRefBonusPaid(uint256 nftId, uint256 nftType, uint256 bonus);
    event nftEmergencyWithdraw(address token, uint256 amount);

    constructor(address _paymentToken, address _devWallet, address _nftAirDrop) ERC721("PiBridge Miner Nft", "PIBMINER") {
        _tokenIdTracker.increment();
        paymentToken = _paymentToken;
        devWallet = _devWallet;
        nftAirDrop = _nftAirDrop;
        isPause = true;
    }

    modifier isRun() {
        require(!paused(), "PiBridgeMinerNft: paused");
        require(!isPause, "PiBridgeMinerNft: paused");
        _;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string memory baseTokenURI) external onlyOwner {
        _baseTokenURI = baseTokenURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        string memory currentBaseUri = _baseURI();
        return
            bytes(currentBaseUri).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseUri,
                        Strings.toString(tokenId),
                        ".json"
                    )
                )
                : "";
    }

    function getNftType(uint256 tokenId) public view returns (uint256) {
        return nftTypes[tokenId];
    }

    function mint(address toAddress, uint256 nftType) external onlyOwner returns (uint256) {
        require(toAddress != address(0), "could not mint to zero address");
        require(nftType > 0, "nftType must be greater than 0");

        _mint(toAddress, _tokenIdTracker.current());
        nftTypes[_tokenIdTracker.current()] = nftType;
        emit nftMinted(_tokenIdTracker.current(), nftType);
        _tokenIdTracker.increment();

        return _tokenIdTracker.current() - 1;
    }
    // Swap Nft
    function swapNft(uint256 nftId) external isRun returns (uint256) {
        require(blackListMap[_msgSender()] == false, "You are in black list");
        address _owner = ERC721(nftAirDrop).ownerOf(nftId);
        require(_owner == _msgSender(), "You are not owner of this NFT");
        // transfer nft to devWallet
        ERC721(nftAirDrop).transferFrom(_owner, devWallet, nftId);
        // transfer fee to devWallet
        ERC20(paymentToken).safeTransferFrom(_msgSender(), devWallet, nftAirDropFee);
        // mint nft
        uint256 tokenId = _tokenIdTracker.current();
        _mint(_msgSender(), tokenId);
        nftTypes[tokenId] = nftAirDropType;
        // set ref
        nftRefMap[tokenId] = nftRefMap[tokenId].add(1);

        emit nftMinted(tokenId, nftAirDropType);
        _tokenIdTracker.increment();

        return _tokenIdTracker.current() - 1;
    }

    // Buy Nft
    function buyNft(uint256 nftType, uint256 nftRefId) external isRun returns (uint256) {
        require(blackListMap[_msgSender()] == false, "You are in black list");
        if (nftRefId != 0) {
            require(nftTypes[nftRefId] != 0, "Ref Nft not found");
            require(ownerOf(nftRefId) != _msgSender(), "You can not buy with your own Nft ref code");
        }
        require(nftTypeMaxSupplyMap[nftType] > 0, "nftType not found");
        // decrease supply
        nftTypeMaxSupplyMap[nftType] = nftTypeMaxSupplyMap[nftType].sub(1);
        // mint nft
        uint256 tokenId = _tokenIdTracker.current();
        _mint(_msgSender(), tokenId);
        nftTypes[tokenId] = nftType;
        // calculate bonus
        uint256 price = nftTypePriceMap[nftType];
        processCommission(nftRefId, price);
        // set ref
        nftRefMap[tokenId] = nftRefMap[tokenId].add(1);
        if (nftRefId > 0) {
            nftRefMap[nftRefId] = nftRefMap[nftRefId].add(1);
            nftParentMap[tokenId] = nftRefId; // set parent
        }
        emit nftMinted(tokenId, nftType);
        _tokenIdTracker.increment();

        return _tokenIdTracker.current() - 1;
    }

    // Upgrade Nft
    function upgradeNft(uint256 tokenId) external isRun {
        require(blackListMap[_msgSender()] == false, "You are in black list");
        require(nftRefMap[tokenId] > 0, "nftType not found");
        address _owner = ownerOf(tokenId);
        require(_owner == _msgSender(), "not owner");
        // set type
        nftTypes[tokenId] = nftTypes[tokenId].add(1);
        uint256 nftRefId = nftParentMap[tokenId];
        // calculate bonus
        uint256 price = nftTypeUpgradeFeeMap[nftTypes[tokenId]];
        processCommission(nftRefId, price);
        emit nftUpgraded(tokenId, nftTypes[tokenId]);
    }

    // proccess commission
    function processCommission(uint256 nftRefId, uint256 price) internal {
        uint256 slippage1 = 0;
        uint256 slippage2 = 0;
        // calculate bonus 1
        uint256 nftTypeRef = nftTypes[nftRefId];
        uint256 bonusPercentLv1 = nftRefBonusPercentLv1Map[nftTypeRef];
        if (nftRefCustomBonusPercentLv1Map[nftRefId] > 0 && nftRefCustomBonusPercentLv1Map[nftRefId] < nftRefBonusPercentLv1Map[nftTypeRef]) {
            bonusPercentLv1 = nftRefCustomBonusPercentLv1Map[nftRefId];
        }
        nftRefBonusMap[nftRefId] = nftRefBonusMap[nftRefId].add(price.mul(bonusPercentLv1).div(baseRate));

        // Check discount
        if (bonusPercentLv1 < nftRefBonusPercentLv1Map[nftTypeRef]) {
            // slippage
            slippage1 = price.mul(nftRefBonusPercentLv1Map[nftTypeRef]).mul((nftRefBonusPercentLv1Map[nftTypeRef].sub(bonusPercentLv1))).div(nftRefBonusPercentLv1Map[nftTypeRef]).div(baseRate);
        }

        // Emit event
        emit nftProccessComission(nftRefId, nftTypeRef, nftRefBonusMap[nftRefId]);
        // calculate bonus 2
        if (nftParentMap[nftRefId] > 0) { // has parent
            uint256 parentId = nftParentMap[nftRefId];
            uint256 parentType = nftTypes[parentId];
            uint256 bonusPercentLv2 = nftRefBonusPercentLv2Map[parentType];
            if (nftRefCustomBonusPercentLv2Map[parentId] > 0) {
                bonusPercentLv2 = nftRefCustomBonusPercentLv2Map[parentId];
            }
            nftRefBonusMap[parentId] = nftRefBonusMap[parentId].add(price.mul(bonusPercentLv2).div(baseRate));

            // Check discount
            if (bonusPercentLv2 < nftRefBonusPercentLv2Map[parentType] && nftRefCustomBonusPercentLv2Map[parentId] < nftRefBonusPercentLv2Map[parentType]) {
                // slippage
                slippage2 = price.mul(nftRefBonusPercentLv2Map[nftTypeRef]).mul((nftRefBonusPercentLv2Map[nftTypeRef].sub(bonusPercentLv2))).div(nftRefBonusPercentLv2Map[nftTypeRef]).div(baseRate);
            }

            // Emit event
            emit nftProccessComission(parentId, parentType, nftRefBonusMap[parentId]);
        }
        // calculate price after discount
        price = price.sub(slippage1).sub(slippage2);
        // make payment
        ERC20(paymentToken).safeTransferFrom(
            _msgSender(),
            address(this),
            price
        );
    }

    // Claim referral bonus
    // Check non-reentrancy
    function claimReferralBonus(uint256 tokenId) external nonReentrant isRun {
        require(blackListMap[_msgSender()] == false, "You are in black list");
        require(nftRefBonusMap[tokenId] > 0, "no bonus to claim");
        address _owner = ownerOf(tokenId);
        require(_owner == _msgSender(), "not owner");
        
        // Safe transfer payment
        ERC20(paymentToken).safeTransfer(
            _owner,
            nftRefBonusMap[tokenId]
        );

        // Set bonus to 0
        nftRefBonusMap[tokenId] = 0;

        emit nftRefBonusPaid(tokenId, nftTypes[tokenId], nftRefBonusMap[tokenId]);
    }
    
    // calculate discount
    function calculateDiscount(uint256 typeNft, uint256 nftRefId) external view returns (uint256) {
        require(nftTypes[nftRefId] != 0, "Ref Nft not found");
        uint256 slippage1 = 0;
        uint256 slippage2 = 0;
        // calculate bonus 1
        uint256 nftTypeRef = nftTypes[nftRefId];
        uint256 price = nftTypePriceMap[typeNft];
        uint256 bonusPercentLv1 = nftRefBonusPercentLv1Map[nftTypeRef];
        if (nftRefCustomBonusPercentLv1Map[nftRefId] > 0 && nftRefCustomBonusPercentLv1Map[nftRefId] < nftRefBonusPercentLv1Map[nftTypeRef]) {
            bonusPercentLv1 = nftRefCustomBonusPercentLv1Map[nftRefId];
        }
        // Check discount
        if (bonusPercentLv1 < nftRefBonusPercentLv1Map[nftTypeRef]) {
            // slippage
            slippage1 = price.mul(nftRefBonusPercentLv1Map[nftTypeRef]).mul((nftRefBonusPercentLv1Map[nftTypeRef].sub(bonusPercentLv1))).div(nftRefBonusPercentLv1Map[nftTypeRef]).div(baseRate);
        }
        // calculate bonus 2
        if (nftParentMap[nftRefId] > 0) { // has parent
            uint256 parentId = nftParentMap[nftRefId];
            uint256 parentType = nftTypes[parentId];
            uint256 bonusPercentLv2 = nftRefBonusPercentLv2Map[parentType];
            if (nftRefCustomBonusPercentLv2Map[parentId] > 0 && nftRefCustomBonusPercentLv2Map[parentId] < nftRefBonusPercentLv2Map[parentType]) {
                bonusPercentLv2 = nftRefCustomBonusPercentLv2Map[parentId];
            }
            // Check discount
            if (bonusPercentLv2 < nftRefBonusPercentLv2Map[parentType]) {
                // slippage
                slippage2 = price.mul(nftRefBonusPercentLv2Map[nftTypeRef]).mul((nftRefBonusPercentLv2Map[nftTypeRef].sub(bonusPercentLv2))).div(nftRefBonusPercentLv2Map[nftTypeRef]).div(baseRate);
            }
        }
        // calculate price after discount
        price = price.sub(slippage1).sub(slippage2);
        return price;
    }

    // Set black list
    function setBlackList(address[] memory account, bool _enable) external onlyOwner {
        for (uint256 i = 0; i < account.length; i++) {
            blackListMap[account[i]] = _enable;
        }
    }

    // set nftTypePriceMap
    function setNftTypePriceMap(uint256[] memory _nftType, uint256[] memory _prices) external onlyOwner {
        for (uint256 i = 0; i < _nftType.length; i++) {
            nftTypePriceMap[_nftType[i]] = _prices[i];
        }
    }

    // set nftTypeMaxSupplyMap
    function setNftTypeMaxSupplyMap(uint256[] memory _nftType, uint256[] memory _maxSupply) external onlyOwner {
        for (uint256 i = 0; i < _nftType.length; i++) {
            nftTypeMaxSupplyMap[_nftType[i]] = _maxSupply[i];
        }
    }

    // set nftTypeUpgradeFeeMap
    function setNftTypeUpgradeFeeMap(uint256[] memory _nftType, uint256[] memory _upgradeFee) external onlyOwner {
        for (uint256 i = 0; i < _nftType.length; i++) {
            nftTypeUpgradeFeeMap[_nftType[i]] = _upgradeFee[i];
        }
    }

    // set nftRefBonusPercentLv1Map
    function setNftRefBonusPercentLv1Map(uint256[] memory _nftType, uint256[] memory _bonusPercent) external onlyOwner {
        for (uint256 i = 0; i < _nftType.length; i++) {
            nftRefBonusPercentLv1Map[_nftType[i]] = _bonusPercent[i];
        }
    }

    // set nftRefBonusPercentLv2Map
    function setNftRefBonusPercentLv2Map(uint256[] memory _nftType, uint256[] memory _bonusPercent) external onlyOwner {
        for (uint256 i = 0; i < _nftType.length; i++) {
            nftRefBonusPercentLv2Map[_nftType[i]] = _bonusPercent[i];
        }
    }    

    // set nftRefMap
    function setNftRefMap(uint256[] memory _tokenId, uint256[] memory _refCount) external onlyOwner {
        for (uint256 i = 0; i < _tokenId.length; i++) {
            nftRefMap[_tokenId[i]] = _refCount[i];
        }
    }

    // set nftRefBonusMap
    function setNftRefBonusMap(uint256[] memory _tokenId, uint256[] memory _bonus) external onlyOwner {
        for (uint256 i = 0; i < _tokenId.length; i++) {
            nftRefBonusMap[_tokenId[i]] = _bonus[i];
        }
    }

    // set nftParentMap
    function setNftParentMap(uint256[] memory _tokenId, uint256[] memory _parentId) external onlyOwner {
        for (uint256 i = 0; i < _tokenId.length; i++) {
            nftParentMap[_tokenId[i]] = _parentId[i];
        }
    }

    // set baseRate
    function setBaseRate(uint256 _baseRate) external onlyOwner {
        baseRate = _baseRate;
    }

    // owner nft can set bonus fee
    // require new value <= nftBonusType
    function setNftRefCustomBonusPercentLv1Map(uint256 tokenId, uint256 _bonus) external {
        require(ownerOf(tokenId) == _msgSender(), "not owner");
        uint256 nftType = nftTypes[tokenId];
        uint256 nftBonusType = nftRefBonusPercentLv1Map[nftType];
        require(_bonus <= nftBonusType, "bonus too high");
        nftRefCustomBonusPercentLv1Map[tokenId] = _bonus;
    }

    function setNftRefCustomBonusPercentLv2Map(uint256 tokenId, uint256 _bonus) external {
        require(ownerOf(tokenId) == _msgSender(), "not owner");
        uint256 nftType = nftTypes[tokenId];
        uint256 nftBonusType = nftRefBonusPercentLv2Map[nftType];
        require(_bonus <= nftBonusType, "bonus too high");
        nftRefCustomBonusPercentLv2Map[tokenId] = _bonus;
    }
    // set devWallet
    function setDevWallet(address _devWallet) external onlyOwner {
        devWallet = _devWallet;
    }
    // set nftAirDrop
    function setNftAirDrop(address _nftAirDrop) external onlyOwner {
        nftAirDrop = _nftAirDrop;
    }
    // set AirDrop fee and type
    function setAirDrop(uint256 _nftAirDropFee, uint256 _nftAirDropType) external onlyOwner {
        nftAirDropFee = _nftAirDropFee;
        nftAirDropType = _nftAirDropType;
    }
    // Emergency withdraw
    function emergencyWithdraw() external onlyOwner {
        uint256 balance = ERC20(paymentToken).balanceOf(address(this));
        ERC20(paymentToken).transfer(owner(), balance);
        emit nftEmergencyWithdraw(paymentToken, balance);
    }

    function pause() public onlyOwner {
        _pause();
        isPause = true;
    }

    function unpause() public onlyOwner {
        _unpause();
        isPause = false;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlEnumerable, ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}