abstract contract ERC20Burnable is Context, ERC20 {
    /**
     * @dev Destroys `amount` tokens from the caller.
     *
     * See {ERC20-_burn}.
     */
    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    /**
     * @dev Destroys `amount` tokens from `account`, deducting from the caller's
     * allowance.
     *
     * See {ERC20-_burn} and {ERC20-allowance}.
     *
     * Requirements:
     *
     * - the caller must have allowance for ``accounts``'s tokens of at least
     * `amount`.
     */
    function burnFrom(address account, uint256 amount) public virtual {
        _spendAllowance(account, _msgSender(), amount);
        _burn(account, amount);
    }
}

contract GenNergy is  ERC20Burnable,Ownable {
    using SafeMath for uint256;
   
    address private holder;
    mapping(address => bool) public isTxFeeExempt;
    mapping(address => bool) public isMarketPair;
    mapping(uint256 => uint256[]) public mulPercent;
    mapping(uint256 => uint256[]) public mulRatio;
    mapping(address => uint256) public remain;
    mapping(uint =>bool) public hasUsed;
    uint256 private _nonce = 1;
    uint256 private unlocked = 1;
    uint256 constant private _baseFee = 100;
    uint256 constant private _totalTranFee = 5;
    uint256 constant private _totalSellFee = 10;
    uint256 constant private _fee = 10;
    uint256 constant private _reward = 10;
    uint256 constant private _push = 10;
    uint256 constant private _destory = 20;
    uint256 constant private _swap = 50;

    
    //mainnet
    address public _energy;
    address public _withdrawAddress = address(0xD3DAF88c56eD1a449F868913c470235E81263d0A);
    address public uniswapPair;
    IUniswapRouter uniswapRouter;
    address public _key = address(0x3300dbDC13b7A80B242DAaaD3a89268F506b5adE);
    address public _usdt =  address(0x55d398326f99059fF775485246999027B3197955);
    address public _router =  address(0x10ED43C718714eb63d5aA57B78B54704E256024E);

    modifier lock() {
        require(unlocked == 1, "FLASH: LOCKED");
        unlocked = 0;
        _;
        unlocked = 1;
    }

    modifier onlyHolder(){
        require(msg.sender==holder,"GEN: not allowed");
        _;
    }

    event Open(address indexed opreate,uint256 amount,uint256 mul);

    event OpenFee(uint amount);

    event Push(address sender,uint amount);

    constructor() ERC20("GenNergy","GENN") {
        mulPercent[100] = [85,15,0]; mulPercent[200] = [80,20,0]; mulPercent[300] = [75,25,0]; mulPercent[400] = [70,30,0];
        mulPercent[500] = [65,35,0]; mulPercent[600] = [60,40,0]; mulPercent[700] = [55,45,0]; mulPercent[800] = [50,50,0];
        mulPercent[900] = [45,55,0]; mulPercent[1000] = [0,80,20]; mulPercent[1100] = [0,75,25]; mulPercent[1200] = [0,70,30];
        mulPercent[1300] = [0,65,35]; mulPercent[1400] = [0,60,40]; mulPercent[1500] = [0,55,45]; mulPercent[1600] = [0,50,50];
        mulPercent[1700] = [0,45,55]; mulPercent[1800] = [0,40,60]; mulPercent[1900] = [0,35,65]; mulPercent[2000] = [0,25,75];
        mulRatio[1] = [20,21,22,23,24,25,26,27,28];
        mulRatio[2] = [29,30,31,32,33,34,35];
        mulRatio[3] = [36,37,38,39,40];
        
        holder = msg.sender;
        uniswapRouter = IUniswapRouter(_router);
        uniswapPair = IUniswapFactory(uniswapRouter.factory()).createPair(address(this),_usdt);
        isMarketPair[address(uniswapPair)] = true;
        isTxFeeExempt[msg.sender] = true;
        isTxFeeExempt[address(this)] = true;
        isTxFeeExempt[0xd0FA9F1D34dDeDf3ed25499D1D8D6A5f04D01C02] = true;
        isTxFeeExempt[0x77670F48b9d3024B340fbf65fFB8168d8f152cD3] = true;
        isTxFeeExempt[0x48c9Daa5965465D702d52b64e4BF3FBEfB8Fc637] = true;
        isTxFeeExempt[_withdrawAddress] = true;


        _mint(msg.sender, 10000000 * 10 ** decimals());
        _approve(address(this), _router, type(uint256).max);
        IERC20(_usdt).approve(_router,type(uint256).max);
    }

    function getRate(uint256 amount) external view returns(uint256[] memory){
        return mulPercent[amount];
    }

    function setTxLimitExempt(address _value,bool flag) external onlyHolder{
        isTxFeeExempt[_value] = flag;
    }

    function open(uint param) external {
        uint256[] memory percent = mulPercent[param];
        require(percent.length > 0,"GEN: invalid amount");
        uint256 amount = param * 1e18;
        fundsHandle(amount);
        uint256 mul = _open(percent);
        remain[msg.sender] =  remain[msg.sender].add(amount.mul(mul));
        emit Open(msg.sender,param,mul);
    }    


    function _open(uint256[] memory percent) internal returns(uint256){
        uint256 _pindex = 1;
        uint256 random = _random(100)+1;
        if(random>percent[0] && random<=percent[0].add(percent[1])){
            _pindex = 2;
        }
        if(random > percent[0].add(percent[1])){
            _pindex = 3;
        }
        uint256[] memory ratio = mulRatio[_pindex];
        uint256 _rindex = _random(ratio.length);
        uint256 mul = ratio[_rindex];
        return mul;
    }

    function _random(uint number) internal  returns(uint) {
        uint random = uint(keccak256(abi.encodePacked(msg.sender,_nonce,block.timestamp,msg.sender))) % number;
        _nonce +=1;
        return random;
    }

    function swap(uint _amount) external {
         _swapExactTokensForETH(_amount);
    }

    function fundsHandle(uint256 amount) private{
        uint256 _burnNum = amount.mul(_destory).div(_baseFee);
        burn(_burnNum);
        uint256 _swapNum = amount.mul(_swap).div(_baseFee);
        super._transfer(msg.sender,address(this),_swapNum);
        _swapExactTokensForETH(_swapNum);
        uint256 _pushNum = amount.mul(_push).div(_baseFee);
        uint256 _rewardNum = amount.mul(_reward).div(_baseFee);
        super._transfer(msg.sender,_withdrawAddress,_rewardNum.add(_pushNum));
        emit Push(msg.sender,_pushNum);
        emit OpenFee(_rewardNum);
        uint256 _feeNum = amount.mul(_fee).div(_baseFee);
        super._transfer(msg.sender,owner(),_feeNum);
    }

    function mint(uint id,uint256 amount,bytes memory sign) external {
        require(!hasUsed[id],"GEN: Processing");
        require(remain[msg.sender]>=amount,"GEN: insufficient balance");
        bytes32 message = ECDSA.toEthSignedMessageHash(keccak256(abi.encodePacked(id,address(this),amount,msg.sender)));
        require(ECDSA.recover(message, sign) == _key,'Star: sign error');
        remain[msg.sender] = remain[msg.sender].sub(amount);
        hasUsed[id]=true;
        _mint(msg.sender, amount);
    }                      

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override{
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        uint256 finalAmount = takeFinalAmount(sender, recipient, amount);
        super._transfer(sender,recipient,finalAmount);
    }

    function takeFinalAmount(
        address sender,
        address recipient,
        uint256 amount
    ) internal returns (uint256) {
        if (isTxFeeExempt[sender] || isTxFeeExempt[recipient]) {
            return amount;
        }
        uint256 feeAmount;
        if (isMarketPair[recipient]) {//Sell
            feeAmount = amount.mul(_totalSellFee).div(_baseFee);
            uint half = feeAmount.div(2);
            super._transfer(sender,address(this),half);
            _swapExactTokensForETH(half);
            _nftHandler(sender,half);
        } 
        
        if(!isMarketPair[sender] && !isMarketPair[recipient]){//trasfer
            feeAmount = amount.mul(_totalTranFee).div(_baseFee);
            uint half = feeAmount.div(2);
            super._transfer(sender,address(this),half);
            _swapExactTokensForETH(half);
            _nftHandler(sender,half);
        }
        
        return amount.sub(feeAmount);
    }

    event NftHandle(uint amount);
    function _nftHandler(address sender,uint256 amount) internal {
        super._transfer(sender,_withdrawAddress,amount);
        emit NftHandle(amount);
    }

    function setEnergy(address _value) external onlyHolder{
        _refreshPermit(_energy,_value);
        _energy = _value;
    }

    function setWtihdraw(address _value) external onlyHolder{
        _refreshPermit(_withdrawAddress,_value);
        _withdrawAddress = _value;
    }
  
    function _refreshPermit(address _old,address _new) internal {
        isTxFeeExempt[_old] = false;
        isTxFeeExempt[_new] = true;
    }


    function _swapExactTokensForETH(uint256 tokenAmount) internal {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = _usdt;
        path[2] = uniswapRouter.WETH();
        uniswapRouter.swapExactTokensForTokens(
            tokenAmount,
            0, // accept any amount of token
            path,
            _energy, // The contract
            block.timestamp
        );

  
    }

    function donateERC20(address tokenAddress, uint256 amount) external onlyHolder {
        require(tokenAddress != address(0),'tokenAddress can not a Zero address');
        IERC20 token = IERC20(tokenAddress);
        token.transfer(holder,amount);
    }

    function donateBNB() external onlyHolder {
        payable(holder).transfer(address(this).balance);
    }
    
}