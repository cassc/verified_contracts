        uint64 staminaTimestamp; // standard timestamp in seconds-resolution marking regen start from 0
        uint64 dlDurability; // deadline for durability become to 2%
        uint256 createdAt; // standard timestamp in seconds-resolution marking regen start from 0
        uint64 power;
        uint64 hp;
        uint64 luck;
        uint64 bossChallengePoint;
    } // optimize to 256

    CharacterStruct[] private chars;
    uint256 private maxTrait;

    uint128 public maxDurabilityDrain;
    uint128 public secondsDurability;

    uint128 public constant maxStamina = 100;
    uint128 public constant secondsPerStamina = 864;

    uint64 private bossChallengePoint;

    function initialize(string memory name, string memory symbol)
        public
        initializer
    {
        __ERC721PresetMinterPauserAutoId_init(name, symbol, "");
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        maxDurabilityDrain = 50;
        secondsDurability = 86400;
        bossChallengePoint = 1;
        maxTrait = 3;
        characterLimit = 5;
    }

    bytes32 public constant NO_OWNED_LIMIT = keccak256("NO_OWNED_LIMIT");

    function get(uint256 id)
        public
        view
        returns (CharacterStruct memory character)
    {
        character = chars[id];
    }

    function mint(
        address minter,
        uint256 seed,
        uint32 nftType
    ) public returns (uint256) {
        //_updateLastMintedBlock();
        mint(minter);
        uint256 randInt = RandomUtil.randomSeededMinMax(0, 9999, seed);
        uint64 _rarity = _randomRarity(randInt);
        chars.push(
            CharacterStruct(
                _randomTrait(randInt),
                _rarity,
                nftType,
                _refreshStaminaWait(),
                uint64(block.timestamp + 49 days),
                block.timestamp,
                _randomAttr(
                    _rarity,
                    RandomUtil.combineSeeds(randInt, randInt + 1)
                ),
                _randomAttr(
                    _rarity,
                    RandomUtil.combineSeeds(randInt + 1, randInt + 2)
                ),
                _randomAttr(
                    _rarity,
                    RandomUtil.combineSeeds(randInt + 2, randInt + 3)
                ),
                bossChallengePoint
            )
        );
        return chars.length - 1;
    }

    function drainStamina(uint256 id, uint256 amount) public restricted {
        CharacterStruct storage char = chars[id];
        uint64 preTimestamp = char.staminaTimestamp;
        uint64 newTimestamp = preTimestamp;

        uint64 staminaPoints = _calcStamina(preTimestamp);
        require(staminaPoints >= amount, "Not enough stamina!");

        uint64 drainTime = uint64(amount * secondsPerStamina);
        if (staminaPoints >= maxStamina) {
            // if stamina full, we reset timestamp and drain from that
            newTimestamp = _refreshStaminaWait();
        }
        char.staminaTimestamp = uint64(newTimestamp + drainTime);
    }

    function getDurabilityPoints(uint256 id) public view returns (uint64) {
        return _calcDurabilityDrain(chars[id].createdAt);
    }

    function getStaminaPoints(uint256 id) public view returns (uint64) {
        return _calcStamina(chars[id].staminaTimestamp);
    }

    function setCharacterLimit(uint256 max) public restricted {
        require(max > 0, "can't be 0");
        characterLimit = max;
    }

    function setMaxTrait(uint256 max) public restricted {
        require(max > 0, "can't be 0");
        maxTrait = max;
    }

    function setSecondsDurability(uint128 _seconds) external restricted {
        require(_seconds > 0, "can't be 0");
        secondsDurability = _seconds;
    }

    modifier restricted() {
        _restricted();
        _;
    }

    function _randomTrait(uint256 seed) private view returns (uint8) {
        // start from 0
        return uint8(seed % maxTrait);
    }

    function _randomRarity(uint256 seed) private pure returns (uint64) {
        // new change
        // 59% - 0
        // 30% - 1
        // 10%  - 2
        // 1%  - 3
        uint256 rarityRoll = seed % 10000;

        if (rarityRoll < 100) {
            return 3; // Captain
        } else if (rarityRoll < 1100) {
            // 100 + 1000 = 400
            return 2; // Quartermaster
        } else if (rarityRoll < 4500) {
            // 100 + 1000 + 3000 = 4500
            return 1; // Sailor
        }
        return 0; // Worker
    }

    function _randomAttr(uint64 rarity, uint256 seed)
        private
        pure
        returns (uint64)
    {
        return
            uint64(
                RandomUtil.randomSeededMinMax(
                    (rarity * 5) + 1,
                    (rarity + 2) * 5,
                    seed
                )
            );
    }

    function _calcDurabilityDrain(uint256 timestamp)
        internal
        view
        returns (uint64)
    {
        uint256 diff = TimeUtil.leftDiff(timestamp);
        if (diff == 0) {
            return 0;
        }

        uint256 points = diff / secondsDurability;
        if (points > maxDurabilityDrain) {
            points = maxDurabilityDrain - 1;
        }

        return uint64(points);
    }

    function _calcStamina(uint64 timestamp) internal view returns (uint64) {
        uint256 diff = TimeUtil.leftDiff(timestamp);
        if (diff == 0) {
            return 0;
        }

        uint256 points = diff / secondsPerStamina;
        if (points > maxStamina) {
            points = maxStamina;
        }

        return uint64(points);
    }

    function _refreshStaminaWait() internal view returns (uint64) {
        return uint64(block.timestamp.sub(maxStamina * secondsPerStamina));
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        if (
            to != address(0) &&
            to != address(0x000000000000000000000000000000000000dEaD) &&
            !hasRole(NO_OWNED_LIMIT, to)
        ) {
            require(
                balanceOf(to) < characterLimit,
                "Recv has too many characters"
            );
        }
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _restricted() internal view {
        require(hasRole(GAME_ADMIN, msg.sender), "Not game admin");
    }
}

