        if (from == address(0)) {
            _addTokenToAllTokensEnumeration(tokenId);
        } else if (from != to) {
            _removeTokenFromOwnerEnumeration(from, tokenId);
        }
        if (to == address(0)) {
            _removeTokenFromAllTokensEnumeration(tokenId);
        } else if (to != from) {
            _addTokenToOwnerEnumeration(to, tokenId);
        }
    }

    /**
     * @dev Private function to add a token to this extension's ownership-tracking data structures.
     * @param to address representing the new owner of the given token ID
     * @param tokenId uint256 ID of the token to be added to the tokens list of the given address
     */
    function _addTokenToOwnerEnumeration(address to, uint256 tokenId) private {
        uint256 length = ERC721.balanceOf(to);
        _ownedTokens[to][length] = tokenId;
        _ownedTokensIndex[tokenId] = length;
    }

    /**
     * @dev Private function to add a token to this extension's token tracking data structures.
     * @param tokenId uint256 ID of the token to be added to the tokens list
     */
    function _addTokenToAllTokensEnumeration(uint256 tokenId) private {
        _allTokensIndex[tokenId] = _allTokens.length;
        _allTokens.push(tokenId);
    }

    /**
     * @dev Private function to remove a token from this extension's ownership-tracking data structures. Note that
     * while the token is not assigned a new owner, the `_ownedTokensIndex` mapping is _not_ updated: this allows for
     * gas optimizations e.g. when performing a transfer operation (avoiding double writes).
     * This has O(1) time complexity, but alters the order of the _ownedTokens array.
     * @param from address representing the previous owner of the given token ID
     * @param tokenId uint256 ID of the token to be removed from the tokens list of the given address
     */
    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId) private {
        // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = ERC721.balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        delete _ownedTokensIndex[tokenId];
        delete _ownedTokens[from][lastTokenIndex];
    }

    /**
     * @dev Private function to remove a token from this extension's token tracking data structures.
     * This has O(1) time complexity, but alters the order of the _allTokens array.
     * @param tokenId uint256 ID of the token to be removed from the tokens list
     */
    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {
        // To prevent a gap in the tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = _allTokens.length - 1;
        uint256 tokenIndex = _allTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary. However, since this occurs so
        // rarely (when the last minted token is burnt) that we still do the swap here to avoid the gas cost of adding
        // an 'if' statement (like in _removeTokenFromOwnerEnumeration)
        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        // This also deletes the contents at the last position of the array
        delete _allTokensIndex[tokenId];
        _allTokens.pop();
    }
}

contract GBBAS is ERC721Enumerable, ReentrancyGuard, IERC721Receiver, Ownable {
    uint256 public maxSupply;
    uint256 public mintPrice;
    mapping(uint256 => MarketItem) private idToMarketItem;
    mapping (uint256 => string) private _tokenURIs;
    CommissionParty[] parties;
    BEP20Token private immutable TEZA;
    bool public active;

    constructor(uint256 _maxSupply, uint256 _mintPrice) ERC721("GBB Allstar So-so ULTRA RARE", "GBBAS") {
        TEZA = BEP20Token(0x53291A09d8dAa7354852497134897848884f3296);
        maxSupply = _maxSupply;
        mintPrice = _mintPrice;
        active = true;
        parties.push(CommissionParty(payable(0x0000000000000000000000000000000000000001), 250, 0));
        parties.push(CommissionParty(payable(0xf9fCe99C4EA0BE55D5166725e79EEF42eD7a060c), 1750, 250));
        parties.push(CommissionParty(payable(0x0e29d21Ef19440E265383a92DAa3692D67d26696), 3350, 250));
        parties.push(CommissionParty(payable(0xA798eBeD23f2080D4d6975164380b962b6f7bc74), 3350, 250));
        parties.push(CommissionParty(payable(0xc16115Fc0e3619B95a1921a2882EdcDFEF980399), 1300, 250));
    }

    struct CommissionParty {
        address payable wallet;
        uint256 primaryRoyalty;
        uint256 secondaryRoyalty;
    }

    struct MarketItem {
        uint256 tokenId;
        address payable seller;
        address payable owner;
        uint256 price;
        bool sold;
        bool active;
    }

    event MarketItemCreated (
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price,
        bool sold,
        bool active
    );

    event PrimarySaleTransfer(
        uint256 indexed tokenId,
        address buyer,
        address recevier,
        uint256 price
    );

    event SecondarySaleTransfer(
        uint256 indexed tokenId,
        address buyer,
        address recevier,
        uint256 price
    );

    event MarketItemSold (
        uint256 indexed tokenId,
        address seller,
        address buyer,
        uint256 price
    );

    event CommissionPartiesChange (
        address party,
        uint256 prevPrimaryRoyalty,
        uint256 newPrimaryRoyalty,
        uint256 prevSecondaryRoyalty,
        uint256 newSecondaryRoyalty
    );

    event MarketItemDelisted (
        uint256 indexed tokenId
    );

    modifier checkActive() {
        require(active == true, "Contract is paused. Try again later.");
        _;
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function setMintPrice(uint256 _mintPrice) public onlyOwner {
        mintPrice = _mintPrice;
    }

    function mint(uint256 _tokenId, string memory tokenURI_) public nonReentrant checkActive {
        require(totalSupply() < maxSupply, "Total mint limit exceeded.");
        uint transferred = 0;
        for (uint i = 0; i < parties.length; i++) {
            uint toTransfer = mintPrice*parties[i].primaryRoyalty / 10000;
            require(TEZA.transferFrom(msg.sender, parties[i].wallet, toTransfer), "transfer error");
            emit PrimarySaleTransfer(_tokenId, msg.sender, parties[i].wallet, toTransfer);
            transferred += toTransfer;
        }

        require(transferred == mintPrice, 'Transfer error, please contact admin with the transaction hash');
        _mint(msg.sender, _tokenId);
        _setTokenURI(_tokenId, tokenURI_);
    }

    function _setTokenURI(uint256 tokenId, string memory _tokenURI) internal virtual {
        require(_exists(tokenId), "ERC721Metadata: URI set of nonexistent token");
        _tokenURIs[tokenId] = _tokenURI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        string memory _tokenURI = _tokenURIs[tokenId];
        return _tokenURI;
    }

    function createMarketItem(
        uint256 tokenId,
        uint256 price
    ) public payable nonReentrant checkActive {
        require(price > 0, "Price must be at least 1 TEZA");
        require(ownerOf(tokenId) == msg.sender, 'Not token owner');

        idToMarketItem[tokenId] =  MarketItem(
            tokenId,
            payable(msg.sender),
            payable(address(0)),
            price,
            false,
            true
        );

        approve(address(this), tokenId);
        setApprovalForAll(address(this), true);

        emit MarketItemCreated(
            tokenId,
            msg.sender,
            address(0),
            price,
            false,
            true
        );
    }

    function removeMarketItem(
        uint256 tokenId
    ) public payable nonReentrant checkActive {
        MarketItem memory item = idToMarketItem[tokenId];
        require(item.seller == msg.sender, 'Not item owner');
        require(item.active == true, 'Item is already delisted');

        approve(address(0), tokenId);
        item.active = false;
        emit MarketItemDelisted(tokenId);
    }

    function createMarketSale(
        uint256 _tokenId
    ) public payable nonReentrant checkActive {
        MarketItem memory item = idToMarketItem[_tokenId];
        uint price = item.price;
        uint transferred = 0;

        require(item.active, 'Listed item not found');
        for (uint i = 0; i < parties.length; i++) {
            if (parties[i].secondaryRoyalty > 0) {
                uint toTransfer = price*parties[i].secondaryRoyalty / 10000;
                require(TEZA.transferFrom(msg.sender, parties[i].wallet, toTransfer), "transfer error");
                emit SecondarySaleTransfer(_tokenId, msg.sender, parties[i].wallet, toTransfer);
                transferred += toTransfer;
            }
        }
        require(TEZA.transferFrom(msg.sender, item.seller, price - transferred), "transfer error");
        emit MarketItemSold(_tokenId, item.seller, msg.sender, price - transferred);
        IERC721(address(this)).transferFrom(item.seller, msg.sender, _tokenId);
        item.owner = payable(msg.sender);
        item.sold = true;
    }

    function changeCommissionParty(
        address _artistAddress,
        uint256 _primaryRoyalty,
        uint256 _secondaryRoyalty
    ) public onlyOwner {
        uint256 _prevPrimaryRoyalty = 0;
        uint256 _prevSecondaryRoyalty = 0;

        for (uint i = 0; i < parties.length; i++) {
            if (parties[i].wallet == _artistAddress) {
                _prevPrimaryRoyalty = parties[i].primaryRoyalty;
                _prevSecondaryRoyalty = parties[i].secondaryRoyalty;
                parties[i] = parties[parties.length - 1];
                parties.pop();
            }
        }
        if (_primaryRoyalty>0 && _secondaryRoyalty > 0) {
            parties.push(CommissionParty(payable(_artistAddress), _primaryRoyalty, _secondaryRoyalty));
        }

        emit CommissionPartiesChange(_artistAddress, _prevPrimaryRoyalty, _prevSecondaryRoyalty, _primaryRoyalty, _secondaryRoyalty);
    }

    function toggleActive(
        bool _status
    ) public onlyOwner {
        active = _status;
    }

    function getPartiesCommission(
        uint256 _index
    ) public view returns (address, uint256, uint256) {
        return (parties[_index].wallet, parties[_index].primaryRoyalty, parties[_index].secondaryRoyalty);
    }

    function checkOwners(
        uint256[] memory _tokenIds,
        address sender
    ) internal view returns (bool) {
        for(uint i = 0; i < _tokenIds.length; i++) {
            if (ownerOf(_tokenIds[i]) != sender) {
                return false;
            }
        }
        return true;
    }
}