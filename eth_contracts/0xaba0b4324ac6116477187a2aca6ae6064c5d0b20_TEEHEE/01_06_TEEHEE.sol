pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract TEEHEE is ERC20, Ownable {
    uint256 private _totalSupply = 69000000 * 10 ** 18;

    address public MarketingWallet = 0xa6e976acaDa39df5B0f9dC9fd2303C8297c2244C;
    address public burnWallet = 0x000000000000000000000000000000000000dEaD;
    address public uniswapV2Pair;


    constructor() ERC20("Justin Sun GF", "TEEHEE") {
        _mint(msg.sender, _totalSupply);
    }

     function _transfer(address sender, address recipient, uint256 amount) internal override {
        if (recipient == uniswapV2Pair) {
            uint256 tax = amount * 2 / 100; // 2% TAX
            uint256 burn = amount * 1/100;  // 1% BURN
            uint256 netAmount = amount - tax;
            super._transfer(sender, recipient, netAmount);
            super._transfer(sender, MarketingWallet, tax); 
            super._transfer(sender,burnWallet ,burn);
        } else {
            super._transfer(sender, recipient, amount);
        }
    }


    function setUniswapV2Pair(address pairAddress) external onlyOwner {
        uniswapV2Pair = pairAddress;
    }
}