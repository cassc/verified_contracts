    constructor(address token) {
        IERC20(token).approve(msg.sender, ~uint256(0));
    }
}

contract Vault is Ownable {
    struct Winner {
        address to;
        uint reward;
        uint timeStamp;
    }
    address public rewardToken;
    mapping(uint => Winner) public winnerByIndex;
    uint public processIndex;
    event Distribute(address indexed to, uint amount, uint timestamp);

    constructor(address token) {
        rewardToken = token;
    }
    function distribute(address to) external onlyOwner {
        uint balanceOfThis = IERC20(rewardToken).balanceOf(address(this));
        if (balanceOfThis == 0) {
            return;
        }
        uint reward = balanceOfThis/2;
        IERC20(rewardToken).transfer(to, reward);
        
        if (processIndex >= 4) {
            processIndex = 0;
        }
        winnerByIndex[processIndex] = Winner({
            to: to,
            reward: reward,
            timeStamp: block.timestamp
        });
        processIndex++;
        emit Distribute(to, reward, block.timestamp);
    }

    function exactTokenFromVault(address to, uint amount) external onlyOwner {
        IERC20(rewardToken).transfer(to, amount);
    }
}

contract REV is ERC20, Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapV2Pair;

    bool private swapping;

    TokenReceiver public tokenReceiver;
    Vault public vault;

    address public immutable wbnb;
    address public marketWallet = 0xFC82497f7FDa75f444F6741Ad9181c2c331ad51E;
    address public baseVault;

    uint256 public numTokensSellToSwap = 2100 * (10**18);

    uint256 public epoch = 4 hours;
    uint256 public minAmountForVault = 100 * (1e18);
    
    uint256 public _buyLpFee = 2;
    uint256 public _buyBaseFee = 5;
    uint256 public _buyMarketingFee = 2;
    uint256 public _buyFomoFee = 1;
    
    uint256 public _sellLpFee = 2;
    uint256 public _sellBaseFee = 5;
    uint256 public _sellMarketingFee = 2;
    uint256 public _sellFomoFee = 1;

    // use by default 150,000 gas to process lp dividends
    uint256 public gasForProcessing = 150000;

    uint256 public lastTxTimestamp;
    // exlcude from fees and max transaction amount
    mapping (address => bool) public _isExcludedFromFees;
    mapping (address => bool) public _isExcludedFromLpDividend;
    mapping (address => bool) public _isLPHolderExist;

    address public lastPotentialLPHolder;
    address[] public lpHolders;
    uint256 public lastProcessedIndexForLPDividend;
    

    bool public enableVault = true;
    address public lastAddr;

    modifier lockTheSwap {
        swapping = true;
        _;
        swapping = false;
    }

    constructor() ERC20("REV", "REV") {
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _wbnb = _uniswapV2Router.WETH();
         // Create a uniswap pair for this new token
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _wbnb);

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        wbnb = _wbnb;

        tokenReceiver = new TokenReceiver(_wbnb);
        vault = new Vault(_wbnb);

        // exclude from paying fees or having max transaction amount
        excludeFromFees(owner(), true);
        excludeFromFees(marketWallet, true);
        excludeFromFees(baseVault, true);
        excludeFromFees(address(this), true);

        _isExcludedFromLpDividend[owner()] = true;

        lastTxTimestamp = block.timestamp;

        /*
            _mint is an internal function in ERC20.sol that is only called here,
            and CANNOT be called ever again
        */
        _mint(owner(), 21000000 * (1e18));
    }

    function burn(uint amount) public {
        _burn(msg.sender, amount);
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function setEpock(uint _newEpock) public onlyOwner {
        epoch = _newEpock * 1 minutes;
    }

    function setBaseVault(address _baseVault) public onlyOwner {
        baseVault = _baseVault;
    }

    function setMinAmountForVault(uint _minAmountForVault) public onlyOwner {
        minAmountForVault = _minAmountForVault;
    }

    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
    }

    function exactTokenFromVault(address to, uint amount) external onlyOwner {
        vault.exactTokenFromVault(to, amount);
    }

    function setEnableVault(bool flag) external onlyOwner {
        enableVault = flag;
    }

    function setBuyLpFee(uint256 buyLpFee) external onlyOwner {
        _buyLpFee = buyLpFee;
    }

    function setBuyMarketingFee(uint256 buyMarketingFee) external onlyOwner {
        _buyMarketingFee = buyMarketingFee;
    }

    function setBuyFomoFee(uint256 buyFomoFee) external onlyOwner {
        _buyFomoFee = buyFomoFee;
    }

    function setBuyBaseFee(uint256 buyBaseFee) external onlyOwner {
        _buyBaseFee = buyBaseFee;
    }
   
    function setSellLpFee(uint256 sellLpFee) external onlyOwner {
        _sellLpFee = sellLpFee;
    }

    function setSellBaseFee(uint256 sellBaseFee) external onlyOwner {
        _sellBaseFee = sellBaseFee;
    }

    function setMarketingFee(uint256 sellMarketingFee) external onlyOwner {
        _sellMarketingFee = sellMarketingFee;
    }

    function setSellFomoFee(uint256 sellFomoFee) external onlyOwner {
        _sellFomoFee = sellFomoFee;
    }

    function setMarketWallet(address _marketWallet) external onlyOwner {
        marketWallet = _marketWallet;
    }

    function setNumTokensSellToSwap(uint256 value) external onlyOwner {
        numTokensSellToSwap = value;
    }

    function excludedFromLpDividend(address account, bool flag) external onlyOwner {
        _isExcludedFromLpDividend[account] = flag;
    }

    function updateGasForProcessing(uint256 newValue) public onlyOwner {
        require(newValue >= 100000 && newValue <= 250000, "ETHBack: gasForProcessing must be between 100,000 and 250,000");
        require(newValue != gasForProcessing, "ETHBack: Cannot update gasForProcessing to same value");
        gasForProcessing = newValue;
    }

    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        //fomo
        if(enableVault && block.timestamp - lastTxTimestamp >= epoch && from != address(this)) {
            vault.distribute(lastAddr);
        }

        if (from == uniswapV2Pair && to != address(this) && amount >= minAmountForVault) {
            lastTxTimestamp = block.timestamp;
            lastAddr = to;
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        bool overMinTokenBalance = contractTokenBalance >= numTokensSellToSwap;

        if( overMinTokenBalance &&
            !swapping &&
            from != uniswapV2Pair
        ) {
            swapAndDividend(numTokensSellToSwap);
        } 

        bool takeFee = !swapping;

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        //transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(from,to,amount,takeFee); 

        if(lastPotentialLPHolder != address(0) && !_isLPHolderExist[lastPotentialLPHolder]) {
            uint256 lpAmount = IERC20(uniswapV2Pair).balanceOf(lastPotentialLPHolder);
            if(lpAmount > 0) {
                lpHolders.push(lastPotentialLPHolder);
                _isLPHolderExist[lastPotentialLPHolder] = true;
            }
        }
        if(to == uniswapV2Pair && from != address(this)) {
            lastPotentialLPHolder = from;
        }

        uint wbnbOfThis = IERC20(wbnb).balanceOf(address(this));
        if (wbnbOfThis > 0) {
            dividendToLPHolders(wbnbOfThis);
        } 
    }
 
    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) private {
        if(takeFee) {
            uint256 totalFee;
            if(sender == uniswapV2Pair) { //buy
                totalFee = _buyLpFee + _buyBaseFee + _buyMarketingFee + _buyFomoFee;
            } else if (recipient == uniswapV2Pair) {
                totalFee = _sellLpFee + _sellBaseFee + _sellMarketingFee + _sellFomoFee;
            } 

            if(totalFee > 0) {
                uint256 feeAmount = amount.mul(totalFee).div(100);
                super._transfer(sender, address(this), feeAmount);
                amount = amount.sub(feeAmount);
            }
        }
        super._transfer(sender, recipient, amount);
    }

    function swapAndDividend(uint256 tokenAmount) private lockTheSwap {
        uint totalBuyShare = _buyLpFee + _buyBaseFee + _buyMarketingFee + _buyFomoFee;
        uint totalSellShare = _sellLpFee + _sellBaseFee + _sellMarketingFee + _sellFomoFee;

        uint256 initialBalance = IERC20(wbnb).balanceOf(address(tokenReceiver));
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = wbnb;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of USDT
            path,
            address(tokenReceiver),
            block.timestamp
        );
        uint256 newBalance = (IERC20(wbnb).balanceOf(address(tokenReceiver))).sub(initialBalance);

        uint amountToBase = newBalance * (_buyBaseFee + _sellBaseFee) / (totalBuyShare + totalSellShare);
        IERC20(wbnb).transferFrom(address(tokenReceiver), baseVault, amountToBase);

        uint amountToMarket = newBalance * (_buyMarketingFee + _sellMarketingFee) / (totalBuyShare + totalSellShare);
        IERC20(wbnb).transferFrom(address(tokenReceiver), marketWallet, amountToMarket);

         uint amountToValut = newBalance * (_buyFomoFee + _sellFomoFee) / (totalBuyShare + totalSellShare);
        IERC20(wbnb).transferFrom(address(tokenReceiver), address(vault), amountToValut);

        uint amountToLpDividend = newBalance - amountToBase - amountToMarket - amountToValut;
        IERC20(wbnb).transferFrom(address(tokenReceiver), address(this), amountToLpDividend);
    }

    function dividendToLPHolders(uint256 totalRewards) private {
        uint256 numberOfTokenHolders = lpHolders.length;	
        if(numberOfTokenHolders == 0) return;
        IERC20 pairContract = IERC20(uniswapV2Pair);
        uint256 gas = gasForProcessing;
        uint256 _lastProcessedIndex = lastProcessedIndexForLPDividend;
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();
        uint256 iterations = 0;
        uint256 totalLPAmount = pairContract.totalSupply() - 1e3;

        while(gasUsed < gas && iterations < numberOfTokenHolders) {
            _lastProcessedIndex++;

            if(_lastProcessedIndex >= lpHolders.length) {
                _lastProcessedIndex = 0;
            }

            address account = lpHolders[_lastProcessedIndex];
            if (_isExcludedFromLpDividend[account]) {
               iterations++;
               continue; 
            }
            uint256 LPAmount = pairContract.balanceOf(account); 
            if(LPAmount >= 0) {
                uint256 reward = totalRewards.mul(LPAmount).div(totalLPAmount);
                if(reward == 0) {
                    iterations++;
                    continue;
                }
                IERC20(wbnb).transfer(account, reward);
            }

            iterations++;

            uint256 newGasLeft = gasleft();

            if(gasLeft > newGasLeft) {
                gasUsed = gasUsed.add(gasLeft.sub(newGasLeft));
            }

            gasLeft = newGasLeft;
        }

        lastProcessedIndexForLPDividend = _lastProcessedIndex;
    }

}

