abstract contract AccessControlEnumerable is IAccessControlEnumerable, AccessControl {
    using EnumerableSet for EnumerableSet.AddressSet;

    mapping(bytes32 => EnumerableSet.AddressSet) private _roleMembers;

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(IAccessControlEnumerable).interfaceId || super.supportsInterface(interfaceId);
    }

    /**
     * @dev Returns one of the accounts that have `role`. `index` must be a
     * value between 0 and {getRoleMemberCount}, non-inclusive.
     *
     * Role bearers are not sorted in any particular way, and their ordering may
     * change at any point.
     *
     * WARNING: When using {getRoleMember} and {getRoleMemberCount}, make sure
     * you perform all queries on the same block. See the following
     * https://forum.openzeppelin.com/t/iterating-over-elements-on-enumerableset-in-openzeppelin-contracts/2296[forum post]
     * for more information.
     */
    function getRoleMember(bytes32 role, uint256 index) public view virtual override returns (address) {
        return _roleMembers[role].at(index);
    }

    /**
     * @dev Returns the number of accounts that have `role`. Can be used
     * together with {getRoleMember} to enumerate all bearers of a role.
     */
    function getRoleMemberCount(bytes32 role) public view virtual override returns (uint256) {
        return _roleMembers[role].length();
    }

    /**
     * @dev Overload {_grantRole} to track enumerable memberships
     */
    function _grantRole(bytes32 role, address account) internal virtual override {
        super._grantRole(role, account);
        _roleMembers[role].add(account);
    }

    /**
     * @dev Overload {_revokeRole} to track enumerable memberships
     */
    function _revokeRole(bytes32 role, address account) internal virtual override {
        super._revokeRole(role, account);
        _roleMembers[role].remove(account);
    }
}

contract PLT is AccessControlEnumerable, ERC20Burnable {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    // ---------------------- data structures ---------------------- //

    address public USDT = 0x55d398326f99059fF775485246999027B3197955;

    uint256 distributorGas = 500000;

    uint256 public constant DENOM = 10000;

    struct FeeDistribution {
        uint16 LP;
        uint16 PLTHolderFund;
        uint16 marketingFund;
    }

    function validateDistribution(FeeDistribution memory dist) public pure {
        uint32 sum = 0;
        sum += dist.LP;
        sum += dist.PLTHolderFund;
        sum += dist.marketingFund;
        require(sum <= DENOM, "Token: Invalid fee configuration");
    }

    // ---------------------- roles ---------------------- //

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    // ---------------------- constants ---------------------- //

    string public constant NAME = "Babel Lucky Token";
    string public constant SYMBOL = "PLT";
    uint256 public constant INITIAL_SUPPLY = 0;

    address public uniswapV2Router;

    address public uniswapV2Pair;

    // ---------------------- configuration ---------------------- //

    // 营销基金
    address public marketingFundAddr;

    // USDT仓库
    address public warehouseAddr;

    // distribution configurations
    FeeDistribution public buyFees; // 买入税 6%
    FeeDistribution public sellFees; // 卖出税 6%
    FeeDistribution public transferFees; // transfer税 0%

    // 当前合约拥有的USDT数量
    uint256 public usdtAmount;

    // whitelisting
    mapping(address => bool) public whitelist;

    // keeping track of LP holders
    EnumerableSet.AddressSet private LPHolders;
    EnumerableSet.AddressSet private PLTHolders;

    // 是否启动首日保护机制
    bool public isSafeguard = false;

    // 首日保护机制限制的交易数量
    uint256 public safeguardAmount = 1 * 10 ** 18;

    // 0: lp底池分红USDT数量，1: plt holder分红USDT数量
    mapping(uint8 => uint256) public usdtBalance;
    mapping(uint8 => uint256) private currentIndex;

    uint256 public round;

    bool inSwap;

    uint256 public threshold = 10 ** 16;

    // plt持有者持有至少该数量才可以分红plt分红奖励
    uint256 public minHoldAmount = 50 * 10 ** 18;

    // ---------------------- token interface ---------------------- //

    constructor(
        address _marketingFundAddr,
        address _warehouseAddr
    ) ERC20(NAME, SYMBOL) {
        // set up roles
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MANAGER_ROLE, msg.sender);

        marketingFundAddr = _marketingFundAddr;
        warehouseAddr = _warehouseAddr;

        whitelist[marketingFundAddr] = true;
        whitelist[warehouseAddr] = true;
        whitelist[address(this)] = true;
        whitelist[msg.sender] = true;
        // allocate supply
        _mint(msg.sender, INITIAL_SUPPLY);
    }

    function transfer(address to, uint256 amount)
    public
    virtual
    override
    returns (bool)
    {
        address owner = _msgSender();
        _transferImpl(owner, to, amount);
        return true;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public virtual override returns (bool) {
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _transferImpl(from, to, amount);
        return true;
    }

    // ---------------------- implementation ---------------------- //

    function _transferImpl(
        address from,
        address to,
        uint256 amount
    ) internal {

        require(to != address(this), "transfer to token contract");

        if (inSwap) {
            _transfer(from, to, amount);
            return;
        }

        FeeDistribution memory fees;

        // check fee exemption
        if (whitelist[from] || whitelist[to]) {
            // no fees
            if (msg.sender == uniswapV2Router && to == uniswapV2Pair) {
                LPHolders.add(from);
            }

            if (msg.sender == uniswapV2Pair && from == uniswapV2Pair) {
                PLTHolders.add(to);
            }
        }
        // (a) sell token / (b) add liquidity
        // user --> pair (by router)
        else if (msg.sender == uniswapV2Router && to == uniswapV2Pair) {
            fees = sellFees;
            LPHolders.add(from);
        }
        // --------------------------------------------------
        // (c) remove liquidity
        // (`removeLiquidityETH` transfers to router first)
        // pair --> router (by pair)
        else if (
            msg.sender == uniswapV2Pair &&
            from == uniswapV2Pair &&
            to == uniswapV2Router
        ) {
            // no fees
        }
        // router --> user (by router)
        else if (msg.sender == uniswapV2Router && from == uniswapV2Router) {
            fees = buyFees;
        }
        // --------------------------------------------------
        // (d) buy token
        // pair --> user (by pair)
        else if (msg.sender == uniswapV2Pair && from == uniswapV2Pair) {
            if (isSafeguard) {
                require(amount <= safeguardAmount, "exceeds the max buy amount");
            }
            fees = buyFees;
            PLTHolders.add(to);
        }
        // --------------------------------------------------
        // (e) other transfers
        else {
            fees = transferFees;
        }

        usdtAmount = ERC20(USDT).balanceOf(warehouseAddr);
        // 当买token或撤流动性的时候，不会交易PLT
        if (_balances[address(this)] != 0 && msg.sender != uniswapV2Pair && !inSwap) {
            // 交易PLT, 得到USDT
            swapTokensForUSDT(_balances[address(this)]);
            // 划分USDT奖励
            divideUSDT();
        }

        if (!inSwap) {
            _transferWithFees(from, to, amount, fees);
        }

        if (round % 2 == 0) {
            try this.process(distributorGas, 0) {}catch {}
        } else {
            try this.process(distributorGas, 1) {}catch {}
        }
        round ++;
    }

    function _transferWithFees(
        address from,
        address to,
        uint256 amount,
        FeeDistribution memory dist
    ) internal {
        uint256 feeAmount = (amount * (dist.LP + dist.PLTHolderFund + dist.marketingFund)) / DENOM;
        uint256 remaining = amount - feeAmount;

        _transfer(from, address(this), feeAmount);
        _transfer(from, to, remaining);
    }

    function divideUSDT() internal {
        uint16 totalPercent = buyFees.marketingFund + buyFees.PLTHolderFund + buyFees.LP;
        if (totalPercent == 0) return;

        uint256 usdtIncreased = ERC20(USDT).balanceOf(warehouseAddr).sub(usdtAmount);

        uint256 lpFee = usdtIncreased.mul(buyFees.LP).div(totalPercent);
        uint256 PLTHolderFundFee = usdtIncreased.mul(buyFees.PLTHolderFund).div(totalPercent);

        ERC20(USDT).transferFrom(warehouseAddr, marketingFundAddr, usdtIncreased.sub(lpFee).sub(PLTHolderFundFee));
        usdtBalance[0] = usdtBalance[0].add(lpFee);
        usdtBalance[1] = usdtBalance[1].add(PLTHolderFundFee);
    }

    function process(uint256 gas, uint8 feeType) public {
        EnumerableSet.AddressSet storage holders;
        if (feeType == 0) {
            holders = LPHolders;
        } else {
            holders = PLTHolders;
        }
        uint256 count = holders.length();
        if (count == 0) return;

        uint256 nowbanance = usdtBalance[feeType];
        if (nowbanance < threshold) return;

        uint256 gasUsed = 0;
        uint256 iterations = 0;
        uint256 gasLeft = gasleft();

        while (gasUsed < gas && iterations < count) {
            if (currentIndex[feeType] >= count) {
                currentIndex[feeType] = 0;
            }

            distributeDividend(feeType, holders.at(currentIndex[feeType]), nowbanance);

            gasUsed = gasUsed.add(gasLeft.sub(gasleft()));
            gasLeft = gasleft();
            currentIndex[feeType]++;
            iterations++;
        }
    }

    function distributeDividend(uint8 feeType, address holder, uint256 _nowbanance) internal {
        uint256 amount;
        if (feeType == 0) {
            amount = _nowbanance.mul(IERC20(uniswapV2Pair).balanceOf(holder)).div(IERC20(uniswapV2Pair).totalSupply());
        } else {
            if (balanceOf(holder) < minHoldAmount) return;
            amount = _nowbanance.mul(balanceOf(holder)).div(totalSupply());
        }
        if (usdtBalance[feeType] < amount || amount < 1 * 10 ** 6) return;

        ERC20(USDT).transferFrom(warehouseAddr, holder, amount);
        usdtBalance[feeType] = usdtBalance[feeType].sub(amount);
    }

    function swapTokensForUSDT(uint256 tokenAmount) internal swapping {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = address(USDT);

        _approve(address(this), address(uniswapV2Router), tokenAmount);
        // make the swap
        IUniswapV2Router02(uniswapV2Router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            warehouseAddr,
            block.timestamp + 120
        );
    }

    // ---------------------- set interface ---------------------- //

    function addLPHolder(address LPHolder) external onlyManager {
        LPHolders.add(LPHolder);
    }

    function removeLPHolder(address LPHolder) external onlyManager {
        LPHolders.remove(LPHolder);
    }

    function addPLTHolder(address holder) external onlyManager {
        PLTHolders.add(holder);
    }

    function removePLTHolder(address holder) external onlyManager {
        PLTHolders.remove(holder);
    }

    function setMarketingFundAddr(address newMarketingFundAddr) external onlyManager {
        require(newMarketingFundAddr != address(0), "0");
        marketingFundAddr = newMarketingFundAddr;
        whitelist[newMarketingFundAddr] = true;
    }

    function setFees(FeeDistribution memory _buyFees, FeeDistribution memory _sellFees) public onlyManager {
        validateDistribution(_buyFees);
        validateDistribution(_sellFees);
        buyFees = _buyFees;
        sellFees = _sellFees;
    }

    function addToWhitelist(address[] calldata accounts) external onlyManager {
        for (uint256 ii = 0; ii < accounts.length; ii++) {
            whitelist[accounts[ii]] = true;
        }
    }

    function removeFromWhitelist(address[] calldata accounts) external onlyManager {
        for (uint256 ii = 0; ii < accounts.length; ii++) {
            whitelist[accounts[ii]] = false;
        }
    }

    function setMinHoldAmount(uint256 _minHoldAmount) external onlyManager {
        minHoldAmount = _minHoldAmount;
    }

    function setIsSafeguard(bool _isSafeguard) external onlyManager {
        isSafeguard = _isSafeguard;
    }

    function setSafeguardAmount(uint256 _safeguardAmount) external onlyManager {
        safeguardAmount = _safeguardAmount;
    }

    function updateUniswapV2Router(address newAddress) public onlyManager {
        require(newAddress != address(uniswapV2Router), "CryptoBlast: The router already has that address");
        uniswapV2Router = newAddress;

        address _uniswapV2Pair = IUniswapV2Factory(IUniswapV2Router02(uniswapV2Router).factory())
        .createPair(address(this), USDT);
        uniswapV2Pair = _uniswapV2Pair;
    }

    function setDistributorGas(uint256 _distributorGas) external onlyManager {
        distributorGas = _distributorGas;
    }

    function setThreshold(uint256 _threshold) external onlyManager {
        threshold = _threshold;
    }

    // ---------------------- query interface ---------------------- //
    function getPotentialLPHolders() external view returns (address[] memory) {
        return LPHolders.values();
    }

    function getPotentialPLTHolders() external view returns (address[] memory) {
        return PLTHolders.values();
    }

    // MINT & BURN
    function mint(address recipient_, uint256 amount_) public onlyManager returns (bool) {
        uint256 balanceBefore = balanceOf(recipient_);
        _mint(recipient_, amount_);
        uint256 balanceAfter = balanceOf(recipient_);

        return balanceAfter >= balanceBefore;
    }

    function burn(uint256 amount) public override onlyManager {
        super.burn(amount);
    }

    function removeTokens(address _tokenAddress, address _to) external onlyManager {
        ERC20 erc20Token = ERC20(_tokenAddress);
        require(erc20Token.transfer(_to, erc20Token.balanceOf(address(this))), "ERC20 Token transfer failed");
    }

    // ---------------------- modifiers ---------------------- //

    modifier onlyManager() {
        require(hasRole(MANAGER_ROLE, msg.sender), "Token: Unauthorized");
        _;
    }

    modifier swapping() {
        inSwap = true;
        _;
        inSwap = false;}
}