    function _findSlot() private view returns (uint256 slot) {
        AssetStakerStorage storage _storage = assetStakerStorage();

        for (uint256 i = 1; i <= _storage.slotSize; i++) {
            if (!_storage.stakedSlots[msg.sender].contains(i)) {
                slot = i;
                break;
            }
        }
    }

