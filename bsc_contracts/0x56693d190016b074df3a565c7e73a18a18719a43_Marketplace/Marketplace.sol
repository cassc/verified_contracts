abstract contract ChannelDataFacet is AccessControlEnumerableFacet {
    struct ChannelData {
        address orderSigner;
        address cashier;
        mapping(uint256 => bool) usedInvoices;
    }

    struct ChannelDataStorage {
        mapping(uint256 => ChannelData) marketplaceChannels;
    }

    event MarketplaceChannelChanged(uint256 indexed channelId, address oldOrderSigner, address oldCashier, address indexed newOrderSigner, address indexed newCashier);

    function setPaymentChannel(uint256 channelId, address newOrderSigner, address newCashier) external onlyRole(Constant.MARKETPLACE_MANAGER_ROLE) {
        require((newOrderSigner != address(0) && newCashier != address(0))
            || (newOrderSigner == address(0) && newCashier == address(0)), "Dramarket: newOrderSigner is address(0) while newCashier is not, and vice versa");
        ChannelData storage marketplaceChannel = channelDataStorage().marketplaceChannels[channelId];
        address oldOrderSigner = marketplaceChannel.orderSigner;
        address oldCashier = marketplaceChannel.cashier;
        marketplaceChannel.orderSigner = newOrderSigner;
        marketplaceChannel.cashier = newCashier;
        emit MarketplaceChannelChanged(channelId, oldOrderSigner, oldCashier, newOrderSigner, newCashier);
    }

    function isSettled(uint256 channelId, uint256 invoiceNo) external view returns (bool) {
        return channelDataStorage().marketplaceChannels[channelId].usedInvoices[invoiceNo];
    }

    function channelDataStorage() internal pure returns (ChannelDataStorage storage cd) {
        bytes32 position = StorageSlots.CHANNEL_DATA_STORAGE_POSITION;
        assembly {
            cd.slot := position
        }
    }
}


