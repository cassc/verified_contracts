     * @notice  by calling this function, the admin can set metadata baseURI.
     * @dev Returns the base URI set via _setBaseURI. This will be automatically added as a prefix in tokenURI to each token’s URI, or to the token ID if no specific URI is set for that token ID..
     * @param _baseUri base URI.
     */
    function setBaseURI(string calldata _baseUri) external onlyOwner {
        baseURI = _baseUri;
    }

    /**
     * @notice  by calling this function, can get metadata baseURI.
     */
    function getBaseURI() external view returns (string memory) {
        return baseURI;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    /**
     * @notice  By calling this function, Admin can stop all activities of this smart contract.
\     */
    function pause() public onlyOwner {
        _pause();
    }

    /**
     * @notice  By calling this function, admin can issue permission to resume all activities.
\     */
    function unpause() public onlyOwner {
        _unpause();
    }

    function safeMint(
        uint256 tokenId,
        FarmType _FarmType,
        address owner
    ) internal {
        _safeMint(owner, tokenId);
        _setTokenURI(tokenId, Strings.toString(tokenId));
        FarmTypes[tokenId] = _FarmType;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721, ERC721URIStorage)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    /**
     * @notice  This function is used to mint turbo farms.
     * @dev Farms with numbers 1 to 6 are sent to the owner's address. Farms 7 to 60 are sent to the treasury address.
     * @param _count The number of turbo farms you want to mint.
     */
    function turboFarmMint(uint256 _count) external onlyOwner {
        require(
            numberOfTurbo.total - numberOfTurbo.minted >= _count,
            "SixPackMiningFarm::TurboFarmMint: The input value entered is not valid"
        );
        totalSupply += _count;
        for (uint256 i = 0; i < _count; i++) {
            numberOfTurbo.minted = numberOfTurbo.minted + 1;
            uint256 tokenId = numberOfTurbo.minted;

            address recevierAddress = address(0);
            if (tokenId <= 6) {
                recevierAddress = owner();
            } else {
                recevierAddress = Treasury;
            }
            safeMint(tokenId, FarmType.TURBO, recevierAddress);
        }
    }

    /**
     * @notice  This function is used to mint pro farms.
     * @dev Farms 61 to 240 are pro type and are sent to the treasury address.
     * @param _count The number of pro forms you want to mint.
     */
    function proFarmMint(uint256 _count) external onlyOwner {
        require(
            numberOfPro.total - numberOfPro.minted >= _count,
            "SixPackMiningFarm::ProFarmMint: The input value entered is not valid"
        );
        totalSupply += _count;
        for (uint256 i = 0; i < _count; i++) {
            numberOfPro.minted = numberOfPro.minted + 1;
            uint256 tokenId = numberOfTurbo.total + numberOfPro.minted;
            address recevierAddress = Treasury;

            safeMint(tokenId, FarmType.PRO, recevierAddress);
        }
    }

    /**
     * @notice  This function is used to mint common farms.
     * @dev Farms 241 to 600 are common type and are sent to the treasury address.
     * @param _count The number of common forms you want to mint.
     */
    function commonFarmMint(uint256 _count) external onlyOwner {
        require(
            numberOfCommon.total - numberOfCommon.minted >= _count,
            "SixPackMiningFarm::CommonFarmMint: The input value entered is not valid"
        );
        totalSupply += _count;

        for (uint256 i = 0; i < _count; i++) {
            numberOfCommon.minted = numberOfCommon.minted + 1;
            uint256 tokenId = numberOfTurbo.total +
                numberOfPro.total +
                numberOfCommon.minted;
            address recevierAddress = Treasury;

            safeMint(tokenId, FarmType.COMMON, recevierAddress);
        }
    }

    function getFarmType(uint256 tokenId) public view returns (FarmType) {
        return (FarmTypes[tokenId]);
    }

    function setApprovalForAll(address operator, bool approved)
        public
        override
        onlyAllowedOperatorApproval(operator)
    {
        super.setApprovalForAll(operator, approved);
    }

    function approve(address operator, uint256 tokenId)
        public
        override
        onlyAllowedOperatorApproval(operator)
    {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }
}

