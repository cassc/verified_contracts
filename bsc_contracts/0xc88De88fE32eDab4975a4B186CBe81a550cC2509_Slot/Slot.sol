contract DividendPayingToken is ERC20, RoleControl {
    uint256 cap;

    using SafeMath for uint256;
    using SafeMathUint for uint256;
    using SafeMathInt for int256;

    uint256 public dividendPerToken;

    mapping(address => uint256) dividendBalanceOf;
    mapping(address => uint256) dividendCreditedTo;

    event EtherDistributed(address indexed from, uint256 amount);
    event DividendPerTokenUpdated(uint256 amount);

    // With `magnitude`, we can properly distribute dividends even if the amount of received ether is small.
    // For more discussion about choosing the value of `magnitude`,
    //  see https://github.com/ethereum/EIPs/issues/1726#issuecomment-472352728
    // uint256 internal constant magnitude = 2**128;
    // uint256 internal constant magnitude = 2**128;
    uint256 internal constant magnitude = 1000000000000000000;

    mapping(address => int256) internal magnifiedDividendCorrections;

    constructor(
        uint256 limit
    ) 
        ERC20("Dividend Paying Token", "DPT") 
        RoleControl(msg.sender)
    {
        cap = limit * 10 ** decimals();
        // uint256 firstMint = 1000 * 10 ** decimals();
        // mint(msg.sender, firstMint);
    }

    function mint(address to, uint256 amount) public onlyAdmin {
        uint256 total = totalSupply();
        require(total + amount <= cap, "Token limit reached!");
        _mint(to, amount);
    }

    // function update(address account) internal {
    //     uint256 owed = dividendPerToken - dividendCreditedTo[account];
    //     dividendBalanceOf[account] += balanceOf(account) * owed;
    //     dividendCreditedTo[account] = dividendPerToken;
    // }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        super._transfer(from, to, amount);

        int256 _magCorrection = dividendPerToken
            .mul(amount)
            .toInt256Safe();
        magnifiedDividendCorrections[from] = magnifiedDividendCorrections[from]
            .add(_magCorrection);
        magnifiedDividendCorrections[to] = magnifiedDividendCorrections[to].sub(
            _magCorrection
        );

        // update(msg.sender);  //  added to ERC20 contract
        // update(to);          //  added to ERC20 contract
    }

    function updateDividendPerToken() public payable {
        require(totalSupply() > 0, "Total Supply is 0");
        require(msg.value > 0, "Eth amout is 0");

        dividendPerToken = dividendPerToken.add(
                (msg.value).mul(magnitude) / totalSupply()
        );

        emit EtherDistributed(msg.sender, msg.value);
        emit DividendPerTokenUpdated(dividendPerToken);
    }

    function getDPT() 
        public 
        view 
        returns (uint256)
    {
        return dividendPerToken;
    }

    /// @dev Distributes dividends whenever ether is paid to this contract.
    receive() external payable {
        updateDividendPerToken();
    }

    function dividendOf(address account)
        public
        view
        returns (uint256)
    {
        return
            dividendPerToken
                .mul(balanceOf(account))
                .toInt256Safe()
                .add(magnifiedDividendCorrections[account])
                .toUint256Safe() / magnitude;
    }

    function burn(address to, uint256 amount) public onlyAdmin {
        uint256 total = totalSupply();
        require(total - amount >= 0, "Can't burn tokens anymore!");
        _burn(to, amount);
    }
}








