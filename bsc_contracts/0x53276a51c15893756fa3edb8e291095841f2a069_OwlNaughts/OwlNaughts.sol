contract OwlNaughts is ERC721, IERC721Enumerable, Ownable {

    // Mapping from owner to list of owned token IDs
    mapping(address => mapping(uint256 => uint256)) private _ownedTokens;

    // Mapping from token ID to index of the owner tokens list
    mapping(uint256 => uint256) private _ownedTokensIndex;

    // Array with all token ids, used for enumeration
    uint256[] private _allTokens;

    // Mapping from token id to position in the allTokens array
    mapping(uint256 => uint256) private _allTokensIndex;

    // total number of NFTs Minted
    uint256 public currentSupplyIndex;

    // max supply cap
    uint256 public constant MAX_SUPPLY = 4732;

    // cost for minting NFT
    uint256 public cost = 1 * 10**16;

    // base URI
    string private baseURI = "temp";
    string private ending = ".json";

    // Enable Trading
    bool public mintEnabled = true;

    // Dev address
    address private constant dev = 0x06fe7eB32a532Bce5a8e63f21DD597927E923B0e;

    constructor()ERC721("Owl Naughts", "Hoot"){}


    function enableMinting() external onlyOwner {
        mintEnabled = true;
    }

    function disableMinting() external onlyOwner {
        mintEnabled = true;
    }

    function withdraw() external onlyOwner {
        (bool s,) = payable(msg.sender).call{value: address(this).balance}("");
        require(s);
    }

    function withdrawToken(address token_) external onlyOwner {
        require(token_ != address(0), 'Zero Address');
        IERC20(token_).transfer(msg.sender, IERC20(token_).balanceOf(address(this)));
    }

    function setCost(uint256 newCost) external onlyOwner {
        cost = newCost;
    }

    function setBaseURI(string calldata newURI) external onlyOwner {
        baseURI = newURI;
    }

    function setURIExtention(string calldata newExtention) external onlyOwner {
        ending = newExtention;
    }


    /** 
     * Mints `numberOfMints` NFTs To Caller
     */
    function mint(uint256 numberOfMints) external payable {
        require(
            mintEnabled,
            'Trading Not Enabled'
        );
        require(
            numberOfMints > 0, 
            'Invalid Input'
        );
        require(
            cost * numberOfMints <= msg.value, 
            'Incorrect Value Sent'
        );

        for (uint i = 0; i < numberOfMints;) {
            _mint(msg.sender, currentSupplyIndex);
            unchecked { ++i; }
        }

        (bool s,) = payable(dev).call{value: address(this).balance / 20}("");
        require(s);

        (s,) = payable(owner()).call{value: address(this).balance}("");
        require(s);
    }

