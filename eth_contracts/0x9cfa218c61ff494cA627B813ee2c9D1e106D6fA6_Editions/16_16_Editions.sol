pragma solidity 0.8.18;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";

contract Editions is
    ERC1155,
    ERC1155Burnable,
    ERC1155Supply,
    AccessControl,
    Ownable
{
    struct Release {
        bool frozenMetadata;
        uint256 maxSupply;
        string uri;
    }

    error ReleaseNotFound();
    error MetadataIsFrozen();
    error NotEnoughSupply();

    event ReleaseCreated(uint256 __id);
    event PermanentURI(string __value, uint256 indexed __id);

    // The token name
    string public name;

    // The token symbol
    string public symbol;

    // Used to track ID of next release
    uint256 private _nextReleaseID = 1;

    // Mapping of releases
    mapping(uint256 => Release) private _releases;

    // Minter Role used in minting operations
    bytes32 constant MINTER_ROLE = bytes32("MINTER_ROLE");

    /**
     * @dev Sets name/symbol and grants initial roles to owner upon construction.
     */
    constructor(string memory __name, string memory __symbol) ERC1155("") {
        name = __name;
        symbol = __symbol;

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(MINTER_ROLE, _msgSender());
    }

    ////////////////////////////////////////////////////////////////////////////
    // MODIFIERS
    ////////////////////////////////////////////////////////////////////////////

    /**
     * @dev Checks if release exists.
     *
     * Requirements:
     *
     * - `__id` must be of existing release.
     */
    modifier onlyExistingRelease(uint256 __id) {
        if (!releaseExists(__id)) {
            revert ReleaseNotFound();
        }
        _;
    }

    /**
     * @dev Checks if Release exists.
     *
     * Requirements:
     *
     * - `__ids` must be of existing release(s).
     */
    modifier onlyExistingReleaseBatch(uint256[] calldata __ids) {
        for (uint256 i = 0; i < __ids.length; i++) {
            if (!releaseExists(__ids[i])) {
                revert ReleaseNotFound();
            }
        }
        _;
    }

    ////////////////////////////////////////////////////////////////////////////
    // INTERNALS
    ////////////////////////////////////////////////////////////////////////////

    function _beforeTokenTransfer(
        address __operator,
        address __from,
        address __to,
        uint256[] memory __ids,
        uint256[] memory __amounts,
        bytes memory __data
    ) internal virtual override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(
            __operator,
            __from,
            __to,
            __ids,
            __amounts,
            __data
        );

        if (__from == address(0) && __ids.length > 1) {
            for (uint256 i = 0; i < __ids.length; i++) {
                if (_releases[__ids[i]].maxSupply != 0) {
                    if (totalSupply(__ids[i]) > _releases[__ids[i]].maxSupply)
                        revert NotEnoughSupply();
                }
            }
        }
    }

    ////////////////////////////////////////////////////////////////////////////
    // ADMIN
    ////////////////////////////////////////////////////////////////////////////

    /**
     * @dev Used to create a new release.
     *
     * Emits a {ReleaseCreated} event.
     *
     */
    function createRelease(
        uint256 __maxSupply,
        string calldata __uri
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 releaseId = _nextReleaseID++;

        _releases[releaseId] = Release({
            frozenMetadata: false,
            maxSupply: __maxSupply,
            uri: __uri
        });

        emit ReleaseCreated(releaseId);
    }

    /**
     * @dev Used to edit the token URI of a release.
     *
     * Emits a {ReleaseURIUpdated} event.
     *
     */
    function editURI(
        uint256 __id,
        string calldata __uri
    ) external onlyRole(DEFAULT_ADMIN_ROLE) onlyExistingRelease(__id) {
        if (_releases[__id].frozenMetadata) {
            revert MetadataIsFrozen();
        }

        _releases[__id].uri = __uri;

        emit URI(__uri, __id);
    }

    /**
     * @dev Used to freeze metadata.
     *
     * Emits a {ReleaseFrozen} event.
     *
     */
    function freezeMetadata(
        uint256 __id
    ) external onlyRole(DEFAULT_ADMIN_ROLE) onlyExistingRelease(__id) {
        _releases[__id].frozenMetadata = true;

        emit PermanentURI(_releases[__id].uri, __id);
    }

    ////////////////////////////////////////////////////////////////////////////
    // MINTER
    ////////////////////////////////////////////////////////////////////////////

    /**
     * @dev Used to mint token(s) of a release for a one account.
     */
    function mint(
        address __account,
        uint256 __id,
        uint256 __amount
    ) external onlyRole(MINTER_ROLE) onlyExistingRelease(__id) {
        if (_releases[__id].maxSupply != 0) {
            if (totalSupply(__id) + __amount > _releases[__id].maxSupply)
                revert NotEnoughSupply();
        }

        _mint(__account, __id, __amount, "");
    }

    /**
     * @dev Used to mint token(s) of a release for a one account.
     */
    function mintMany(
        address[] calldata __accounts,
        uint256 __id,
        uint256[] calldata __amounts
    ) external onlyRole(MINTER_ROLE) onlyExistingRelease(__id) {
        if (_releases[__id].maxSupply != 0) {
            uint256 totalAmount = 0;
            for (uint i = 0; i < __amounts.length; i++) {
                totalAmount += __amounts[i];
            }
            if (totalSupply(__id) + totalAmount > _releases[__id].maxSupply)
                revert NotEnoughSupply();
        }

        for (uint256 i = 0; i < __accounts.length; i++) {
            _mint(__accounts[i], __id, __amounts[i], "");
        }
    }

    /**
     * @dev Used to mint token(s) of Release(s) for a one account.
     */
    function mintBatch(
        address __account,
        uint256[] calldata __ids,
        uint256[] calldata __amounts
    ) external onlyRole(MINTER_ROLE) onlyExistingReleaseBatch(__ids) {
        _mintBatch(__account, __ids, __amounts, "");
    }

    ////////////////////////////////////////////////////////////////////////////
    // READS
    ////////////////////////////////////////////////////////////////////////////

    /**
     * @dev Returns whether or not a release exists.
     */
    function releaseExists(uint256 __id) public view returns (bool) {
        if (__id != 0 && __id < _nextReleaseID) {
            return true;
        }
        return false;
    }

    /**
     * @dev Returns a release.
     */
    function getRelease(
        uint256 __id
    ) external view onlyExistingRelease(__id) returns (Release memory) {
        return _releases[__id];
    }

    /**
     * @dev Returns the max supply of a release.
     */
    function maxSupply(
        uint256 __id
    ) external view onlyExistingRelease(__id) returns (uint256) {
        return _releases[__id].maxSupply;
    }

    /**
     * @dev Returns the number of total releases.
     */
    function totalReleases() external view returns (uint256) {
        return _nextReleaseID - 1;
    }

    /**
     * @dev Returns the token URI of a release.
     */
    function uri(
        uint256 __id
    )
        public
        view
        virtual
        override
        onlyExistingRelease(__id)
        returns (string memory)
    {
        return _releases[__id].uri;
    }

    /**
     * @dev See {ERC1155-supportsInterface} and {AccessControl-supportsInterface}.
     */
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC1155, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}