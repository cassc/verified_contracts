contract _ERC721MultiCurrency is _ERC721 {
    bytes32 public constant WITHDRAWER_ROLE = keccak256("WITHDRAWER_ROLE");
    uint256 public constant DEFAULT_CURRENCY = 0;
    Currency[] private _currencies;
    address _paymentSplitter;

    constructor(
        string memory name,
        string memory symbol,
        address feeAccount
    ) _ERC721(name, symbol, feeAccount) {
        _grantRole(WITHDRAWER_ROLE, msg.sender);
    }

    function getCurrency(uint256 id)
        public
        view
        returns (
            string memory,
            uint256,
            address,
            bool
        )
    {
        Currency memory currency = _currencies[id];
        return (currency.name, currency.price, currency.erc20, currency.burn);
    }

    function addCurrency(
        string memory name,
        uint256 price,
        address erc20,
        bool burn
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _currencies.push(Currency(name, price, erc20, burn));
    }

    function withdraw() external onlyRole(WITHDRAWER_ROLE) {
        Address.sendValue(payable(_paymentSplitter), address(this).balance);

        address[] memory currencies = new address[](_currencies.length);

        for (uint256 i = 0; i < _currencies.length; i++) {
            Currency memory currency = _currencies[i];

            if (currency.erc20 == address(0)) {
                continue;
            }

            _ERC20 erc20 = _ERC20(currency.erc20);
            erc20.transfer(_paymentSplitter, erc20.balanceOf(address(this)));
            currencies[i] = currency.erc20;
        }

        IPaymentSplitter(_paymentSplitter).split(currencies);
    }

    function getPaymentSplitter() external view returns (address) {
        return _paymentSplitter;
    }

    function setPaymentSplitter(address paymentSplitter)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _paymentSplitter = paymentSplitter;
    }

    function setCurrency(
        uint256 id,
        string calldata name,
        uint256 price,
        address erc20,
        bool burn
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _currencies[id] = Currency(name, price, erc20, burn);
    }

    function _buy(uint256 currencyId, uint256 count) internal {
        uint256 price = _currencies[currencyId].price * count;
        if (currencyId == DEFAULT_CURRENCY) {
            require(msg.value == price, "Incorrect value sent.");
        } else {
            Currency memory c = _currencies[currencyId];
            require(c.erc20 != address(0), "Incorrect currency address");

            _ERC20 erc20 = _ERC20(c.erc20);
            require(
                erc20.allowance(msg.sender, address(this)) >= price &&
                    erc20.balanceOf(msg.sender) >= price,
                "Not enough allowance or value"
            );
            erc20.transferFrom(msg.sender, address(this), price);

            if (c.burn) {
                erc20.burn(price);
            }
        }
    }

    struct Currency {
        string name;
        uint256 price;
        address erc20;
        bool burn;
    }
}

