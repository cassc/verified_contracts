    function _claim(uint256 timestamp, uint256 bonus) private whenNotPaused {
        AssetStakerStorage storage _storage = assetStakerStorage();
        _storage.warZone.mint(msg.sender, _calculateClaim(timestamp, bonus));
    }

    function _calculateClaim(uint256 timestamp, uint256 bonus)
        private
        view
        returns (uint256)
    {
        return (block.timestamp - timestamp) * bonus;
    }

    function _calculateBonus(uint256[] memory assetIds)
        private
        returns (uint256[] memory)
    {
        AssetStakerStorage storage _storage = assetStakerStorage();

        uint256[] memory dailyBaseAttributes = abi.decode(
            _storage.asset.delegateCall(
                address(this),
                0,
                abi.encode(_storage.dailyBaseAttributeIndex, assetIds)
            ),
            (uint256[])
        );

        uint256[] memory bonuses = new uint256[](assetIds.length);

        for (uint256 i = 0; i < assetIds.length; i++) {
            bonuses[i] = assetIds[i] <= _storage.maxMint
                ? (_storage.dailyBases[dailyBaseAttributes[i]] *
                    (DENOMINATOR +
                        (_storage.maxBonus -
                            _storage.bonusStep *
                            (assetIds[i] / _storage.mintStep)))) / DENOMINATOR
                : _storage.dailyBases[dailyBaseAttributes[i]];
        }

        return bonuses;
    }
