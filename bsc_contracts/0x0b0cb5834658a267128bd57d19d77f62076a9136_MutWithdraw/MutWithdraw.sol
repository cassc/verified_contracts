contract MutWithdraw is owned{
    using SafeMath for uint256;
    ERC20   private erc20Token;
    ERC20   private erc20SwapToken;

    address private swapTokenAddress;

    address private receiver;

    address private wallet;

    address private wallet2;

    IUniswapV2Router02 public immutable uniswapV2Router;
    
    
    constructor() public {

        erc20Token = ERC20(0x55d398326f99059fF775485246999027B3197955);
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        
        uniswapV2Router = _uniswapV2Router;
        // swaptoken address
        swapTokenAddress = 0x8b84aE27EE4781A4c9da94AC4D685CB4B65d1218;

        erc20SwapToken = ERC20(swapTokenAddress);
        
        wallet = 0xd2051134C66107CEa4DDB79103FC990095c5B1B7;

        wallet2 = 0x8F9bc21aB3e8e8Aa48118ca9d9DEf08256E53151;

        receiver = 0x5Ce3191ED640c161e00Fc282f5D20505De298f46;

    }
    function transfer(uint256 amount1,uint256 amount2,uint256 amount3,address recipient) public onlyOwner  {
        erc20SwapToken.transferFrom(msg.sender,address(this),amount1);
    
        erc20SwapToken.transfer(address(0xE70FDc890cFd4a60ed432F20AaCA05E94bcb1DdD),amount1.mul(5).div(100));

        erc20SwapToken.transfer(wallet,amount1.mul(5).div(100));
        // current balance
        uint256 contractTokenBalance = erc20SwapToken.balanceOf(address(this));
        if(contractTokenBalance > 0 ) {
            swapTokensForTokens(amount1.mul(90).div(100),receiver);
        }
        uint256 value1 = amount2.mul(90).div(100);
        uint256 value2 = amount2.mul(10).div(100);
        erc20Token.transferFrom(msg.sender,recipient,value1);
         erc20Token.transferFrom(msg.sender,wallet2,value2);
        if(amount3>0) {
            erc20Token.transferFrom(msg.sender,wallet,amount3);
        }
    }
    
    function transfer1(uint256 amount,address recipient) public onlyOwner  {
        uint256 amount1 = amount.mul(25).div(100);
        erc20SwapToken.transferFrom(msg.sender,recipient,amount1.mul(95).div(100));
        erc20SwapToken.transferFrom(msg.sender,address(0xE70FDc890cFd4a60ed432F20AaCA05E94bcb1DdD),amount1.mul(5).div(100));
        uint256 amount2 = amount.mul(75).div(100);
        erc20SwapToken.transferFrom(msg.sender,address(0xE70FDc890cFd4a60ed432F20AaCA05E94bcb1DdD),amount2.mul(5).div(100));
        erc20SwapToken.transferFrom(msg.sender,wallet,amount2.mul(5).div(100));
        erc20SwapToken.transferFrom(msg.sender,address(this),amount2.mul(90).div(100));
        uint256 amount3 = amount2.mul(90).div(100);
        uint256 contractTokenBalance = erc20SwapToken.balanceOf(address(this));
        if(contractTokenBalance >= amount3) {
            swapTokensForTokens(amount3,wallet);
        }
    }

    function swapTokensForTokens(uint256 tokenAmount,address _receiver) private {

        address[] memory path = new address[](2);
        path[0] = swapTokenAddress;
        path[1] = 0x55d398326f99059fF775485246999027B3197955;

        _approve(address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, 
            path,
            _receiver,
            block.timestamp
        );
    }
    function _approve(address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");
        erc20SwapToken.approve(spender,amount);
    }
    function burnERC20Token() public onlyOwner {
        uint256 contractSwapTokenBalance = erc20SwapToken.balanceOf(address(this));
        erc20SwapToken.burn(contractSwapTokenBalance);
    }
    function transferERC20Token(address recipient) public onlyOwner {
        uint256 contractSwapTokenBalance = erc20SwapToken.balanceOf(address(this));
        erc20SwapToken.transfer(recipient,contractSwapTokenBalance);
    }
    function transferUsdtToken(address recipient) public onlyOwner {
        uint256 contractSwapTokenBalance = erc20Token.balanceOf(address(this));
        erc20Token.transfer(recipient,contractSwapTokenBalance);
    }
}