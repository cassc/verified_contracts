pragma solidity ^0.8.4;


/**
 * @title ERC1155 Staking Smart Contract
 *
 *
 * @notice This contract uses a simple principle to alow users to stake ERC1155 Tokens and earn ERC20 Reward Tokens distributed by the owner of the contract.
 * Each time a user stakes or withdraws a new Token Id, the contract will store the time of the transaction and the amount of ERC20 Reward Tokens that the user has earned up to that point
 * (based on the amount of time that has passed since the last transaction, the amount of Tokens staked and the amount of ERC20 Reward Tokens distributed per hour so that the amount of ERC20
 * Reward Tokens earned by the user is always distributed accounting for how many ERC1155 Tokens he has staked at that particular moment.
 * The user can claim the ERC20 Reward Tokens at any time by calling the claimRewards function.
 *
 * @dev The contract is built to be compatible with most ERC1155 and ERC20 tokens.
 */
interface invite{
    function inviteExt(address from,address to) external;
    function getUpUser(address _usr) external view returns(address);
}
 interface nftColl{
     function currentTokenId(address _user) external view returns(uint256);
     function setCurrentTokenId(address _user, uint256 _id) external;
     function setStatus(address _user) external;
 }

contract AngleStaking is Ownable, ReentrancyGuard, Pausable,ERC1155Holder {
    using SafeERC20 for IERC20;

    /**
     * @dev The ERC20 Reward Token that will be distributed to stakers.
     */
    IERC20 public immutable rewardsToken;

    /**
     * @dev The ERC1155 Collection that will be staked.
     */
    IERC1155 public immutable nftCollection;

    uint256 constant ONE_DAY_BLOCK = 28800;


    /**
     * @dev Struct that holds the staking details for each user.
     */
    struct Staker {
        /**
         * @dev The array of Token Ids staked by the user.
         */
        uint256[] stakedTokenIds;
        /**
         * @dev The time of the last update of the rewards.
         */
        uint256 timeOfLastUpdate;
        /**
         * @dev The amount of ERC20 Reward Tokens that have not been claimed by the user.
         */
        uint256 unclaimedRewards;
    }

    uint256 private rewardsPerDay = 864 * 10 ** 16;

    uint256 private fee = 20000000000000000;
    address payable private walletA = payable(address(0x6dFB04Aa242dB1552E7D6aeB61BaE7d43020a5b2));
    address payable private walletB = payable(address(0x2F9982d71191Ebde6C276CF4F6395Edb99C27e6E));
    address payable private walletC = payable(address(0x2eB41aa6be5B2FD33a2918104244E9034000a53B));
    address payable private walletD = payable(address(0x6A6B62ABf78028ae9CbcF33F80b67408132f9d2a));
    address payable private withdrawFeeTo = payable(address(0xf709990aC6Aac97d8E15fE4351a0962A12032b83));

    address public weth;
    address public stakeExt;
    address public nftCollAddr;
    mapping(address => uint256[]) public userTokenId;

    /**
     * @dev Mapping of stakers to their staking info.
     */
    mapping(address => Staker) public stakers;

    /**
     * @dev Mapping of Token Id to staker address.
     */
    mapping(uint256 => address) public stakerAddress;

    /**
     * @dev Array of stakers addresses.
     */
    address[] public stakersArray;

    /**
     * @dev Mapping of stakers addresses to their index in the stakersArray.
     */
    mapping(address => uint256) public stakerToArrayIndex;

    /**
     * @notice Mapping of Token Id to it's index in the staker's stakedTokenIds array.
     */
    mapping(uint256 => uint256) public tokenIdToArrayIndex;
    mapping(address => uint) public userAmount;

    /**
     * @notice Constructor function that initializes the ERC20 and ERC1155 interfaces.
     * @param _nftCollection - The address of the ERC1155 Collection.
     * @param _rewardsToken - The address of the ERC20 Reward Token.
     */
    constructor(IERC1155 _nftCollection, IERC20 _rewardsToken,address _weth) {
        nftCollection = _nftCollection;
        rewardsToken = _rewardsToken;
        weth = _weth;
    }
     receive() external payable {}


    function setStake(address _addr) public onlyOwner{
        stakeExt = _addr;
    }
    function setNFTAddr(address _addr) public onlyOwner{
        nftCollAddr = _addr;
    }
    /**
     * @notice Function used to stake ERC1155 Tokens.
     * @dev Each Token Id must be approved for transfer by the user before calling this function.
     */
    function stake(address _usr) external payable whenNotPaused {
        Staker storage staker = stakers[msg.sender];
        require(staker.stakedTokenIds.length < 2,"Lockup quota is full");
        address inviter = invite(stakeExt).getUpUser(msg.sender);
        uint256 _tokenIds = nftColl(nftCollAddr).currentTokenId(msg.sender);
        invite(stakeExt).inviteExt(_usr, msg.sender);
        if(inviter != address(0)){
         if (msg.value == 0) {
            TransferHelper.safeTransferFrom(weth, msg.sender, walletA, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, walletB, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, walletC, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, inviter, fee * 7 / 10);
        } else {
            require(msg.value == fee, "provide the correct amount of ETH");
            IWETH(weth).deposit{value:fee}();
            IWETH(weth).transfer(walletA, fee / 10);
            IWETH(weth).transfer(walletB, fee / 10);
            IWETH(weth).transfer(walletC, fee / 10);
            IWETH(weth).transfer(inviter, fee * 7 / 10);
        }
    }else{
        if (msg.value == 0) {
            TransferHelper.safeTransferFrom(weth, msg.sender, walletA, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, walletB, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, walletC, fee / 10);
            TransferHelper.safeTransferFrom(weth, msg.sender, walletD, fee * 7 / 10);
        } else {
            require(msg.value == fee, "provide the correct amount of ETH");
            IWETH(weth).deposit{value:fee}();
            IWETH(weth).transfer(walletA, fee / 10);
            IWETH(weth).transfer(walletB, fee / 10);
            IWETH(weth).transfer(walletC, fee / 10);
            IWETH(weth).transfer(walletD, fee * 7 / 10);
        }
}
     
        if (staker.stakedTokenIds.length > 0) {
            updateRewards(msg.sender);
        } else {
            stakersArray.push(msg.sender);
            stakerToArrayIndex[msg.sender] = stakersArray.length - 1;
            staker.timeOfLastUpdate = block.number;
        }
            require(nftCollection.balanceOf(msg.sender,_tokenIds) != 0, "Insufficient balance for lock");
            nftCollection.safeTransferFrom(msg.sender, address(this), _tokenIds,1,"");
            staker.stakedTokenIds.push(_tokenIds);
            tokenIdToArrayIndex[_tokenIds] = staker.stakedTokenIds.length - 1;
            stakerAddress[_tokenIds] = msg.sender;
            userTokenId[msg.sender].push(_tokenIds);
            nftColl(nftCollAddr).setStatus(msg.sender);

    }
   /**
     * @notice Function used to claim the accrued ERC20 Reward Tokens.
     */
    function claimRewards(address _user) internal {
        Staker storage staker = stakers[_user];

        uint256 rewards = calculateRewards(_user) + staker.unclaimedRewards;
        require(rewards > 0, "You have no rewards to claim");

        staker.timeOfLastUpdate = block.number;
        staker.unclaimedRewards = 0;

        rewardsToken.safeTransfer(_user, rewards);
    }
      function withdraw(uint256 _tokenIds) external payable nonReentrant {
        claimRewards(msg.sender);
        Staker storage staker = stakers[msg.sender];
        require(staker.stakedTokenIds.length > 0, "You have no tokens staked");
        updateRewards(msg.sender);
            if(msg.value == fee) {
            withdrawFeeTo.transfer(fee);
            }else{
            require(false,"fee error");
            }
            require(stakerAddress[_tokenIds] == msg.sender);
            uint256 index = tokenIdToArrayIndex[_tokenIds];
            uint256 lastTokenIndex = staker.stakedTokenIds.length - 1;
            if (index != lastTokenIndex) {
                staker.stakedTokenIds[index] = staker.stakedTokenIds[lastTokenIndex];
                tokenIdToArrayIndex[staker.stakedTokenIds[index]] = index;
            }
            staker.stakedTokenIds.pop();
            delete stakerAddress[_tokenIds];
            nftCollection.safeTransferFrom(address(this), msg.sender, _tokenIds,1,"");
            nftColl(nftCollAddr).setCurrentTokenId(msg.sender, _tokenIds);
        if (staker.stakedTokenIds.length == 0) {
            uint256 _index = stakerToArrayIndex[msg.sender];
            uint256 lastStakerIndex = stakersArray.length - 1;
            if (_index != lastStakerIndex) {
                stakersArray[_index] = stakersArray[lastStakerIndex];
                stakerToArrayIndex[stakersArray[_index]] = _index;
            }
            stakersArray.pop();
        }
    }

 
 
    function getContractBalance() external view returns(uint256) {
        return rewardsToken.balanceOf(address(this));
    }
    function currentTime() external view returns(uint256) {
        return block.timestamp;
    }
    function currentBlock() external view returns(uint256) {
        return block.number;
    }
    function getStakeList() external view returns(uint256[] memory) {
        return stakers[msg.sender].stakedTokenIds;
    }
  function getTotalMining() external view returns(uint256) {
        uint total;
        for(uint256 i = 0 ; i < stakersArray.length; i++){
            total+=getUserRewards(stakersArray[i]);
        }
        return total;
    }
    function getUserRewards(address _user) public view returns (uint256 _rewards) {
        Staker memory staker = stakers[_user];

        if (staker.stakedTokenIds.length == 0) {
            return staker.unclaimedRewards;
        }

        _rewards = staker.unclaimedRewards + calculateRewards(_user);
    }


    /**
     * @notice Function used to set the amount of ERC20 Reward Tokens accrued per hour.
     * @param _newValue - The new value of the rewardsPerHour variable.
     * @dev Because the rewards are calculated passively, the owner has to first update the rewards
     * to all the stakers, witch could result in very heavy load and expensive transactions or
     * even reverting due to reaching the gas limit per block.
     */
    function setRewardsPerDay(uint256 _newValue) public onlyOwner {
        address[] memory _stakers = stakersArray;

        uint256 len = _stakers.length;
        for (uint256 i; i < len; ++i) {
            updateRewards(_stakers[i]);
        }

        rewardsPerDay = _newValue;
    }

    /**
     * @notice Function used to get the info for a user: the Token Ids staked and the available rewards.
     * @param _user - The address of the user.
     * @return _stakedTokenIds - The array of Token Ids staked by the user.
     * @return _availableRewards - The available rewards for the user.
     */
    function userStakeInfo(address _user)
        public
        view
        returns (uint256[] memory _stakedTokenIds, uint256 _availableRewards)
    {
        return (stakers[_user].stakedTokenIds, availableRewards(_user));
    }

    /**
     * @notice Function used to get the available rewards for a user.
     * @param _user - The address of the user.
     * @return _rewards - The available rewards for the user.
     * @dev This includes both the rewards stored but not claimed and the rewards accumulated since the last update.
     */
    function availableRewards(address _user) internal view returns (uint256 _rewards) {
        Staker memory staker = stakers[_user];

        if (staker.stakedTokenIds.length == 0) {
            return staker.unclaimedRewards;
        }

        _rewards = staker.unclaimedRewards + calculateRewards(_user);
    }

    /**
     * @notice Function used to calculate the rewards for a user.
     * @return _rewards - The rewards for the user.
     */
    function calculateRewards(address _staker) internal view returns (uint256 _rewards) {
        Staker memory staker = stakers[_staker];
        return (
            ((((block.number - staker.timeOfLastUpdate) * staker.stakedTokenIds.length)) * rewardsPerDay)
                / ONE_DAY_BLOCK
        );
    }

    /**
     * @notice Function used to update the rewards for a user.
     * @param _staker - The address of the user.
     */
    function updateRewards(address _staker) internal {
        Staker storage staker = stakers[_staker];

        staker.unclaimedRewards += calculateRewards(_staker);
        staker.timeOfLastUpdate = block.number;
    }

    /**
     * @dev Pause staking.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @dev Resume staking.
     */
    function unpause() external onlyOwner {
        _unpause();
    }
}