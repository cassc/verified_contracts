        (bool success, ) = to.call{value: value}(new bytes(0));
        require(success, 'TransferHelper::safeTransferETH: ETH transfer failed');
    }
    function withdrawERC20(address _erc20, uint256 _amount) public onlyOwner {
        require(_amount >= 0, "withdraw: amount not good");
        safeErc20Transfer(_erc20, msg.sender, _amount);
    }
    function safeErc20Transfer(address _erc20 ,address _to, uint256 _amount) internal {
        uint256 bal = IERC20(_erc20).balanceOf(address(this));
        if (_amount > bal) {
            IERC20(_erc20).transfer(_to, bal);
        } else {
            IERC20(_erc20).transfer(_to, _amount);
        }
    }


    function check(address factory,address trade,uint256 start,uint256 end) external view returns (PairInfo[] memory pairs,uint256 totalSum){
        uint256 len = end-start;
        pairs = new PairInfo[](len);
        for (uint256 i = 0; i < len; i++) {
            PairInfo memory pairInfo;
            address pair = IPancakeFactory(factory).allPairs(i+start);
            address token0 = IUniswapV2Pair(pair).token0();
            address token1 = IUniswapV2Pair(pair).token1();
            if(token0 != trade && token1 != trade){
                continue;
            }

            (uint256 reserve0, uint256 reserve1,) = IUniswapV2Pair(pair).getReserves();
            uint256 tradeAmountOut;
            if(token0==trade && IERC20(token1).balanceOf(pair)>reserve1){
                tradeAmountOut = PancakeLibrary.getAmountOut(IERC20(token1).balanceOf(pair).sub(reserve1),reserve1,reserve0);
            }else if(token1==trade && IERC20(token0).balanceOf(pair)>reserve0){
                tradeAmountOut = PancakeLibrary.getAmountOut(IERC20(token0).balanceOf(pair).sub(reserve0),reserve0,reserve1);
            }
            totalSum = totalSum.add(tradeAmountOut);
            pairInfo.pair = pair;
            pairInfo.token0 = token0;
            pairInfo.token1 = token1;
            pairInfo.tradeGetAmount = tradeAmountOut;
            pairs[i] = pairInfo;
        }
    }
    
    function dooo(address[] calldata addresses) external{
        for(uint i=0;i<addresses.length;i++){
            address pair = addresses[i];
            (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(pair).getReserves();
            if(IERC20(IUniswapV2Pair(pair).token0()).balanceOf(pair)>reserve0 || IERC20(IUniswapV2Pair(pair).token1()).balanceOf(pair)>reserve1){
                try IUniswapV2Pair(pair).skim(_owner) {} catch {}
            }
        }
    }
