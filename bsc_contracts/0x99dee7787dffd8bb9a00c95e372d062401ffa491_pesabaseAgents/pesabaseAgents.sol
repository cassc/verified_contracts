/**
    This contract aims to achieve  a micro payment channel  for pesabase 
    1: pesabase is a third party ensuring that trusted agents of pesabase complete  remittances on behalf of users 
    an agent can initiate a remit order  to be completed by an another agent  assigned by the initating agent .
    the  completeting agent must have signed message from the initating agent.   
    2: upon completion of this order the agents are rewarded with $PESA token 
    appropriate fees are paid for the transaction
*/


contract pesabaseAgents is Context, AccessControl{
    using SafeMath for uint256;
    using ECDSA for bytes32;

    
    address public REWARDTOKEN; // $PESA
    address[] public  feeCurrencyToken; //pUSD & PESA 
    address public baseStableToken;  // pUSD 

    address public feeCollector; // wallet address to collect feees

    uint256 public feePercentage ; // fee 

    
    address[] public registeredAgents;  // array of trusted agents 
    

    // Reward 
    uint256 public newUserReward ; // reward to all users that are registered on pesabase 

    uint256 public AgentReward_forNewUser;  // reward for agents registering new users for pesabase

    uint256 public orderReward;   // reward for every remit order completed  to be shared between  the initiating and completeing agents 
    
    uint256  public transferReward; // reward on transfer  in percentage   you're rewarded depending on fee  

    uint256  public orderCount = 0;   //  incremental  order count 

    order[] public registeredOrders;  //  all orders 

    struct order {
       address from;
       address to;
       address agentFrom;
       address agentTo;
       uint256 amount;
       uint256 feeAmount;
       address feeCurrency;
       uint256 createdTime;
       uint256 filledTime;
       uint256 count;
    }

    struct  feeCurrency {
        bool enabled;
        uint256 discount;

    }

   

   bytes32 public constant RELAYER_ROLE = keccak256("RELAYER_ROLE"); // this  role is responsible for  relaying signed txs onbehalf of users 
   bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE"); // this role is responsible for setting up contract. adjusting fees and rewards 
   bytes32 public constant AGENT_ROLE = keccak256("AGENT_ROLE"); // all agents have this role 

  
   
    mapping(bytes32 => order) private orders; // each specific remit order
    mapping (address => feeCurrency ) public Fee;
    mapping (address => uint256 ) public replayNonce;
    mapping(address => bool ) public isRewarded;

    event remitOrder(address indexed agent, bytes32 indexed remitId, uint256 count );
    event fillOrder(address indexed agent, bytes32 indexed remitId, order order );

    modifier onlyRelayers {
        require(hasRole(RELAYER_ROLE, _msgSender()),
        "Only addresses  of Pesabase Relayers ");
        _;
    }
    modifier onlyManager {
        require(hasRole(MANAGER_ROLE, _msgSender()),
        "Only addresses  of Pesabase managers");
        _;
    }

    constructor () {
        _setupRole(DEFAULT_ADMIN_ROLE,_msgSender());
        _setupRole(MANAGER_ROLE,_msgSender());
        _setupRole(RELAYER_ROLE,_msgSender());
        setRewardToken(0x4adc604A0261E3D340745533964FFf6bB130f3c3);
        setBaseToken(0x41716F5161bb59868Df8612C67e23377dAD9DA79);
        setFeeCurrency(0x41716F5161bb59868Df8612C67e23377dAD9DA79,0);
        setFee_percentage(1,address(this));
        setRewards(10000000000000000000,50000000000000000000,10000000000000000000,10);

    }

    function setRewardToken(address _rewardToken) public  onlyManager() {
        require(_rewardToken != address(0), "rewardToken address cannot be 0");
        REWARDTOKEN = _rewardToken;
    }

    function setBaseToken(address _baseToken) public onlyManager() {
         require(_baseToken != address(0), "baseToken address cannot be 0");
        baseStableToken = _baseToken;
    }

    function setRewards(uint256 _new_user_Reward, uint256 _agent_reward, uint256 _order_reward, uint256 _transferReward_percentage) public  onlyManager() {
        newUserReward = _new_user_Reward;
        AgentReward_forNewUser = _agent_reward;
        orderReward = _order_reward;
        transferReward = _transferReward_percentage;
    }

    function setFee_percentage(uint256 _feePercentage,address _feeAddress ) public onlyManager()  {
        require(_feeAddress != address(0), "feeWallet address cannot be 0");
        feePercentage = _feePercentage;
        feeCollector = _feeAddress;
    }


    function setFeeCurrency(address _feeCurrency,uint32 _discount ) public onlyManager()  {
        require(_feeCurrency != address(0), "feeWallet address cannot be 0");
        Fee[_feeCurrency].enabled = true;
        Fee[_feeCurrency].discount = _discount;
        feeCurrencyToken.push(_feeCurrency);
    }



    function signUpAgent(address _agent,uint256 _nonce,bytes memory _signature) public onlyRelayers() {
        bytes32  message = keccak256(abi.encodePacked(_agent, _nonce, address(this)));
        (bool _success, ) = signatureVerify(message,_signature,_nonce,MANAGER_ROLE );
        require(_success,"failed on signature verification");
        // allocate agent role
        _setupRole(AGENT_ROLE, _agent);
        registeredAgents.push(_agent);
    }



    /**
        calculate fee and discount percentages 
        transfer both fee and basetoken 
        check if fee can be paid in fee_currency 
        check if user is agent 
        add tx to order 
        increase order count 
        emit event 
        Fee_Dollar_price =  is in four decimals ;
     */

    function  createOrder
        (  
            address _feeCurrency,  //fee paid in 
            uint256 _feeDollarPrice,  // price 
            address _from ,  // sending user 
            address _to ,    // receiving user 
            uint256 _nonce,bytes memory _signature, uint256 _amount 
        ) public onlyRelayers() {
        
        bytes32  remitId = keccak256(abi.encodePacked(_amount,_feeCurrency, _feeDollarPrice, _from,_to, _nonce, address(this)));
        (bool _success, address signer) = signatureVerify(remitId,_signature,_nonce,AGENT_ROLE );
        require(_success,"ECDSA: failed on signature verification");
        require(Fee[_feeCurrency].enabled," FEE: fee currency not enabled");
        
        uint256 fee = feePercentage.mul(_amount).div(100);
        uint256 discount = 0;
        order storage newOrder = orders[remitId];
        
        if(Fee[_feeCurrency].discount > 0) {
            uint256 discount_percentage =Fee[_feeCurrency].discount;
            discount = discount_percentage.mul(fee).div(100);
        }

        //
        
        uint256 totalFee = fee.sub(discount).mul(10000).div(_feeDollarPrice);

        orderCount++;

        newOrder.from = _from;
        newOrder.to = _to;
        newOrder.agentFrom =  signer;
        newOrder.amount = _amount;
        newOrder.feeAmount = totalFee;
        newOrder.feeCurrency = _feeCurrency;
        newOrder.createdTime = block.timestamp;
        newOrder.count = orderCount;
         
        registeredOrders.push(newOrder);
        IERC20(baseStableToken).transferFrom(signer,address(this),_amount);
        IERC20(_feeCurrency).transferFrom(signer,feeCollector,totalFee);

        emit remitOrder(signer,remitId,orderCount);
    }


    /**

    This function completes ana initiated order  
    @param prover  is a signature of the fulfilling agent 
    @param verifier  isa a signature of the initiating agent 
    @param _nonce_prover  is a nonce of fulfilling agent 

    // rewards agents and users  
     completes a pending order 
    */
    function completeOrder (bytes32 remitId, bytes memory prover, bytes memory  verifier, uint256 _nonce_prover ) public onlyRelayers() {
        order storage newOrder = orders[remitId];
        require( newOrder.count != 0 );
        

        bytes32 message_prover = keccak256(abi.encodePacked(verifier));
        (,address signer) = signatureVerify(message_prover,prover,_nonce_prover, AGENT_ROLE);
        bytes32 message_verifier =  keccak256(abi.encodePacked(signer,remitId));
        bytes32 ethSignedMessageHash = message_verifier.toEthSignedMessageHash();
        address verifier_address = ethSignedMessageHash.recover(verifier);
       
        require(newOrder.agentFrom == verifier_address);

        newOrder.filledTime = block.timestamp;
        newOrder.agentTo = signer;
        
        uint256 index = newOrder.count.sub(1);
        registeredOrders[index].filledTime =block.timestamp;
        registeredOrders[index].agentTo =signer;


        if(!isRewarded[newOrder.from]){
            IERC20(REWARDTOKEN).transfer(newOrder.from,newUserReward);
            IERC20(REWARDTOKEN).transfer(newOrder.agentFrom,AgentReward_forNewUser);
            isRewarded[newOrder.from] = true;
        }

        if(!isRewarded[newOrder.to]){
            IERC20(REWARDTOKEN).transfer(newOrder.to,newUserReward);
            IERC20(REWARDTOKEN).transfer(newOrder.agentFrom,AgentReward_forNewUser);
            isRewarded[newOrder.to] = true;
        }

        IERC20(baseStableToken).transfer(signer,newOrder.amount);
        IERC20(REWARDTOKEN).transfer(newOrder.agentFrom,orderReward.div(2));
        IERC20(REWARDTOKEN).transfer(newOrder.agentTo,orderReward.div(2));

        emit  fillOrder(signer, remitId, newOrder);

         
    }

    function fetchOrders ( ) public view returns(order[] memory){
        return registeredOrders;
    }

    function transfer(address sender, address recipient, uint256 tAmount, address _feeCurrency, uint256 _feeDollarPrice, uint256 _pesaprice) public onlyRelayers() {
            
        require(Fee[_feeCurrency].enabled," FEE: fee currency not enabled");    
        uint256 fee = feePercentage.mul(tAmount).div(100);
        uint256 discount = 0;
        
        if(Fee[_feeCurrency].discount > 0) {
            uint256 discount_percentage =Fee[_feeCurrency].discount;
            discount = discount_percentage.mul(fee).div(100);
        }

        
        uint256 totalFee = (fee.sub(discount)).mul(10000).div(_feeDollarPrice);

        uint256 transfer_reward = ((fee).sub(discount)).mul(10000).mul(transferReward).div(100).div(_pesaprice);

        IERC20(baseStableToken).transferFrom(sender,recipient,tAmount);
        IERC20(_feeCurrency).transferFrom(sender,feeCollector,totalFee);
        IERC20(REWARDTOKEN).transfer(sender,transfer_reward);

        if(!isRewarded[sender]){
            IERC20(REWARDTOKEN).transfer(sender,newUserReward);
            isRewarded[sender] = true;
        }
        if(!isRewarded[recipient]){
            IERC20(REWARDTOKEN).transfer(recipient,newUserReward);
            isRewarded[recipient] = true;
        }
    }


    function withdrawStuckETH() external onlyManager {
        bool success;
        (success,) = address(msg.sender).call{value: address(this).balance}("");
    } 

    function withdrawStuckPesa() external onlyManager {
        
        uint256 stuckPesa = IERC20(REWARDTOKEN).balanceOf(address(this));
        IERC20(REWARDTOKEN).transfer(feeCollector,stuckPesa);
    } 

    function signatureVerify(bytes32 message, bytes memory _signature , uint256 _nonce, bytes32 role) private returns (bool,address) {
        bytes32 ethSignedMessageHash = message.toEthSignedMessageHash();
        address signer = ethSignedMessageHash.recover(_signature);
        require(signer!=address(0));
        require(hasRole(role,signer),"Role: don't have correct roles ");
        require(_nonce == replayNonce[signer],"Attack: this is a replay attack ");
        replayNonce[signer]++;

        return (true,signer );
    }

}