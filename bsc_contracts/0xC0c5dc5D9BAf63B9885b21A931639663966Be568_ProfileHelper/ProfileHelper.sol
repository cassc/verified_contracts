    IMoonPets public nftContract;
    uint256 public profilePictureSetCost = 0 ether;

    mapping(uint256 => bool) public petHasProfilePicture;

    event SetProfilePicture(uint256 petId);
    event RemoveProfilePicture(uint256 petId);

    constructor(IMoonPets _nftContract) {
        nftContract = _nftContract;
    }

    function setProfilePictureSetCost(uint256 _newCost) external onlyOwner {
        profilePictureSetCost = _newCost;
    }

    function setProfilePicture(uint256 _petId) external payable {
        require(msg.sender == nftContract.ownerOf(_petId), "Caller is not owner of the item");
        require(msg.value == profilePictureSetCost, "Not enough value in tx");
        petHasProfilePicture[_petId] = true;
        emit SetProfilePicture(_petId);
    }

    function removeProfilePicture(uint256 _petId) external payable {
        require(msg.sender == nftContract.ownerOf(_petId), "Caller is not owner of the item");
        require(msg.value == profilePictureSetCost, "Not enough value in tx");
        petHasProfilePicture[_petId] = false;
        emit RemoveProfilePicture(_petId);
    }

    function withdrawOwner() public payable onlyOwner {
        require(address(this).balance > 0, "Zero balance");
        (bool transferResult, ) = payable(owner()).call{value: address(this).balance}("");
        require(transferResult);
    }
}