    address constant USDT_WBNB_PAIR =
        0x16b9a82891338f9bA80E2D6970FddA79D1eb0daE; // 0x5F52Ad4bD4f519AE79999400ad8B83A3D002fD92;
    address constant USDC_WBNB_PAIR =
        0x30479874f9320A62BcE3bc0e315C920E1D73E278; // 0xd1742b5eC6798cEB8C791e0ebbEf606A4946f67E;
    address constant BUSD_WBNB_PAIR =
        0x58F876857a02D6762E0101bb5C46A8c1ED44Dc16; // 0x4E96D2e92680Ca65D58A0e2eB5bd1c0f44cAB897;

    // The address of factory
    address public CRIPTO_NO_PIX_FACTORY;

    // The fee address
    address public feeAddress = 0xd6693250A91Ace9d00779e1592ea543e1f4167AD;
    // The fee amount
    uint256 public feeAmount;
    uint256 feeDenominator = 10000;
    // The Pre Sale Token
    IERC20 public rewardToken;
    

    IERC20 public constant usdt = IERC20(USDT);
    IERC20 public constant usdc = IERC20(USDC);
    IERC20 public constant busd = IERC20(BUSD);
    IERC20 public constant wbnb = IERC20(WBNB);
    IUniswapV2Router02 public constant uniswapRouter =
        IUniswapV2Router02(PANCAKE_ROUTER);
    IUniswapV2Pair public constant usdtWbnbPair =
        IUniswapV2Pair(USDT_WBNB_PAIR);
    IUniswapV2Pair public constant usdcWbnbPair =
        IUniswapV2Pair(USDC_WBNB_PAIR);
    IUniswapV2Pair public constant busdWbnbPair =
        IUniswapV2Pair(BUSD_WBNB_PAIR);

    uint256 public maxBuy;
    uint256 public minBuy;

    uint256 public startBlock;
    uint256 public bonusEndBlock;

    uint256 public totalstakedAmount = 0;
    uint256 public totalSold;

    uint256 public totalPresale;
    // Whether it is initialized
    bool public isInitialized;

    // dev that will receive payments

    address public devWallet;

    // The price to sell share
    uint256 public price;

    mapping(address => uint256) public boughtAmount;

    event Buy(
        address indexed user,
        uint8 indexed token,
        uint256 amount,
        uint256 investment
    ); //token: 0 for BNB, 1 for USDT, 2 for USDC, 3 for BUSD.
    event SetPrice(uint256 price);
    event SetFeeAmount(uint256 feeAmount);
    event SetMaxBuy(uint256 amount);
    event SetMinBuy(uint256 amount);
    event UpdatedFeeAddress(address oldfeeaddress, address newfeeaddress);
    event UpdatedDevWallet(address olddevwallet, address newdevwallet);
    event NewStartAndEndBlocks(uint256 startBlock, uint256 endBlock);
    event AdminTokenRecovery(address tokenAddress, uint256 tokenAmount); 

    constructor() {
        CRIPTO_NO_PIX_FACTORY = msg.sender;
    }

    /*
     * @notice Initialize the contract
     * @param _rewardToken: Pre Sale Token
     * @param _startBlock: start block
     * @param _bonusEndBlock: end block
     * @param _poolLimitPerUser: pool limit per user in stakedToken (if any, else 0)
     * @param _admin: admin address with ownership
     */

    function initialize(
        IERC20 _rewardToken,
        uint256 _price,
        uint256 _startBlock,
        uint256 _bonusEndBlock,
        uint256 _maxBuy,
        uint256 _minBuy,
        uint256 _feeAmount,
        address _admin,
        address _devWallet
    ) external {
        require(!isInitialized, "Already initialized");
        require(msg.sender == CRIPTO_NO_PIX_FACTORY, "Not factory");
        // Make this contract initialized
        isInitialized = true;
        rewardToken = _rewardToken;
        price = _price;
        maxBuy = _maxBuy;
        minBuy = _minBuy;
        feeAmount = _feeAmount;
        startBlock = _startBlock;
        bonusEndBlock = _bonusEndBlock;
        devWallet = _devWallet;

        // Transfer ownership to the admin address who becomes owner of the contract
        transferOwnership(_admin);
    }

    receive() external payable {
        _buyWithBNB();
    }

    modifier onlyWhenSaleIsOn() {
        require(
            block.number >= startBlock && block.number <= bonusEndBlock,
            "Cripto No Pix: Pre venda nao iniciada"
        );
        _;
    }

    function buyWithUSDT(uint256 amount) external onlyWhenSaleIsOn {
        // transfer this usdt here
        usdt.safeTransferFrom(msg.sender, address(this), amount);
        
        uint256 amountPixFee = amount.mul(feeAmount).div(feeDenominator);
        uint256 amountUserTransaction = amount.sub(amountPixFee);
        // calculate the amount of token to send
        uint256 tokenAmount = (amountUserTransaction) / price;
        // send the token to the user
        rewardToken.safeTransfer(msg.sender, tokenAmount * 10 ** 18 );
        usdt.safeTransfer(feeAddress, amountPixFee);
        usdt.safeTransfer(devWallet, amountUserTransaction);
        // update the bought amount
        _buy(_msgSender(), 1, tokenAmount, amount);
    }

    function buyWithBUSD(uint256 amount) external onlyWhenSaleIsOn {
        // transfer this busd here
        busd.safeTransferFrom(msg.sender, address(this), amount);

        uint256 amountPixFee = amount.mul(feeAmount).div(feeDenominator);
        uint256 amountUserTransaction = amount.sub(amountPixFee);
        // calculate the amount of token to send
        uint256 tokenAmount = (amountUserTransaction) / price;
        // send the token to the user
        rewardToken.safeTransfer(msg.sender, tokenAmount * 10 ** 18);
        busd.safeTransfer(feeAddress, amountPixFee);
        busd.safeTransfer(devWallet, amountUserTransaction);
        // update the bought amount
        _buy(_msgSender(), 3, tokenAmount, amount);
    }

    function buyWithUSDC(uint256 amount) external onlyWhenSaleIsOn {
        // transfer this usdc here
        usdc.safeTransferFrom(msg.sender, address(this), amount);

        uint256 amountPixFee = amount.mul(feeAmount).div(feeDenominator);
        uint256 amountUserTransaction = amount.sub(amountPixFee);

        // calculate the amount of token to send
        uint256 tokenAmount = (amountUserTransaction ) / price;
        // send the token to the user
        rewardToken.safeTransfer(msg.sender, tokenAmount * 10 ** 18);
        usdc.safeTransfer(feeAddress, amountPixFee);
        usdc.safeTransfer(devWallet, amountUserTransaction);
        // update the bought amount
        _buy(_msgSender(), 2, tokenAmount, amount);
    }

    function buyWithBNB() external payable onlyWhenSaleIsOn {
        _buyWithBNB();
    }

    function _buyWithBNB() internal {
        uint256 usdtValue = _getBNBValueInUSDT(msg.value);
        uint256 amountPixFee = usdtValue.mul(feeAmount).div(feeDenominator);
        uint256 amountUserTransaction = usdtValue.sub(amountPixFee);
        // calculate the amount of token token to send
        uint256 tokenAmount = (amountUserTransaction) / price;
        // send the token to the user
        rewardToken.safeTransfer(msg.sender, tokenAmount * 10 ** 18);

        uint256 amountToFee = (msg.value).mul(feeAmount).div(feeDenominator);
        uint256 amountToDev = (msg.value).sub(amountToFee);
        if (feeAmount > 0){
         (bool tmpSuccess,) = payable(feeAddress).call{value: amountToFee, gas: 30000}("");
         (bool tmpSuccess1,) = payable(devWallet).call{value: amountToDev, gas: 30000}("");
                tmpSuccess = false; 
                tmpSuccess1 = false;  
                
        } else {
            (bool tmpSuccess,) = payable(devWallet).call{value: msg.value, gas: 30000}("");
             tmpSuccess = false; 
        }
        
        // update the bought amount
        _buy(_msgSender(), 0, tokenAmount, usdtValue);
    }

    /**
     * @dev returns the price of wbnb in usdt (18 decimals)
     */
    function _getBNBValueInUSDT(uint256 amount) public view returns (uint256) {
        address _wbnb = address(wbnb);
        address usdtAddress = address(usdt);

        (uint256 reserve0, uint256 reserve1, ) = usdtWbnbPair.getReserves();

        // this will be way mroe gas efficient that calling pair.token0() and pair.token1()
        // check the PancakeFactory.createPair() for more details on this logic
        if (_wbnb < usdtAddress) {
            // wbnb is token0
            return
                uniswapRouter.getAmountOut(amount, reserve0, reserve1);
        } else {
            // wbnb is token1
            return
                uniswapRouter.getAmountOut(amount, reserve1, reserve0);
        }
    }

    function _buy(
        address user,
        uint8 token,
        uint256 amount,
        uint256 investment
    ) internal {
        require(amount >= minBuy, "Cripto No Pix: montante menor que o minimo");
        boughtAmount[user] += amount;
        require(boughtAmount[user] <= maxBuy, "Cripto No Pix: excedeu a compra maxima");
        totalSold += amount;
        require(
            totalSold <= totalPresale,
            "Cripto No Pix: excedeu o total disponivel"
        );
        emit Buy(user, token, amount, investment);
    }

    function withdrawBNB() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawUSDT() external onlyOwner {
        usdt.safeTransfer(msg.sender, usdt.balanceOf(address(this)));
    }

    function withdrawUSDC() external onlyOwner {
        usdc.safeTransfer(msg.sender, usdc.balanceOf(address(this)));
    }

    function withdrawBUSD() external onlyOwner {
        busd.safeTransfer(msg.sender, busd.balanceOf(address(this)));
    }

    function payTokenOwner() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
        usdt.safeTransfer(msg.sender, usdt.balanceOf(address(this)));
        usdc.safeTransfer(msg.sender, usdc.balanceOf(address(this)));
        busd.safeTransfer(msg.sender, busd.balanceOf(address(this)));
    }

    function setPrice(uint256 _price) external onlyOwner {
        require(_price > 0, "Cripto No Pix: Price must be greater than 0");
        price = _price;
        emit SetPrice(_price);
    }

    function setFee(uint256 _feeAmount) external onlyOwner {
        feeAmount = _feeAmount;
        emit SetFeeAmount(_feeAmount);
    }

    function setMaxBuy(uint256 _maxBuy) external onlyOwner {
        maxBuy = _maxBuy;
        emit SetMaxBuy(_maxBuy);
    }

    function updateStartAndEndBlocks(
        uint256 _startBlock,
        uint256 _bonusEndBlock
    ) external onlyOwner {
        require(
            _startBlock < _bonusEndBlock,
            "New startBlock must be lower than new endBlock"
        );
        require(
            block.number < _startBlock,
            "New startBlock must be higher than current block"
        );

        startBlock = _startBlock;
        bonusEndBlock = _bonusEndBlock;

        emit NewStartAndEndBlocks(_startBlock, _bonusEndBlock);
    }

    function setMinBuy(uint256 _minBuy) external onlyOwner {
        minBuy = _minBuy;
        emit SetMinBuy(_minBuy);
    }

    function withdrawPresaleToken(uint256 amount) external onlyOwner {
        rewardToken.safeTransfer(msg.sender, amount);
        totalPresale = totalPresale.sub(amount);
    }

    function depositPresaleToken(uint256 amount) external onlyOwner {
        rewardToken.safeTransferFrom(msg.sender, address(this), amount);
        totalPresale = totalPresale.add(amount);
    }

    function stopReward() external onlyOwner {
        bonusEndBlock = block.number;
    }

    function updateFeeAddress(address newFeeAddress) external onlyOwner {
        emit UpdatedFeeAddress(feeAddress, newFeeAddress);
        feeAddress = newFeeAddress;
    }
    function updateDevWallet(address newDevWallet) external onlyOwner {
        emit UpdatedDevWallet(devWallet, newDevWallet);
        devWallet = newDevWallet;
    }
    function withdrawTokens(address _tokenAddress, address _walletAddress) external onlyOwner {
        uint256 tokenBalance = IERC20(_tokenAddress).balanceOf(address(this));
        IERC20(_tokenAddress).transfer(address(_walletAddress), tokenBalance);
        emit AdminTokenRecovery(_tokenAddress, tokenBalance);
    }

}