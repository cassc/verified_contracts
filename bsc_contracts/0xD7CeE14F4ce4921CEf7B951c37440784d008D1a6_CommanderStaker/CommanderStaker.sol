        private
        returns (AssetStake memory assetStake)
    {
        AssetStakerStorage storage _storage = assetStakerStorage();

        assetStake = _storage.slots[msg.sender][slot];
        delete _storage.slots[msg.sender][slot];
        _storage.stakedSlots[msg.sender].remove(slot);
