pragma solidity 0.8.7;

import "./ERC20.sol";
import "./ERC20Burnable.sol";
import "./Ownable.sol"; 

contract MCPEPECOIN is ERC20, ERC20Burnable, Ownable {
      
    constructor()
        ERC20("MCPEPE COIN", "MCPEPE") 
    {
        address supplyRecipient = 0xFbBe5f98d60812bBDd625cF75669842906c1a934;
        
        _mint(supplyRecipient, 694200000000 * (10 ** decimals()));
        _transferOwnership(0xFbBe5f98d60812bBDd625cF75669842906c1a934);
    }

    receive() external payable {}

    function decimals() public pure override returns (uint8) {
        return 18;
    }
    
    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        override
    {
        super._beforeTokenTransfer(from, to, amount);
    }

    function _afterTokenTransfer(address from, address to, uint256 amount)
        internal
        override
    {
        super._afterTokenTransfer(from, to, amount);
    }
}