contract TokenDistributor {
    constructor (address token) {
        IERC20(token).approve(msg.sender, uint(~uint256(0)));
    }
}

contract WyToken is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    using DateTimeLibrary for uint256;
    using Strings for uint256;
    mapping (address => uint256) private _balances;
    mapping (address => mapping (address => uint256)) private _allowances;

    string private _name = 'WY TOKEN';
    string private _symbol = 'WY';
    uint8 private _decimals = 18;
    uint256 private _totalSupply = 21000000 * 10**uint256(_decimals);
    
    mapping (address => bool) public isExcludedFee;
    mapping (address => bool) public isMarketPair;
    bool excludedMarketStatus=false;
    uint256 public _totalTaxIfBuying = 6;

    uint256 public _marketingTaxIfSelling = 1;
    uint256 public _nftTaxIfSelling = 1;
    uint256 public _lpTaxIfSelling = 2;
    uint256 public _rankingTaxIfSelling = 1;
    uint256 public _lpReturnTaxIfSelling = 1;
    uint256 public _totalTaxIfSelling = 6;

    address public  _marketingAddress=0x1A3554BFdDBE1D0D799448437E6e377ff8527fa9;
    address public  _nftAddress=0xd7cBeFFe8075808488dE61bF6A0419b518F1bE3B;
    address public  _lpReturnAddress=0x2908Dcf204f37e7CB6934C5d29EA57CC11515161;
    address public  _lpAddress=0x06b105401B45f4a642CBf5a690C15E0A8FdFe6A4;
    address public  _rankingAddress=0x11a14ea611E59109e548ea291310E1744Ab7E0f2;

    address public immutable usdt=0x55d398326f99059fF775485246999027B3197955;
    
    uint256 private minimumTokensBeforeSwap = 100 * 10**uint256(_decimals); 
    IPancakeRouter02 public  pancakeRouter02;
    TokenDistributor public _tokenDistributor;
    address public  pancakeV2Pair;
    constructor () {
        pancakeRouter02 = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        pancakeV2Pair = IPancakeFactory(pancakeRouter02.factory()).createPair(address(this), usdt);
        
        isExcludedFee[owner()] = true;
        isExcludedFee[address(this)] = true;
        isMarketPair[address(pancakeV2Pair)] = true;

        _tokenDistributor = new TokenDistributor(usdt);
        _balances[_msgSender()] = _totalSupply;
        
        emit Transfer(address(0),_msgSender(), _totalSupply);
    }
    receive() external payable {}
    function name() public view virtual returns (string memory) {
        return _name;
    }

    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }
 
    function decimals() public view virtual returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }
  
    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);
        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(currentAllowance >= amount, "ERC20: transfer amount exceeds allowance");
        _approve(sender,_msgSender(), currentAllowance.sub(amount));
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender] + addedValue);
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(currentAllowance >= subtractedValue, "ERC20: decreased allowance below zero");
        _approve(_msgSender(), spender, currentAllowance - subtractedValue);
        return true;
    }
    function setMarketPairStatus(address account, bool newValue) public onlyOwner {
        isMarketPair[account] = newValue;
    }

    function setExcludedFee(address addr) external onlyOwner returns (bool){
        isExcludedFee[addr] = true;
        return true;
    }
   
    function unExcludedFee(address addr) external onlyOwner returns (bool){
        isExcludedFee[addr] = false;
        return true;
    }
    
    function setAddress(address _m,address _n,address _l,address _ll,address _r) external onlyOwner returns (bool){
        _marketingAddress = _m;
        _nftAddress = _n;
        _lpReturnAddress = _l;
        _lpAddress = _ll;
        _rankingAddress = _r;
        return true;
    }

    function setSwapStatus(bool _eStatus,uint256 minNum) external onlyOwner returns (bool){
        excludedMarketStatus=_eStatus;
        minimumTokensBeforeSwap=minNum;
        return true;
    }
    uint public reserveAmount = 1 * 10 ** 13;
    function _transfer(address sender, address recipient, uint256 amount) internal virtual returns (bool){
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        if(inSwapAndLiquify){ 
            return _basicTransfer(sender, recipient, amount); 
        }else{
            uint256 contractTokenBalance = balanceOf(address(this));
            bool overMinimumTokenBalance = contractTokenBalance >= minimumTokensBeforeSwap;
            if (overMinimumTokenBalance&&!inSwapAndLiquify && !isMarketPair[sender]){
                swapAndLiquify(contractTokenBalance);
            }
            uint256 realAmount= amount;
            if(!sender.isContract()&&_balances[sender].sub(amount)<=reserveAmount){
                realAmount= amount.sub(reserveAmount);
            }
            _balances[sender] = _balances[sender].sub(realAmount, "Insufficient Balance");
            uint256 finalAmount = (isExcludedFee[sender] || isExcludedFee[recipient]) ? realAmount : takeFee(sender, recipient, realAmount);
            _balances[recipient] = _balances[recipient].add(finalAmount);
            emit Transfer(sender, recipient, finalAmount);
            return true;
        }
    }
    function _basicTransfer(address sender, address recipient, uint256 amount) internal returns (bool) {
        _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
        _balances[recipient] = _balances[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
        return true;
    }

    bool  inSwapAndLiquify;
    event SwapTokensForTokens(
        uint256 amountIn,
        address[] path
    );
    
    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
  
    event SwapAndLiquify(uint256 tokensSwapped,uint256 usdtReceived,uint256 tokensIntoLiqudity);
    function swapAndLiquify(uint256 tokenAmount) private lockTheSwap {
        uint256 half = tokenAmount.div(2);
        uint256 otherHalf = tokenAmount.sub(half);
        uint256 initialBalance= IERC20(usdt).balanceOf(address(this));
        swapTokensForExactTokens(half);
        uint256 newBalance = IERC20(usdt).balanceOf(address(this)).sub(initialBalance);
        addLiquidity(otherHalf, newBalance);
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }
    
    function swapTokensForExactTokens(uint256 tokenAmount) private {
        if(tokenAmount>0){
            address[] memory path = new address[](2);
            path[0] = address(this);
            path[1] = address(usdt);
            _approve(address(this), address(pancakeRouter02), tokenAmount);
            pancakeRouter02.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                tokenAmount,
                0, 
                path,
                address(_tokenDistributor),
                block.timestamp
            );
            uint256 totalAmount = IERC20(usdt).balanceOf(address(_tokenDistributor));
            if(totalAmount > 0){
                IERC20(usdt).transferFrom(address(_tokenDistributor), address(this), totalAmount);
            }
            emit SwapTokensForTokens(tokenAmount, path);
        }
    }

    function addLiquidity(uint256 tokenAmount, uint256 tokenBAmount) private {
        _approve(address(this), address(pancakeRouter02), tokenAmount);
        IERC20(usdt).approve(address(pancakeRouter02), tokenBAmount);
        pancakeRouter02.addLiquidity(
            address(this),
            usdt, 
            tokenAmount,
            tokenBAmount,
            0,
            0,
            _lpReturnAddress,
            block.timestamp
        );
    }

    function takeFee(address sender, address recipient, uint256 amount) internal returns (uint256) {
        uint256 feeAmount = 0;
        if(isMarketPair[sender]) {
            require(!excludedMarketStatus, "Swap: White List Transactions");
            feeAmount = amount.mul(_totalTaxIfBuying).div(100);
        }else if(isMarketPair[recipient]) {
            require(!excludedMarketStatus, "Swap: White List Transactions");
            feeAmount = amount.mul(_totalTaxIfSelling).div(100);
        }
        uint256 marketAmount=feeAmount.mul(_marketingTaxIfSelling).div(_totalTaxIfSelling);
        if(marketAmount > 0) {
            _balances[_marketingAddress] = _balances[_marketingAddress].add(marketAmount);
            setTaxAmount(1,marketAmount);
            emit Transfer(sender, _marketingAddress, marketAmount);
        }
        uint256 nftAmount=feeAmount.mul(_nftTaxIfSelling).div(_totalTaxIfSelling);
        if(nftAmount > 0) {
            _balances[_nftAddress] = _balances[_nftAddress].add(nftAmount);
            setTaxAmount(2,nftAmount);
            emit Transfer(sender, _nftAddress, nftAmount);
        }
        uint256 lpAmount=feeAmount.mul(_lpTaxIfSelling).div(_totalTaxIfSelling);
        if(lpAmount > 0) {
            _balances[_lpAddress] = _balances[_lpAddress].add(lpAmount);
            setTaxAmount(3,lpAmount);
            emit Transfer(sender, _lpAddress, lpAmount);
        }
        uint256 rankAmount=feeAmount.mul(_rankingTaxIfSelling).div(_totalTaxIfSelling);
        if(rankAmount > 0) {
            _balances[_rankingAddress] = _balances[_rankingAddress].add(rankAmount);
            setTaxAmount(4,rankAmount);
            emit Transfer(sender, _rankingAddress, rankAmount);
        }
        uint256 lpReturnAmount =feeAmount.sub(marketAmount).sub(nftAmount).sub(lpAmount).sub(rankAmount);
        if(lpReturnAmount>0){
            _balances[address(this)] = _balances[address(this)].add(lpReturnAmount);
            setTaxAmount(5,lpReturnAmount);
            emit Transfer(sender, address(this), lpReturnAmount);
        }
        return amount.sub(feeAmount);
    }

    mapping (string => mapping (uint => uint256)) private taxAmountMap;
    function setTaxAmount(uint taxType,uint256 amount) internal  {
        (uint year, uint month, uint day )= block.timestamp.timestampToDate();
        string memory dateStr=  string(abi.encodePacked(year.toString(), month.toString(), day.toString()));
        taxAmountMap[dateStr][taxType] = taxAmountMap[dateStr][taxType].add(amount);
    }

    function getTaxAmount(string memory dateStr) public view  returns (uint256,uint256,uint256,uint256,uint256) {
        return (taxAmountMap[dateStr][1],taxAmountMap[dateStr][2],taxAmountMap[dateStr][3],taxAmountMap[dateStr][4],taxAmountMap[dateStr][5]);
    }

    function burn(uint256 amount) public returns (bool) {
        _burn(_msgSender(), amount);
        return true;
    }

    function _burn(address account, uint256 amount) internal {
        require(account != address(0), "BEP20: burn from the zero address");
        _balances[account] = _balances[account].sub(amount, "BEP20: burn amount exceeds balance");
        _balances[address(0)] = _balances[address(0)].add(amount);
        emit Transfer(account, address(0), amount);
    }

    function _approve(address owner, address spender, uint256 amount) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");
        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }
}