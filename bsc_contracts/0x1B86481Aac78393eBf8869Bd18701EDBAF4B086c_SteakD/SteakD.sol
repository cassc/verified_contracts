    function getParis()
        external
        view
        returns (address[] memory availablePairs)
    {
        uint256 length = 0;
        for (uint256 i = 0; i < pairs.length; i++) {
            address pair = pairs[i];
            if (pairsMap[pair]) {
                length.add(1);
            }
        }
        availablePairs = new address[](length);
        uint256 index = 0;
        for (uint256 i = 0; i < pairs.length; i++) {
            address pair = pairs[i];
            if (pairsMap[pair]) {
                availablePairs[index] = pair;
                index.add(1);
            }
        }
    }

