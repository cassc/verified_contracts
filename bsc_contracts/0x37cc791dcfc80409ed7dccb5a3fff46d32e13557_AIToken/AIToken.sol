    function _isLiquidity(address from, address to) internal view returns (bool isAdd, bool isDel){
        address token0 = IPancakePair(address(swapV2Pair)).token0();
        (uint r0,,) = IPancakePair(address(swapV2Pair)).getReserves();
        uint bal0 = IERC20(token0).balanceOf(address(swapV2Pair));
        if (pairs[to]) {
            // 卖币 或者 添加流动性
            if (token0 != address(this) && bal0 > r0) {
                isAdd = bal0 - r0 > addPriceTokenAmount;
            }
        }
        if (pairs[from]) {
            // 买币 或者 撤流动性
            if (token0 != address(this) && bal0 < r0) {
                isDel = r0 - bal0 > 0;
            }
        }
    }

    function processLpReward(uint256 gas) private {

        uint256 shareholderCount = holders.length;
        if (shareholderCount == 0) {
            return;
        }
        IERC20 rewardToken = IERC20(USDT);
        uint256 balance = rewardToken.balanceOf(lpTokenReceiver);
        if (balance < minLpAmount) {
            return;
        }

        IERC20 holdToken = IERC20(swapV2Pair);
        uint holdTokenTotal = holdToken.totalSupply();
        for (uint i = 0; i < excludeSupplyHolder.length; i++) {
            uint256 value = holdToken.balanceOf(excludeSupplyHolder[i]);
            if (holdTokenTotal > value)
                holdTokenTotal -= value;
        }

        address shareHolder;
        uint256 tokenBalance;
        uint256 amount;

        uint256 gasUsed = 0;
        uint256 iterations = 0;
        uint256 gasLeft = gasleft();
        while (gasUsed < gas && iterations < shareholderCount) {
            if (currentIndex >= shareholderCount) {
                currentIndex = 0;
            }
            shareHolder = holders[currentIndex];
            tokenBalance = holdToken.balanceOf(shareHolder);
            if (tokenBalance > 0) {
                amount = balance * tokenBalance / holdTokenTotal;
                if (amount > 0) {
                    IERC20(USDT).transferFrom(lpTokenReceiver, shareHolder, amount);
                }
            }
            gasUsed = gasUsed + (gasLeft - gasleft());
            gasLeft = gasleft();
            currentIndex++;
            iterations++;
        }
    }


