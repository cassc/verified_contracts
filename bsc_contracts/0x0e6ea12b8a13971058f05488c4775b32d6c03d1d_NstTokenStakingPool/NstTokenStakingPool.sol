contract NstTokenStakingPool is Ownable, ReentrancyGuardUpgradeable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    using EnumerableSet for EnumerableSet.AddressSet;

    // Info of each user.
    struct UserInfo {
        uint256 amount;     // How many LP tokens the user has provided.
        uint256 lastRewardTime;
        uint256 rewardReceived; //Reward received
        uint256 pendingInviteReward;//pending  invitation reward
        uint256 receivedInviteReward;//received  invitation reward
    }

    struct DepositOrder {
        uint256 orderTime;
        uint256 amount;
    }

    mapping(uint256 => mapping(address => DepositOrder[])) public userDepositInfo;

    // Info of each pool.
    struct PoolInfo {
        IERC20 lpToken;           // Address of LP token contract.
        uint256 apr;            //APR
        uint256 totalAmount;    // Total amount of current pool deposit.
        uint256 lockSeconds;    //After lock seconds can release to withdraw
        address bonusToken;
        uint256 depositMin;   //deposit limitation of one time
        uint256 depositMax;   //
    }

    // The Platform Token!
    //IToken public iToken;
    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // pid corresponding address
    mapping(address => uint256) public LpOfPid;
    // Control mining
    bool public paused = false;
    // adminAddress
    address public adminAddress;

    uint256 public AprDenominator = 1000;

    uint256[2] public directReferralBonuses;
    //current user index
    uint256 public currentId;
    mapping(address => uint256) public addressToId;
    mapping(uint256 => address) public idToAddress;
    mapping(address => address) public userToReferrer;
    mapping(address => address[]) public directSubordinates;
    mapping(uint256 => mapping(address => mapping(address => uint256))) public inviteRewardInfo;


    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event Claimed(address indexed user, uint256 indexed pid, uint256 amount);
    event claimedInviteReward(address indexed user, uint256 indexed pid, uint256 amount);

    event PoolAdded(uint256 _pid, IERC20 _lpToken, address _bonusToken, uint256 _lockSeconds, uint256 _depositMin, uint256 _depositMax);
    event PoolBonusChanged(uint256 _pid, address _bonusToken, uint256 _lockSeconds, uint256 _depositMin, uint256 _depositMax);
    event PoolDepositChanged(uint256 _pid, uint256 _depositMin, uint256 _depositMax);
    event PoolAprChanged(uint256 _pid, uint256 _apr);
    event NewReferral(address indexed user, address indexed referral);

    constructor(
    )  {
        adminAddress = msg.sender;
        directReferralBonuses = [200, 100];
        currentId = 1;
    }

    modifier onlyAdmin() {
        require(msg.sender == adminAddress, "admin: wut?");
        _;
    }

    // Update admin address by the previous dev.
    function setAdmin(address _adminAddress) public onlyOwner {
        adminAddress = _adminAddress;
    }

    // Set bonuses ratio
    function setDirectReferralBonuses(uint256 _bonuses0, uint256 _bonuses1) public onlyAdmin {
        directReferralBonuses[0] = _bonuses0;
        directReferralBonuses[1] = _bonuses1;
    }

    function _addUser(address _user, address _referrer) private {
        addressToId[_user] = currentId;
        idToAddress[currentId] = _user;
        userToReferrer[_user] = _referrer;
        directSubordinates[_referrer].push(_user);
        currentId++;
        emit NewReferral(_referrer, _user);
    }

    /**
     * @notice  Function to add DirectReferral
     * @param _referrer Address
     */
    function addDirectReferral(address _referrer) public notPause nonReentrant{
        require(_referrer != address(0), "DirectReferral::referrer is zero address");
        require(msg.sender != _referrer, "DirectReferral::user address cannot be the same as referrer");
        require(userToReferrer[msg.sender] == address(0),"DirectReferral::user had bind referrer");

        // If user didn't exsist before
        if (addressToId[msg.sender] == 0) {
            _addUser(msg.sender, _referrer);
        }
    }

    function getTwoParent(address _user) public view returns (address, address){
        address _referrer0 = userToReferrer[_user];
        require(_user != address(0), "getTwoParent::user is zero address");
        if (_referrer0 != address(0)) {
            address _referrer1 = userToReferrer[_referrer0];
            return (_referrer0, _referrer1);
        }else{
            return (address(0), address(0));
        }
    }

    function timestampToDateTime(uint timestamp) public pure returns (uint year, uint month, uint day, uint hour, uint minute, uint second) {
        (year, month, day, hour, minute, second) = BokkyPooBahsDateTimeLibrary.timestampToDateTime(timestamp);
    }

    function blockTimestampToDateTime() public view returns (uint year, uint month, uint day, uint hour, uint minute, uint second) {
        (year, month, day, hour, minute, second) = BokkyPooBahsDateTimeLibrary.timestampToDateTime(block.timestamp);
    }

    function diffNatureDays(uint timestampStart,uint timestampEnd) public pure returns (uint diffDays) {
        diffDays = BokkyPooBahsDateTimeLibrary.diffNatureDays(timestampStart, timestampEnd);
    }

    // Set the pid of pool
    function setBonus(uint256 _pid, address _bonusToken, uint256 _lockSeconds, uint256 _depositMin, uint256 _depositMax) public onlyAdmin {
        require(_pid < poolInfo.length, "pid is invalid");
        require(_bonusToken != address(0x0), "bonusToken error");
        require(_depositMax >= _depositMin, "limit error");

        PoolInfo storage pool = poolInfo[_pid];

        pool.bonusToken = _bonusToken;
        pool.lockSeconds = _lockSeconds;
        pool.depositMin = _depositMin;
        pool.depositMax = _depositMax;

        emit PoolBonusChanged(_pid, _bonusToken, _lockSeconds, _depositMin, _depositMax);
    }

    function poolLength() public view returns (uint256) {
        return poolInfo.length;
    }

    function setPause() public onlyOwner {
        paused = !paused;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(IERC20 _lpToken, uint256 _apr, address _bonusToken, uint256 _lockSeconds, uint256 _depositMin, uint256 _depositMax) public onlyAdmin {
        require(address(_lpToken) != address(0), "_lpToken is the zero address");
        require(_depositMax >= _depositMin, "limit error");
        require(_apr <= AprDenominator, "apr error");

        poolInfo.push(PoolInfo({
        lpToken : _lpToken,
        apr : _apr,
        totalAmount : 0,
        bonusToken : _bonusToken,
        lockSeconds : _lockSeconds,
        depositMin : _depositMin,
        depositMax : _depositMax
        }));

        uint256 _pid = poolLength() - 1;
        LpOfPid[address(_lpToken)] = _pid;

        emit PoolAdded(_pid, _lpToken, _bonusToken, _lockSeconds, _depositMin, _depositMax);
    }

    // Update the given pool's IToken allocation point. Can only be called by the owner.
    function setApr(uint256 _pid, uint256 _apr) public onlyAdmin {
        require(_pid <= poolLength() - 1, "not find this pool");

        poolInfo[_pid].apr = _apr;

        emit PoolAprChanged(_pid, _apr);
    }


    function inviteRewardAmount(address _referrer, address _user) external view returns (uint256){
        require(_referrer != address(0), "DirectReferral::referrer is zero address");
        require(_user != address(0), "DirectReferral::_user is zero address");

        uint256 rewardAmount = 0;
        uint256 len = poolInfo.length;
        for (uint256 i = 0; i < len; i++) {
            rewardAmount = rewardAmount.add(inviteRewardInfo[i][_referrer][_user]);
        }

        return rewardAmount;
    }

    // View function to see pending ITokens on frontend.
    function pending(uint256 _pid, address _user) external view returns (uint256){
        uint256 iTokenAmount = pendingIToken(_pid, _user);
        return iTokenAmount;
    }

    function lockedToken(uint256 _pid, address _user) external view returns (uint256){
        PoolInfo storage pool = poolInfo[_pid];

        uint256 lockedAmount = 0;
        DepositOrder[] memory orders = userDepositInfo[_pid][_user];
        uint256 len = orders.length;
        uint256 checkTime = block.timestamp;

        for (uint256 i = 0; i < len; i++) {
            if (orders[i].orderTime.add(pool.lockSeconds) > checkTime) {
                lockedAmount = lockedAmount.add(orders[i].amount);
            }
        }

        return lockedAmount;
    }


    function pendingIToken(uint256 _pid, address _user) private view returns (uint256){
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];

        uint256 pendingAmount = 0;
        if (user.amount > 0 && user.lastRewardTime > 0) {
            uint256 diffDays = BokkyPooBahsDateTimeLibrary.diffNatureDays(user.lastRewardTime, block.timestamp);
            if (diffDays > 0) {
                pendingAmount = user.amount.mul(pool.apr).div(AprDenominator).mul(diffDays);
            }
        }

        return pendingAmount;
    }

    function setUserInfolastRewardTime(uint256 _pid, address _user, uint256 _lastRewardTime) public notPause onlyAdmin{
        UserInfo storage user = userInfo[_pid][_user];
        if (user.amount > 0) {
            user.lastRewardTime = _lastRewardTime;
        }
    }

    // Deposit LP tokens to HecoPool for IToken allocation.
    function deposit(uint256 _pid, uint256 _amount) public notPause nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        require(_amount == 0 || (_amount >= pool.depositMin && _amount <= pool.depositMax), "deposit amount need in range");
        depositIToken(_pid, _amount, msg.sender);
    }

    function depositIToken(uint256 _pid, uint256 _amount, address _user) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];

        if (user.amount > 0) {
            claimUserReward(_pid, _user);
        }

        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(_user, address(this), _amount);
            pool.totalAmount = pool.totalAmount.add(_amount);
            user.amount = user.amount.add(_amount);
            //first time deposit
            if (user.lastRewardTime == 0) {
                user.lastRewardTime = block.timestamp;
            }
        }

        userDepositInfo[_pid][msg.sender].push(DepositOrder({orderTime : block.timestamp, amount : _amount}));

        emit Deposit(_user, _pid, _amount);
    }

    function claimReward(uint256 _pid, address _user) public notPause nonReentrant {
        claimUserReward(_pid, _user);
    }

    function claimUserReward(uint256 _pid, address _user) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];

        if (user.amount > 0) {
            require(user.lastRewardTime > 0 && user.lastRewardTime < block.timestamp, "lastRewardTime is invalid");
            uint256 diffDays = BokkyPooBahsDateTimeLibrary.diffNatureDays(user.lastRewardTime, block.timestamp);
            if (diffDays > 0) {
                uint256 pendingAmount = user.amount.mul(pool.apr).div(AprDenominator).mul(diffDays);
                if (pendingAmount > 0) {
                    safeBonusTransfer(_pid, _user, pendingAmount);                    
                    user.rewardReceived = user.rewardReceived.add(pendingAmount);
                    user.lastRewardTime = block.timestamp;    

                    //for parents
                    (address referrer0, address referrer1) = getTwoParent(_user);
                    //first level parent
                    addPendingInviteReward(_pid,_user,referrer0, pendingAmount, 0);
                    //second level parent
                    addPendingInviteReward(_pid,_user,referrer1, pendingAmount, 1);        
                }

                emit Claimed(_user, _pid, pendingAmount);
            }
        }
    }

    function addPendingInviteReward(uint256 _pid, address _user,address _referrer, uint256 _pendingAmount, uint256 _bonusesIndex) private {
        if (_referrer != address(0) && _pendingAmount > 0) {
            UserInfo storage user = userInfo[_pid][_referrer];
            uint256 pendingInviteReward = _pendingAmount.mul(directReferralBonuses[_bonusesIndex]).div(1000);
            user.pendingInviteReward = user.pendingInviteReward.add(pendingInviteReward);

            inviteRewardInfo[_pid][_referrer][_user] = inviteRewardInfo[_pid][_referrer][_user].add(pendingInviteReward);
        }
    }

    function claimInviteReward(uint256 _pid, address _user) public notPause nonReentrant{
        claimParentInviteReward(_pid,_user);
    }

    function claimParentInviteReward(uint256 _pid, address _user) private {
        UserInfo storage user = userInfo[_pid][_user];

        uint256 pendingAmount = user.pendingInviteReward;
        if (pendingAmount > 0) {
            safeBonusTransfer(_pid, _user, pendingAmount);
            user.pendingInviteReward = user.pendingInviteReward.sub(pendingAmount);
            user.receivedInviteReward = user.receivedInviteReward.add(pendingAmount);
        }

        emit  claimedInviteReward(_user, _pid, pendingAmount);
    }

    // Withdraw LP tokens from HecoPool.
    function withdraw(uint256 _pid, uint256 _amount) public notPause nonReentrant{
        withdrawIToken(_pid, _amount, msg.sender);
    }

    function withdrawIToken(uint256 _pid, uint256 _amount, address _user) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        require(user.amount >= _amount, "withdrawIToken: not good");

        if (user.amount > 0) {
            claimUserReward(_pid, _user);
        }

        if (user.pendingInviteReward > 0) {
            claimParentInviteReward(_pid, _user);
        }


        if (_amount > 0) {
            //need caculate unlock amount
            uint256 unlockAmount = 0;
            DepositOrder[] memory orders = userDepositInfo[_pid][msg.sender];
            uint256 len = orders.length;
            uint256 index = 0;
            uint256 checkTime = block.timestamp;

            if (len > 0) {
                index = len;
                for (uint256 i = 0; i < len; i++) {
                    if (orders[i].orderTime.add(pool.lockSeconds) <= checkTime) {
                        unlockAmount = unlockAmount.add(orders[i].amount);
                        if (unlockAmount > _amount) {
                            index = i;
                            break;
                        }
                    }
                    else {
                        index = i;
                        break;
                    }
                }

                //pop some orders
                for (uint256 i = 0; i < len - index; i++) {
                    userDepositInfo[_pid][msg.sender][i] = userDepositInfo[_pid][msg.sender][i + index];
                }
                for (uint256 j = len - index; j < len; j++) {
                    userDepositInfo[_pid][msg.sender].pop();
                }

                if (unlockAmount > _amount) {
                    userDepositInfo[_pid][msg.sender][0].amount = unlockAmount.sub(_amount);
                }
                else {
                    _amount = unlockAmount;
                }

            }
            user.amount = user.amount.sub(_amount);
            pool.totalAmount = pool.totalAmount.sub(_amount);
            pool.lpToken.safeTransfer(_user, _amount);
        }
        emit Withdraw(_user, _pid, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public notPause {
        emergencyWithdrawIToken(_pid, msg.sender);
    }

    function emergencyWithdrawIToken(uint256 _pid, address _user) private {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardReceived = 0;
        user.pendingInviteReward = 0;
        user.receivedInviteReward = 0;

        pool.lpToken.safeTransfer(_user, amount);
        pool.totalAmount = pool.totalAmount.sub(amount);
        emit EmergencyWithdraw(_user, _pid, amount);
    }


    modifier notPause() {
        require(paused == false, "Contract has been suspended");
        _;
    }


    // Safe bonus transfer function, just in case if rounding error causes pool to not have enough bonus.
    function safeBonusTransfer(uint256 _pid, address _to, uint256 _amount) internal {
        PoolInfo storage pool = poolInfo[_pid];

        uint256 bal = IERC20(pool.bonusToken).balanceOf(address(this));
        require(_amount <= bal, "pool to not have enough bonusTokens");
        IERC20(pool.bonusToken).transfer(_to, _amount);
    }


    function withdrawEmergency(address tokenaddress, address to) public onlyOwner {
        TransferHelper.safeTransfer(tokenaddress, to, IERC20(tokenaddress).balanceOf(address(this)));
    }

    function withdrawEmergencyNative(address to, uint256 amount) public onlyOwner {
        TransferHelper.safeTransferNative(to, amount);
    }
}