pragma solidity ^0.8.0;
import "./ERC20.sol";
import "./LibString.sol";

/*´:°•.°+.*•´.*:˚.°*.˚•´.°:°•.°•.*•´.*:˚.°*.˚•´.°:°•.°+.*•´.*:*/
/*                        PEEPO                               */
/*.•°:°.´+˚.*°.˚:*.´•*.+°.•°:´*.´•*.•°.•°:°.´:•˚°.*°.˚:*.´+°.•*/
contract Peepo is ERC20 {
    using LibString for uint16;
    uint16 _miladyChain;
    uint256 public _previousBurnedAmount;
    mapping(uint16 => address) public validators;

    constructor() {
        _mint(msg.sender, 69_000_000 * 1e18);
    }

    function name() public pure override returns (string memory) {
        return "Peepo";
    }

    function symbol() public pure override returns (string memory) {
        return "PEEPO";
    }

    function decimals() public pure override returns (uint8) {
        return 18;
    }

    /*´:°•.°+.*•´.*:˚.°*.˚•´.°:°•.°•.*•´.*:˚.°*.˚•´.°:°•.°+.*•´.*:*/
    /*                        MILADY                              */
    /*.•°:°.´+˚.*°.˚:*.´•*.+°.•°:´*.´•*.•°.•°:°.´:•˚°.*°.˚:*.´+°.•*/

    function milady() public pure returns (string memory) {
        return "Milady";
    }

    function miladyChain() public view returns (uint16) {
        return _miladyChain;
    }

    function validateChain(uint256 burnAmount) external {
        require(burnAmount > _previousBurnedAmount, "MORE PEEPO");
        _burn(msg.sender, burnAmount);
        _previousBurnedAmount = burnAmount;
        validators[_miladyChain] = msg.sender;
        assembly {
            let chain := sload(_miladyChain.slot)
            chain := add(chain, 1)
            chain := mod(chain, 10000)
            sstore(_miladyChain.slot, chain)
        }
    }

    function godsFavorite() public view returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "https://www.miladymaker.net/milady/",
                    _miladyChain.toString(),
                    ".png"
                )
            );
    }
}
