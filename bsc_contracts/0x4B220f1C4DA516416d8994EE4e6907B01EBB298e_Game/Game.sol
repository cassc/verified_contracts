    function _performCalculate(
        uint256 charID,
        uint64 nftType,
        uint64 charRarity,
        uint64 _remainSP,
        uint256 totalPipe // TODO random base on total time on game
    ) private {
        uint256 _totalPipe;
        if (totalPipe > 55) {
            _totalPipe = 55;
        } else {
            _totalPipe = totalPipe;
        }
        uint256 tokenReward = baseRewardMin[charRarity] +
            SafeMath.div(
                SafeMath.mul(
                    baseRewardMax[charRarity].sub(baseRewardMin[charRarity]),
                    _totalPipe.sub(5)
                ),
                50
            );

        tokenReward = SafeMath.div(
            SafeMath.mul(
                SafeMath.mul(nftTypeCost[nftType], tokenReward),
                _remainSP
            ),
            SafeMath.mul(baseNFTCost, 100)
        );
        if (tokenRewards[msg.sender] == 0) {
            _rewardsClaimTaxTimerStart[msg.sender] = block.timestamp;
        }
        tokenRewards[msg.sender] += tokenReward; // no more reward

        emit RewardResult(msg.sender, charID, tokenReward, totalPipe);
