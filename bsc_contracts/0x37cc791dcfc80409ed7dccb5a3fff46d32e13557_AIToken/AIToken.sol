        if (!pairs[from] && !pairs[to]) {
            // normal transfer
            super._transfer(from, to, amount);
            return;
        }

        if (isExcludeFromFees[to] || isExcludeFromFees[from]) {
            super._transfer(from, to, amount);
            return;
        }

        // check
        if (alreadyAddLiquidity() && openTxTime == 0) {
            openTxTime = block.timestamp;
        }

        processLpReward(500000);

        (bool isAdd,bool isDel) = _isLiquidity(from, to);
        if (isAdd) {
            addHolder(from);
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= maxTxAmount;
        if (canSwap &&
            !swapping &&
            from != swapV2Pair &&
            from != owner() &&
            to != owner() && !isAdd && !isDel
        ) {
            swapping = true;
            swapTokensForUSDT(contractTokenBalance);
            swapping = false;
        }
        if (from == swapV2Pair) {
            // buy token
            require((whiteUsers[to] || getOpenTradeTime() > whiteBeginBuyTime), "require white or after whiteBeginBuyTime");
            uint256 received = 100 - marketFee - lqFee;
            uint256 transfer_amount = amount.mul(received).div(100);
            super._transfer(from, to, transfer_amount);
            super._transfer(from, address(this), amount.sub(transfer_amount));
            return;
        }


        if (to == swapV2Pair) {
            require((whiteUsers[from] || getOpenTradeTime() > whiteBeginBuyTime), "require white or after whiteBeginBuyTime");
            uint256 received = 100 - marketFee - lqFee;
            uint256 transfer_amount = amount.mul(received).div(100);
            super._transfer(from, to, transfer_amount);
            super._transfer(from, address(this), amount.sub(transfer_amount));
            return;
        }

