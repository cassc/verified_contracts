pragma solidity 0.8.18;

contract RentableNFT is IERC4907, Ownable, ReentrancyGuard, Pausable {
    using SafeMath for uint;
    using SafeERC20 for IERC20;
    using Address for address;
    using EnumerableSet for EnumerableSet.UintSet;

    IERC721 public reNFT;
    uint public beneficiaryFee = 500;
    uint constant DIVISOR = 10000;

    EnumerableSet.UintSet durations;
    mapping(uint => NFT) public nftList;
    mapping(address => bool) public isTokenEnabled;

    struct NFT {
        address beneficiary;
        address user;
        address token;
        uint rentalAmt;
        uint expires;
        uint duration;
    }

    event EnabledToken(
        address indexed token,
        uint at
    );

    event DisabledToken(
        address indexed token,
        uint at
    );

    event ListNFT(
        address indexed beneficiary,
        address indexed token,
        uint indexed tokenId,
        uint rental,
        uint duration,
        uint at
    );

    event RemoveNFT(
        address indexed beneficiary,
        uint indexed tokenId,
        uint at
    );

    event User(
        address indexed user,
        uint indexed tokenId,
        uint expires,
        uint at
    );

    modifier onlyEOA() {
        require(!_msgSender().isContract(), "onlyEOA::non-contract_call");
        _;
    }

    constructor(IERC721 nft, address tag, address gem) {
        reNFT = nft;
        isTokenEnabled[tag] = isTokenEnabled[gem] = true;
    }

    function setReNFT(IERC721 nft) external onlyOwner {
        reNFT = nft;
    }

    function setBeneficiaryFee(uint fee) external onlyOwner {
        beneficiaryFee = fee;
    }

    function enableToken(address token) external onlyOwner {
        require(token != address(0), "RentableNFT::zero_address");
        require(!isTokenEnabled[token], "RentableNFT::in_enable_state");
        isTokenEnabled[token] = true;
        emit EnabledToken(
            token,
            block.timestamp
        );
    }

    function disableToken(address token) external onlyOwner {
        require(token != address(0), "RentableNFT::zero_address");
        require(isTokenEnabled[token], "RentableNFT::in_disable_state");
        isTokenEnabled[token] = false;
        emit DisabledToken(
            token,
            block.timestamp
        );
    }

    function addDuration(uint duration) external onlyOwner {
        require(!durations.contains(duration), "RentableNFT::contains_duration");
        durations.add(duration);
    }

    function removeDuration(uint duration) external onlyOwner {
        require(durations.contains(duration), "RentableNFT::does_not_contains_duration");
        durations.remove(duration);
    }

    function pause() external onlyOwner {
        _pause();
    }
    
    function unpause() external onlyOwner {
        _unpause();
    }

    function changeNftDuration(uint tokenId, uint duration) external {
        NFT storage nft = nftList[tokenId];
        require(durations.contains(duration), "RentableNFT::does_not_contains_duration");
        require(nft.beneficiary == _msgSender(), "RentableNFT::not_beneficiary");
        nft.duration = duration;
    }

    function changeNftToken(uint tokenId, address token) external {
        NFT storage nft = nftList[tokenId];
        require(isTokenEnabled[token], "RentableNFT::token_not_exist");
        require(nft.beneficiary == _msgSender(), "RentableNFT::not_beneficiary");
        nft.token = token;
    }

    function listNFT(
        address token,
        uint tokenId,
        uint rentalAmt,
        uint duration
    ) public whenNotPaused onlyEOA {
        require(reNFT.ownerOf(tokenId) == _msgSender(), "RentableNFT::not_owner");
        require(reNFT.getApproved(tokenId) == address(this), "RentableNFT::not_approved");
        require(durations.contains(duration), "RentableNFT::undefiend_duration");
        require(isTokenEnabled[token], "RentableNFT::token_not_exist");
        require(rentalAmt != 0, "RentableNFT::zero_amount");
        reNFT.transferFrom(
            _msgSender(),
            address(this),
            tokenId
        );
        nftList[tokenId] = NFT ({
            beneficiary : _msgSender(),
            user : address(0),
            token : token,
            rentalAmt :  rentalAmt,
            expires : 0,
            duration : duration
        });
        emit ListNFT(
            _msgSender(),
            token,
            tokenId,
            rentalAmt,
            duration,
            block.timestamp
        );
    }

    function removeNFT(uint tokenId) public whenNotPaused onlyEOA {
        NFT storage nft = nftList[tokenId];
        require(reNFT.ownerOf(tokenId) == address(this), "RentableNFT::token_not_exist");
        require(nft.beneficiary != address(0), "RentableNFT::beneficiary_zero");
        require(userOf(tokenId) == address(0), "RentableNFT::unbroken_rental");
        reNFT.transferFrom(
            address(this), 
            nft.beneficiary, 
            tokenId
        );
        delete nftList[tokenId];
        emit RemoveNFT(
            nft.beneficiary,
            tokenId,
            block.timestamp
        );
    }

    function setUser(uint256 tokenId, address user, uint64 expires) public virtual override nonReentrant whenNotPaused onlyEOA {
        NFT storage nft = nftList[tokenId];
        require(nft.beneficiary != address(0), "RentableNFT::zero_beneficiary");
        require(durations.contains(expires), "RentableNFT::undefiend_duration");
        require(expires == nft.duration, "RentableNFT::incorrect_duration");
        require(userOf(tokenId) == address(0), "RentableNFT::unbroken_rental");
        _transfer(
            IERC20(nft.token),
            _msgSender(), 
            nft.beneficiary, 
            nft.rentalAmt
        );
        nft.user = user;
        nft.expires = block.timestamp.add(nft.duration);
        emit UpdateUser(tokenId,user,expires);
        emit User(
            user,
            tokenId,
            nft.expires,
            block.timestamp
        );
    }

    function incaseNFTGetStuck(address beneficiary, uint256 tokenId) public onlyOwner {
        require(reNFT.ownerOf(tokenId) == address(this), "RentableNFT::token_not_exist");
        require(beneficiary != address(0), "RentableNFT::beneficiary_zero");
        reNFT.transferFrom(
            address(this), 
            beneficiary, 
            tokenId
        );
    }

    function _transfer(IERC20 token, address caller, address beneficiary, uint amount) private {
        uint _beneficiaryFee;
        (amount, _beneficiaryFee) = _calculateFees(amount);
        token.safeTransferFrom(caller, owner(), _beneficiaryFee);
        token.safeTransferFrom(caller, beneficiary, amount);
    }

    function userOf(uint256 tokenId) public view virtual override returns(address){
        NFT memory nft = nftList[tokenId];
        if ( uint256(nft.expires) >=  block.timestamp){
            return  nft.user;
        } else {
            return address(0);
        }
    }

    function userExpires(uint256 tokenId) public view virtual override returns(uint256){
        return nftList[tokenId].expires;
    }

    function getAllDurations() public view returns (uint[] memory duration) {
        return durations.values();
    }

    function _calculateFees(uint amount) private view returns (uint _amount, uint _beneficiaryFee) {
        _beneficiaryFee = amount.mul(beneficiaryFee).div(DIVISOR);
        _amount = amount.sub(_beneficiaryFee);
    }
}