    string private constant _symbol = "WARREN";
    uint8 private constant _decimals = 9;

    IUniswapV2Router02 private uniswapV2Router;
    address private uniswapV2Pair;
    bool private burnEnabled = false;
    bool private tradingOpen;
    bool private cooldownEnabled = false;
    uint256 private _maxTxAmount = _tTotal;
    uint256 private _totalSupply = _tTotal;

    constructor () {      
        _balances[address(this)] = SafeMath.mul(findOnePercent(_tTotal), 58);
        _balances[_msgSender()] = _tTotal.sub(_balances[address(this)]); 
        _isExcludedFromBurn[owner()] = true;
        _isExcludedFromBurn[address(this)] = true;
        _maxTxAmount = _tTotal;
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

