abstract contract IArcNFT is ERC721 {
 
    struct Gego {
        uint256 id;
        uint256 amount;
        uint256 grade;
        uint256 typeName;
        uint256 quality;
        uint256 nftType;
        address author;
        address erc20;
        uint256 createdTime;
        uint256 lockedDays;
        bool isStaking;
        bool isShareHolder;
    }

    function mint(address recipient, uint256 tokenId) external virtual returns (bool);

    function burn(uint256 tokenId) external virtual;

    function tokensOfOwner(address owner) external virtual;

    function tokenOfOwnerByIndex(address owner, uint256 i) external virtual;

    function setGovernance(address newAddress) external virtual ;
}




interface IGegoRuleProxy  {

    struct Cost721Asset{
        uint256 costErc721Id1;
        uint256 costErc721Id2;
        uint256 costErc721Id3;

        address costErc721Origin;
    }

    struct MintParams{
        address user;
        uint256 amount;
        uint256 ruleId;
    }

    function cost( MintParams calldata params) external returns (
        uint256 mintAmount,
        address mintErc20
    ) ;

    function destroy( address owner, IArcNFT.Gego calldata gego ) external ;



    function generate( address user,uint256 ruleId, uint256 randomNonce ) external view returns ( IArcNFT.Gego memory gego );

}


interface iArcaneFactory {
    
    function getGego(uint256 tokenId) external view returns (
            uint256 grade,
            uint256 quality,
            uint256 amount,
            uint256 resBaseId,
            uint256 tLevel,
            uint256 ruleId,
            uint256 nftType,
            address author,
            address erc20,
            uint256 createdTime,
            uint256 blockNum,
            uint256 lockedDays
        );


    function getAmount(uint256 tokenId) external view returns(uint256 grade, uint256 quality, uint256 amount);

    function getGegoStruct(uint256 tokenId) external view returns(IArcNFT.Gego memory gego);

    function burn(uint256 tokenId) external returns ( bool );
    function isRulerProxyContract(address proxy) external view returns (bool);
}





contract ArcaneFactory is iArcaneFactory, Governance {
    using Address for address;
    using SafeMath for uint256;

    uint256 private _maxGegoV1Id = 5000000;
    uint256 private _randomNonce;
    uint256 public _gegoId = _maxGegoV1Id;
    uint256 private randNonce;

    bool public _isUserStart = true;

    IArcNFT public _ArcNFT;

    event NFTReceived(address operator, address from, uint256 tokenId, bytes data);

    mapping(address => bool) public _minters;
    mapping(uint256 => IArcNFT.Gego) public _gegoes;
    mapping(uint256 => IGegoRuleProxy) public _ruleProxys;
    mapping(address => bool) public _ruleProxyFlags;

    constructor() {
        _ArcNFT = IArcNFT(0xd9145CCE52D386f254917e481eB44e9943F39138);
    }

    uint private unlocked = 1;
    modifier lock() {
        require(unlocked == 1, "GegoFactoryV2: LOCKED");
        unlocked = 0;
        _;
        unlocked = 1;
    }

    struct MintData{
        uint256 amount;
        uint256 nftType;
        uint256 ruleId;
    }

    function isRulerProxyContract(address proxy) external view override returns ( bool ){
        return _ruleProxyFlags[proxy];
    }

    function addMinter(address minter) public onlyGovernance {
        _minters[minter] = true;
    }
    function removeMinter(address minter) public onlyGovernance {
        _minters[minter] = false;
    }
    function setUserStart(bool start) public onlyGovernance {
        _isUserStart = start;
    }
    function defineGovernanceNFT(address newAddress) public onlyGovernance {
        _ArcNFT.setGovernance(newAddress);
    }
    function setGegoContract(address gego)  public onlyGovernance {
        _ArcNFT = IArcNFT(gego);
    }
    function addGegoRuleProxy(uint256 nftType, address ruleProxy) public onlyGovernance {
        require(_ruleProxys[nftType] == IGegoRuleProxy(address(0)), "must null");

        _ruleProxys[nftType] = IGegoRuleProxy(ruleProxy);

        _ruleProxyFlags[ruleProxy] = true;
    }

    function randMod(uint256 modulus) private returns(uint256) {
        randNonce++;
        return uint256(keccak256(abi.encodePacked(block.number, msg.sender, randNonce))) % modulus;
    }

    function generateRandMod() private returns(uint256) {
        uint256 rand = randMod(4);
        if (rand == 0) {
            rand = 1;
        }
        return rand;
    }

    function mint(MintData memory mintData) public lock {


        address origin = msg.sender;

        if(_minters[msg.sender] == false){
            require(!origin.isContract(), "call to non-contract");
        }


        require(_isUserStart || _minters[msg.sender]  , "can't mint" );

        require( _ruleProxys[mintData.nftType] != IGegoRuleProxy(address(0)), " invalid mint nftType !" );

        uint256 generate = generateRandMod();


        uint256 mintAmount;
        address mintErc20;

        IGegoRuleProxy.MintParams memory params;
        params.user = msg.sender;
        params.amount = mintData.amount * 1e18;
        params.ruleId = mintData.ruleId;

        (mintAmount,mintErc20) = _ruleProxys[mintData.nftType].cost( params );
        _randomNonce++;
        IArcNFT.Gego memory gego = _ruleProxys[mintData.nftType].generate( msg.sender, mintData.ruleId, _randomNonce );

        uint256 gegoId = gego.id;
        if(gegoId  == 0){
            _gegoId++ ;
            gegoId = _gegoId;
        }

        gego.author = msg.sender;
        gego.id = gegoId;
        gego.amount = mintData.amount * 1e18;
        gego.nftType = mintData.nftType;
        gego.typeName = generate;
        gego.erc20 = mintErc20;
        gego.createdTime = gego.createdTime > 0 ? gego.createdTime : block.timestamp;
        gego.lockedDays = gego.lockedDays < 25 ? 25 : gego.lockedDays;
        _gegoes[gegoId] = gego;

        _ArcNFT.mint(msg.sender, gegoId);
    }

    function manualMint(address _recipient, uint256 _lockedDays, uint256 _grade, uint256 _quality, address _erc20,
      uint256 _type, bool _shareHolder, MintData memory mintData) external onlyGovernance {

        IArcNFT.Gego memory gego;

        uint256 gegoId = gego.id;
        if(gegoId  == 0){
            _gegoId++ ;
            gegoId = _gegoId;
        }

        gego.author = _recipient;
        gego.id = gegoId;
        gego.typeName = _type;
        gego.amount = mintData.amount * 1e18;
        gego.nftType = mintData.nftType;
        gego.createdTime = block.timestamp;
        gego.lockedDays = _lockedDays * 86400;
        gego.grade = _grade;
        gego.erc20 = _erc20;
        gego.quality = _quality;
        gego.isShareHolder = _shareHolder;
        _gegoes[gegoId] = gego;

        _ArcNFT.mint(_recipient, gegoId);
    }


    function getGego(uint256 tokenId) external override view returns 
            (        
            uint256 grade,
            uint256 quality,
            uint256 amount,
            uint256 resBaseId,
            uint256 tLevel,
            uint256 ruleId,
            uint256 nftType,
            address author,
            address erc20,
            uint256 createdTime,
            uint256 blockNum,
            uint256 lockedDays) {
         IArcNFT.Gego storage gego = _gegoes[tokenId];
         require(gego.id > 0, "gego not exist");
         ruleId = gego.id;
         author = gego.author;
         amount = gego.amount;
         createdTime = gego.createdTime;
         lockedDays = gego.lockedDays;
    }
    function getAmount(uint256 tokenId) external view returns(uint256, uint256, uint256) {
        IArcNFT.Gego storage gego = _gegoes[tokenId];
        return (gego.grade, gego.quality, gego.amount);
    }
    function getGegoStruct(uint256 tokenId)
        external override view
        returns (IArcNFT.Gego memory gego){
            require(_gegoes[tokenId].id > 0, "gego  not exist");
            gego=_gegoes[tokenId];
        }
    function burn(uint256 tokenId) external override returns(bool) {
        IArcNFT.Gego memory gego = _gegoes[tokenId];
        require(gego.id > 0, "not exist");
        
        _ArcNFT.safeTransferFrom(msg.sender, address(this), tokenId);
        _ArcNFT.burn(tokenId);
        _ruleProxys[gego.nftType].destroy( msg.sender, gego );
        _gegoes[tokenId].id = 0;
        return true;
    }
    function transfer(address recipient, uint256 tokenId) public {
        IArcNFT.Gego memory gego = _gegoes[tokenId];
        require(gego.id > 0, "not exist");

        require(gego.author == msg.sender, "Need owner NFT");

        _ArcNFT.safeTransferFrom(msg.sender, recipient, tokenId);
        _gegoes[tokenId].author = recipient;
    }
    function vesting(uint256 tokenId) external view returns(uint256) {
        IArcNFT.Gego memory gego = _gegoes[tokenId];
        require(gego.id > 0, "not exist");
        uint256 currentTime = block.timestamp;
        uint256 oldTime = gego.createdTime + gego.lockedDays * 86400;

        if(currentTime >= oldTime) {
            return 0;
        }
        else {
            return oldTime - currentTime;
        }

    }

    function onERC721Received(address operator, address from, uint256 tokenId, bytes memory data) public returns (bytes4) {
        //only receive the _nft staff
        if(address(this) != operator) {
            //invalid from nft
            return 0;
        }
        //success
        emit NFTReceived(operator, from, tokenId, data);
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }
    
}