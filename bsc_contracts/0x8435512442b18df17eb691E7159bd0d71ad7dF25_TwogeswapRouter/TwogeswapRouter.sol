    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[0] == WETH, "Twogeswap: INVALID_PATH");
        amounts = TwogeswapLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= msg.value, "Twogeswap: EXCESSIVE_INPUT_AMOUNT");
        IWETH(WETH).deposit{value: amounts[0]}();
        assert(
