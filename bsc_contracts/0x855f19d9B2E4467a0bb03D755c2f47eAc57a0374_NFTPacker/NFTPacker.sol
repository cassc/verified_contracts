// File contracts/NFTPacker.sol






pragma solidity >=0.4.22 <0.9.0;
pragma abicoder v2; // required to accept structs as function parameters

contract NFTPacker is AccessControl, EIP712 {
    bytes32 public constant ADMIN = keccak256("ADMIN");
    bytes32 public constant MINTER = keccak256("MINTER");
    string private constant SIGNING_DOMAIN = "NFTPacker";
    string private constant SIGNATURE_VERSION = "1";
    bool private enable;
    address payable private commission_wallet;
    uint256 private commission = 0;

    struct Package {
        uint256[] nfts_in_package;
        address collection_address;
        address[] nfts_owner;
        uint64 valid_until;
        address[] address_to_pay;
        address[] token_address_to_pay;
        uint256[] amount_to_pay;
        bytes signature;
    }

    constructor(
        address[] memory allowed_wallets,
        bool enabled,
        address payable wallet_commission
    ) EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION) {
        enable = enabled;
        _setupRole(ADMIN, msg.sender);
        commission_wallet = wallet_commission;
        for (
            uint8 walletIn = 0;
            walletIn < allowed_wallets.length;
            walletIn++
        ) {
            _setupRole(MINTER, allowed_wallets[walletIn]);
        }
    }

    function verifyPackage(Package calldata packageToVerifiy)
        public
        view
        returns (bool result)
    {
        address signer = _verify(packageToVerifiy);
        require(hasRole(MINTER, signer), "Signature invalid or unauthorized");
        require(enable, "Contract is disabled for the moment");
        require(
            verifyOwnersOfNFT(
                packageToVerifiy.nfts_owner,
                packageToVerifiy.nfts_in_package,
                address(packageToVerifiy.collection_address)
            ),
            "One or multiple owners do not have ownership over the requested NFTS."
        );
        require(
            packageToVerifiy.valid_until < block.timestamp,
            "The package has expired"
        );
        return true;
    }

    function allowAccessToNFTCollection(IERC721 nftCollection) public {
        IERC721 nftActualContract = IERC721(nftCollection);
        nftActualContract.setApprovalForAll(address(this), true);
    }

    function redeemPackage(Package calldata packageToRedeem)
        public
        payable
    {
        require(verifyPackage(packageToRedeem));
        for (
            uint payAccount = 0;
            payAccount < packageToRedeem.amount_to_pay.length;
            payAccount++
        ) {
            uint256 amountToPay =   packageToRedeem.amount_to_pay[payAccount];
            address tokenAddress = packageToRedeem.token_address_to_pay[payAccount];
            address payable addresstoPay = payable(packageToRedeem.address_to_pay[payAccount]);

            if(tokenAddress == address(0))
            {       
                addresstoPay.transfer(amountToPay);
                continue;
            }

            IERC20(tokenAddress).transferFrom(msg.sender,addresstoPay,amountToPay);
        }
        uint nftsInPackage = packageToRedeem.nfts_in_package.length;


        for(uint nftPosition =0;nftPosition < nftsInPackage;nftPosition++){
            IERC721(packageToRedeem.collection_address).safeTransferFrom(packageToRedeem.nfts_owner[nftPosition],msg.sender,packageToRedeem.nfts_in_package[nftPosition]);
        }
    }

    function _verify(Package calldata voucher) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }

    function _hash(Package calldata voucher) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "Package(uint256[] nfts_in_package,address collection_address,address[] nfts_owner,uint64 valid_until,address[] address_to_pay,address[] token_address_to_pay,uint256[] amount_to_pay)"
                        ),
                        keccak256(abi.encodePacked(voucher.nfts_in_package)),
                        voucher.collection_address,
                        keccak256(abi.encodePacked(voucher.nfts_owner)),
                        voucher.valid_until,
                        keccak256(abi.encodePacked(voucher.address_to_pay)),
                        keccak256(
                            abi.encodePacked(voucher.token_address_to_pay)
                        ),
                        keccak256(abi.encodePacked(voucher.amount_to_pay))
                    )
                )
            );
    }

    function addAccessToERC20Token(address erc20, uint256 amount) public {
        IERC20 tokenContract = IERC20(erc20);
        tokenContract.approve(address(this), amount);
    }

    function revokeAccessToERC20Token(address erc20) public {
        IERC20 tokenContract = IERC20(erc20);
        tokenContract.approve(address(this), 0);
    }

    function setEnabled(bool enabledStatus) public onlyRole(ADMIN) {
        enable = enabledStatus;
    }

    function revokeAccessToNFTCollection(address nftCollection) public {
        IERC721 nftActualContract = IERC721(nftCollection);
        nftActualContract.setApprovalForAll(address(this), false);
    }

    function verifyOwnersOfNFT(
        address[] calldata owners,
        uint256[] calldata nfts,
        address nftContract
    ) public view returns (bool result) {
        uint16 j;
        address[] memory nfts_owners = new address[](nfts.length);

        IERC721 nftActualContract = IERC721(nftContract);
        for (j = 0; j < nfts.length; j++) {
            nfts_owners[j] = nftActualContract.ownerOf(nfts[j]);
        }

        if (
            keccak256(abi.encodePacked(owners)) ==
            keccak256(abi.encodePacked(nfts_owners))
        ) {
            return true;
        }
        return false;
    }

    function sendFunds() public onlyRole(ADMIN) {
        payable(commission_wallet).transfer(address(this).balance);
    }

    function sendFunds(address token) public onlyRole(ADMIN) {
        IERC20 tokenContract = IERC20(token);
        tokenContract.transfer(
            commission_wallet,
            tokenContract.balanceOf(address(this))
        );
    }

    function setComissionWallet(address payable newAddressWallet)
        public
        onlyRole(ADMIN)
    {
        commission_wallet = newAddressWallet;
    }

    function setRole(bytes32 role, address wallet) public onlyRole(ADMIN) {
        _setupRole(role, wallet);
    }

    function setComission(uint256 newcommission) public onlyRole(ADMIN) {
        require(newcommission < 5);
        commission = newcommission;
    }

    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }
}