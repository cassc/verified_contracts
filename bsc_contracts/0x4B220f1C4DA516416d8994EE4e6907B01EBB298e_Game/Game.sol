    function _getTokenNeedToPayPerAccount(
        uint256 _tokenRewards,
        uint256 _tokenAmount
    ) private pure returns (uint256, uint256) {
        uint256 _cUser = _tokenAmount;

        if (_cUser <= _tokenRewards) {
            return (_cUser, 0);
        }

        _cUser -= _tokenRewards;

        return (_tokenRewards, _cUser);
    }

    function _calcClaimTax(address playerAddress)
        internal
        view
        returns (int128)
    {
        uint256 playerTaxTime = _rewardsClaimTaxTimerStart[playerAddress];

        require(playerTaxTime <= block.timestamp, "Wrong tax time");

        uint256 rewardsClaimTaxTimerEnd = playerTaxTime.add(
            REWARDS_CLAIM_TAX_DURATION
        );

        (, uint256 durationUntilNoTax) = rewardsClaimTaxTimerEnd.trySub(
            block.timestamp
        );

        assert(
            0 <= durationUntilNoTax &&
                durationUntilNoTax <= REWARDS_CLAIM_TAX_DURATION
        );

        int128 frac = ABDKMath64x64.divu(
            durationUntilNoTax,
            REWARDS_CLAIM_TAX_DURATION
        );

        return REWARDS_CLAIM_TAX_MAX.mul(frac);
    }

