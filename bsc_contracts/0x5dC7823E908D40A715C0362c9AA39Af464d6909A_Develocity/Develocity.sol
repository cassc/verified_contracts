    event FeesChanged(uint256 buyFee, uint256 sellFee, bool Status);
    event BurnedTokens(address burnfrom ,uint256 tokens);
    event MaxSellAmountChanged(uint256 OldAmount, uint256 NewAmount);
    event UpdateBotWallet(address BotAddress, bool Status);
    event SwapStatus(bool Status);
    event TransferStatus(bool Status);

    function name() public view virtual returns (string memory) {
        return _name;
    }

    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);

        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(currentAllowance >= amount, "Develocity : transfer amount exceeds allowance");
        _approve(sender, _msgSender(), currentAllowance - amount);


        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender] + addedValue);
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(currentAllowance >= subtractedValue, "Develocity : decreased allowance below zero");
        _approve(_msgSender(), spender, currentAllowance - subtractedValue);

        return true;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) private  {
        require(sender != address(0), "Develocity : transfer from the zero address");
        require(recipient != address(0), "Develocity : transfer to the zero address");

        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount, "Develocity : transfer amount exceeds balance");
        if(amount == 0) {
            return;
        }

        if(!isTransferEnabled && !AMMPairs[sender] && !AMMPairs[recipient]) {
            require( _isExlFFees[sender] || _isExlFFees[recipient] , "Develocity : TRANSFER DISABLED.");
        }

        if(!isSwapEnabled && !_isExlFFees[sender] && !_isExlFFees[recipient]) {
            require(!AMMPairs[sender] && !AMMPairs[recipient] , "Develocity : SWAP DISABLED.");
        }

        if(AMMPairs[recipient] && !_isExlFFees[sender] && !_isExlFFees[recipient]) {
            require(amount <= MaxSell , "Develocity : CHOOSE LESS AMOUNT.");
        }

        if(_isBot[sender]) {
            require(_isExlFFees[recipient], "Develocity : BOT DETECTED.");
        }
        

        takeFee = true;

        if(_isExlFFees[sender] || _isExlFFees[recipient]) {
            takeFee = false;
        }

        if(takeFee && Slip) {
            uint256 Fee;

            if(AMMPairs[recipient] || _isRewardWallet[sender]){
                Fee = amount.mul(SellFee).div(10000);
                _balances[FeeWallet] += Fee;
                _balances[sender] -= Fee;
                emit Transfer(sender, FeeWallet, Fee);
            }else if(AMMPairs[sender]){
                Fee = amount.mul(BuyFee).div(10000);
                _balances[FeeWallet] += Fee;
                _balances[sender] -= Fee;
                emit Transfer(sender, FeeWallet, Fee);
            }
            amount = amount.sub(Fee);
        }

        _beforeTokenTransfer(sender, recipient, amount);

        _balances[sender] -= amount;

        _balances[recipient] += amount;

        emit Transfer(sender, recipient, amount);

        _afterTokenTransfer(sender, recipient, amount);
    }


    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "Develocity : mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);

        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);

        _afterTokenTransfer(address(0), account, amount);
    }

    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "Develocity : burn from the zero address");

        _beforeTokenTransfer(account, address(0), amount);

        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount, "Develocity : burn amount exceeds balance");
         _balances[account] = accountBalance - amount;

        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);

        _afterTokenTransfer(account, address(0), amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "Develocity : approve from the zero address");
        require(spender != address(0), "Develocity : approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}

    function initialize(address _router) public initializer {
        __Context_init_unchained();
        __Ownable_init_unchained();
        DevelocityInit(_router);
    }

    function DevelocityInit(address _router) internal initializer {

        _name = "Develocity";
        _symbol = "DEVE";
        _decimals = 18;
        MaxSell = 1 * (10**5) * (10**18);
        BuyFee  = 300;
        SellFee = 300;
        isSwapEnabled = true;
        isTransferEnabled = true;
        Slip = true;

    	IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_router);

        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;


        setAMMPairs(_uniswapV2Pair, true);

        ExlFromF(owner(), true);
        ExlFromF(address(this), true);
        ExlFromF(address(_uniswapV2Router),true);

        _mint(owner(), 5 * (10**8) * (10**18));
    }

    receive() external payable {
  	}

    function ExlFromF(address account, bool excluded) public onlyOwner{
        _isExlFFees[account] = excluded;

        emit ExcludeFromFees(account , excluded);
    }

    function ExlArrFromF(address [] memory accounts, bool excluded) public onlyOwner{
        for(uint256 i=0;i<accounts.length;i++){
            _isExlFFees[accounts[i]] = excluded;

            emit ExcludeFromFees(accounts[i] , excluded);
        }
    }

    function ChangeMaxSell(uint256 NewAmount) public onlyOwner{
        uint256 OldAmount = MaxSell;
        MaxSell = NewAmount;

        emit MaxSellAmountChanged(OldAmount, NewAmount);
    }

    function ChangeFees(uint256 Buy, uint256 Sell, bool Status) public onlyOwner{
        BuyFee = Buy;
        SellFee = Sell;
        Slip = Status;

        emit FeesChanged(Buy, Sell, Status);
    }

    function GetFees() public view returns(uint256 ,uint256 ,bool){
        return(BuyFee, SellFee, Slip);
    }

    function ChangeFeeWallet(address newFeeWallet) public onlyOwner{
        address oldFeeWallet = FeeWallet;
        FeeWallet = newFeeWallet;

        emit FeeWalletChanged(oldFeeWallet, newFeeWallet);
    }

    function setAMMPairs(address pair, bool value) public onlyOwner{
        require(AMMPairs[pair] != value, "Develocity : Automated market maker pair is already set to that value");
        AMMPairs[pair] = value;

        emit SetAutomatedMarketMakerPair(pair, value);
    }

    function SendBNB(address to, uint256 value) public onlyOwner{
        address payable receiver = payable(to);
        receiver.transfer(value);
    }

    function isBot(address bot, bool value) public onlyOwner{
        _isBot[bot] = value;

        emit UpdateBotWallet(bot, value);     
    }

    function Burn(address account, uint256 amount)public onlyOwner{
        _burn(account, amount);

        emit BurnedTokens(account, amount);
    }

    modifier OwnerOrRewardWallet {
        require(msg.sender == owner() || msg.sender == SellRewardsWallet , "Develocity : You cant ban wallets");
        _;
    }

    function AddRewardWallet(address _wallet) public OwnerOrRewardWallet{
            _isRewardWallet[_wallet] = true;
        }

    function RemoveRewardWallet(address _wallet) public OwnerOrRewardWallet{
            _isRewardWallet[_wallet] = false;
        }

    function lockTransfer() public onlyOwner{
        if(!isTransferEnabled){
            isTransferEnabled = true;
        }else{
            isTransferEnabled = false;
        }
        emit TransferStatus(isTransferEnabled);
    }

    function lockSwap() public onlyOwner{
        if(!isSwapEnabled){
            isSwapEnabled = true;
        }else{
            isSwapEnabled = false;
        }
        emit SwapStatus(isSwapEnabled);     
    }


}