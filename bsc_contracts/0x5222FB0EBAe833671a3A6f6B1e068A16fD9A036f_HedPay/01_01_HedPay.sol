}


contract HedPay is ERC20, Ownable {
    using SafeMath for uint256;

    uint256 public g_buyFee = 0;
    uint256 public g_sellFee = 3;
    uint256 public g_transferFee = 3;
    address public g_devWallet = 0xD72193cCf685D468A3A5BC2af0A9A5845DD13C0B;
    address public g_userWallet = 0x7d4E738477B6e8BaF03c4CB4944446dA690f76B5;
    address public g_liquidityWallet = 0x1573aC99dDb4c3b11d67828c65a503F1906e61C1;
    address public g_dexAddress = 0x0000000000000000000000000000000000000000;
    address public g_cexAddress = 0x0000000000000000000000000000000000000000;
    address public g_interalAddress = 0x0000000000000000000000000000000000000000;
    uint256 public g_minAmount = 0;
    uint256 public g_maxAmount = 0;
    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
    address public g_bscUsdt;

    mapping(address => bool) private _isExcludedFromFees;
    mapping(address => bool) private _isExcludedFromLimit;
    mapping(address => bool) private _isblacklisted;


    constructor(address _addr) ERC20("HEdpAY", "Hdp") {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        g_bscUsdt = _addr;
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), g_bscUsdt);//0x55d398326f99059fF775485246999027B3197955
        uniswapV2Router = _uniswapV2Router;

        _approve(address(this), address(uniswapV2Router), type(uint256).max);

        _isExcludedFromFees[owner()] = true;
        _isExcludedFromFees[address(this)] = true;
        
        _isExcludedFromFees[g_dexAddress] = true;
        _isExcludedFromFees[g_cexAddress] = true;
        _isExcludedFromFees[g_interalAddress] = true;

        _isExcludedFromLimit[owner()] = true;

        g_minAmount = 1000000000000000000; //1 Hdp
        g_maxAmount = 1000000000000000000000000; // 1000000 Hdp

        _mint(owner(), 1000000000 * (10**18));
    }

    receive() external payable {}

    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }

    function setPair(address _pair) public onlyOwner {
        uniswapV2Pair = _pair;
    }

    function excludeFromFees(address account, bool excluded) external onlyOwner {
        require(_isExcludedFromFees[account] != excluded, "Account is already the value of 'excluded'");
        _isExcludedFromFees[account] = excluded;
    }

    function excludeFromLimit(address account, bool excluded) external onlyOwner {
        require(_isExcludedFromLimit[account] != excluded, "Account is already the value of 'exclude'");
        _isExcludedFromLimit[account] = excluded;
    }

    function isExcludedFromFees(address _account) external view returns (bool) {
        return _isExcludedFromFees[_account];
    }

    function isExcludedFromLimit(address _account) external view returns (bool) {
        return _isExcludedFromLimit[_account];
    }

    function blackListAddress(address _account, bool _flag) external onlyOwner {
        _isblacklisted[_account] = _flag;
    }
    function isBlackListed(address _account) external view returns (bool) {
        return _isblacklisted[_account];
    }

    function updateDevWallet(address _newDevWallet) external onlyOwner {
        require(g_devWallet != _newDevWallet, "wallet is already that address");
        require(_newDevWallet != address(0), "wallet cannot be the zero address");
        require(!isContract(_newDevWallet), "wallet cannot be a contract");
        _isExcludedFromFees[g_devWallet] = false;
        _isExcludedFromFees[_newDevWallet] = true;
        g_devWallet = _newDevWallet;

    }

    function updateUserWallet(address _newUserWallet) external onlyOwner {
        require(g_userWallet != _newUserWallet, "wallet is already that address");
        require(_newUserWallet != address(0), "wallet cannot be the zero address");
        require(!isContract(_newUserWallet), "wallet cannot be a contract");
        _isExcludedFromFees[g_userWallet] = false;
        _isExcludedFromFees[_newUserWallet] = true;
        g_userWallet = _newUserWallet;
    }

    function updateLiquidityWallet(address _newLiquidityWallet) external onlyOwner {
        require(g_liquidityWallet != _newLiquidityWallet, "wallet is already that address");
        require(_newLiquidityWallet != address(0), "wallet cannot be the zero address");
        require(!isContract(_newLiquidityWallet), "wallet cannot be a contract");
        _isExcludedFromFees[g_liquidityWallet] = false;
        _isExcludedFromFees[_newLiquidityWallet] = true;
        g_liquidityWallet = _newLiquidityWallet;
    }

    function updatePairAddress(address _addr) external onlyOwner {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_addr);
        uniswapV2Router = _uniswapV2Router;
    }

    function updateDexAddress(address _addr) external onlyOwner {
        g_dexAddress = _addr;
    }

    function updateCexAddress(address _addr) external onlyOwner {
        g_cexAddress = _addr;
    }

    function updateInternalAddress(address _addr) external onlyOwner {
        g_interalAddress = _addr;
    }

    function updateMinAmount(uint256 _amount) external onlyOwner {
        g_minAmount = _amount;
    }

    function updateMaxAmount(uint256 _amount) external onlyOwner {
        g_maxAmount = _amount;
    }

    function updateFees(uint256 _buyFee, uint256 _sellFee, uint256 _transferFee) external onlyOwner {
        g_buyFee = _buyFee;
        g_sellFee = _sellFee;
        g_transferFee = _transferFee;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_isblacklisted[to] && !_isblacklisted[from], "Black listed address!");

        if(!_isExcludedFromLimit[from] && !_isExcludedFromLimit[to]) {
            require(amount >= g_minAmount && amount <= g_maxAmount, "insufficient amount");
        }
        
        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }
        bool m_takeFee = true;
        uint256 fees = 0;
        uint256 feeAmount = 0;

        if(_isExcludedFromFees[from] || _isExcludedFromFees[to])
            m_takeFee = false;

        if (m_takeFee) {
            if (from == uniswapV2Pair) {
                fees = g_buyFee;
            } else if (to == uniswapV2Pair) {
                fees = g_sellFee;
            } else {
                fees = g_transferFee;
            }
        }

        if(fees > 0) {
            feeAmount = (amount.mul(fees)).div(100);
            super._transfer(from, address(this), feeAmount);
            swapAndSendFee(feeAmount);
        }
        super._transfer(from, to, amount - feeAmount);
    }

    function sendUSDT(address payable recipient, uint256 amount) internal {
        uint256 balance = IERC20(g_bscUsdt).balanceOf(address(this));
        require(balance >= amount,"Address: insufficient balance");

        bool success = IERC20(g_bscUsdt).transfer(recipient, amount);
        require(success, "Address: unable to send value, recipient may have reverted");
    }

    function swapAndSendFee(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = g_bscUsdt;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp + 60
        );
        uint256 usdtBalance = IERC20(g_bscUsdt).balanceOf(address(this));
        uint256 devAmount = usdtBalance.div(3);
        uint256 userAmount = usdtBalance.div(3);
        uint256 liquidityAmount = usdtBalance - devAmount - userAmount;

        sendUSDT(payable(g_devWallet), devAmount);
        sendUSDT(payable(g_userWallet), userAmount);
        sendUSDT(payable(g_liquidityWallet), liquidityAmount);
    }

}