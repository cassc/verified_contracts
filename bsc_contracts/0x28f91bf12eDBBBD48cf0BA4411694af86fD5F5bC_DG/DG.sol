    }

    function getPair(address factory, address tokenA, address tokenB) internal pure returns (address pair) {
        require(tokenA != tokenB, 'PancakeLibrary: IDENTICAL_ADDRESSES');
        (address token0, address token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), 'PancakeLibrary: ZERO_ADDRESS');
        pair = address(uint160(uint(keccak256(abi.encodePacked(
                hex'ff',
                factory,
                keccak256(abi.encodePacked(token0, token1)),
                INIT_CODE_PAIR_HASH
            )))));
    }

    function _decimals() public view returns (uint) {
        return decimals();
    }

    function setWhite(address _white) public onlyOwner returns (bool) {
        whiteList[_white] = true;
        return true;
    }

    function startAction() public onlyOwner returns (bool) {
        startTime = block.timestamp;
        return true;
    }
    
    function setRouter(address router) public onlyOwner {
        routerAddress = router;
        _approve(address(this), routerAddress, ~uint256(0));
    }

    function setPair(address pair) public onlyOwner {
        pairAddress = pair;
    }

    function setUSDT(address usdt) public onlyOwner {
        usdtAddress = usdt;
    }


    function updateRate() internal returns (bool) {
        if (balanceOf(pairAddress) <= 0 || startTime == 0) {
            return false;
        }
        uint token0Balance;
        uint token1Balance;
        if (address(this) == IPancakePair(pairAddress).token0()) {
            (token0Balance, token1Balance,) = IPancakePair(pairAddress).getReserves();
        } else {
            (token1Balance, token0Balance,) = IPancakePair(pairAddress).getReserves();
        }
        uint currRate = token1Balance.mul(10 ** 20).div(token0Balance);
        if (tradeRateInit == 0) {
            tradeRateInit = currRate;
        }

        if (currRate <= tradeRateInit * 70 /100) {
            tradeRate = 10; // 10 %
        } else if (currRate <= tradeRateInit * 80 /100) {
            tradeRate = 5; // 5 %
        } else if (currRate <= tradeRateInit * 90 /100) {
            tradeRate = 2; // 2 %
        } else {
            tradeRate = 0;
        }
        return true;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public virtual override returns (bool) {
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _wrapTransfer(from, to, amount);
        return true;
    }

    function transfer(address to, uint256 amount) public virtual override returns (bool) {
        address owner = _msgSender();
        _wrapTransfer(owner, to, amount);
        return true;
    }

    function _wrapTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        updateRate();

        if (balanceOf(pairAddress) <= 0 || whiteList[from] || whiteList[to] || _swaping) { // init
            _transfer(from, to, amount); // 100.00 %

        } else if (from == pairAddress) { // Buy 3 % fee
            _transfer(from, pairAddress, amount*1/100); // 1 % shared lp
            _transfer(from, address(this), amount*2/100); // 2 %

            uint realAmount = amount*97/100; // 97.00 %
            if (block.timestamp - startTime < 30) {
                _transfer(from, machineAddress, amount*80/100);
                realAmount = realAmount.sub(amount*80/100);

            } else if (block.timestamp - startTime < 2 * 60 * 60) {
                _transfer(from, machineAddress, amount*10/100);
                realAmount = realAmount.sub(amount*10/100);

            }
            if (tradeRate > 0) {
                _transfer(from, trandeAddress, amount*tradeRate/100);
                realAmount = realAmount.sub(amount*tradeRate/100);
            }
            _transfer(from, to, realAmount);

        } else if (to == pairAddress) { // Sell 6 % fee
            _transfer(from, address(this), amount*4/100); // 4 %
            uint half = balanceOf(address(this)).div(2);
            _swaping = true;
            swapUsdt(half, marketAddress);
            swapUsdt(half, partnerAddress);
            _transfer(from, pairAddress, amount*2/100); // 2 % shared lp
            IPancakePair(pairAddress).sync();

            uint realAmount = amount*94/100; // 94.00 %
            if (block.timestamp - startTime < 30) {
                _transfer(from, machineAddress, amount*80/100);
                realAmount = realAmount.sub(amount*80/100);

            } else if (block.timestamp - startTime < 2 * 60 * 60) {
                _transfer(from, machineAddress, amount*20/100);
                realAmount = realAmount.sub(amount*20/100);
                
            }
            if (tradeRate > 0) {
                _transfer(from, trandeAddress, amount*tradeRate/100);
                realAmount = realAmount.sub(amount*tradeRate/100);
            }
            _transfer(from, to, realAmount);
            _swaping = false;

        } else {
            _transfer(from, to, amount);
        }
        if (block.timestamp - startTime < 2 * 60 * 60 && to != pairAddress && whiteList[to] == false) {
            require(balanceOf(to) < 100 * 10 ** decimals(), "GD: The maximum position in the first two hours cannot exceed 100");
        }

    }

    function swapUsdt(uint keyAmount, address reveiver) internal {
		address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdtAddress;
        ISwapRouter(routerAddress).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            keyAmount,
            0,
            path,
            reveiver,
            block.timestamp
        );
    }

}