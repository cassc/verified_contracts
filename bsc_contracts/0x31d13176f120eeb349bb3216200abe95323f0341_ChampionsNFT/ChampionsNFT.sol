    function mintAChampion(uint256 championType, uint256 championLevel) public{
        require(saleActive, "Sale is not active");
        require(championType >= 1 && championType <= 5, "There are only 5 types of Champion");
        require(championLevel >= 1 && championLevel <= 3, "There are only 3 levels");
        require(mintedPerTypeAndLevel[championType][championLevel] < levelInfos[championLevel].limit, "Exceed limit of this type & level");

        tokenBFT.transferFrom(msg.sender, address(this), levelInfos[championLevel].price);
        swapNLiquify();

        uint256 nftId = (championType * CHAMPION_TYPE_PRECISION) + championTypeInfos[championType].mintedAmount + 1;

        _safeMint(msg.sender, nftId);

        championInfos[nftId].typeId = championType;
        championInfos[nftId].level = championLevel;
        championTypeInfos[championType].mintedAmount += 1;
        mintedPerTypeAndLevel[championType][championLevel] += 1;
    }

    function buyAChest(uint256 tier) public returns (uint256 requestId){
        require(saleActive, "Sale is not active");
        require(tier >= 1 && tier <= 3, "There are 3 tiers 1->3");
        require(chestInfos[tier].soldAmount < chestInfos[tier].limit, "Sold out that chest tier");

        tokenBFT.transferFrom(msg.sender, address(this), chestInfos[tier].price);
        swapNLiquify();

        chestInfos[tier].soldAmount += 1;

        // Request Chainlink vrf
        requestId = requestRandomness(callbackGasLimit, requestConfirmations, numWords);
        vrfRequests[requestId] = RequestStatus({
            sender: msg.sender,
            chestTier: tier,
            fulfilled: false
        });

        vrfRequestIds.push(requestId);
        vrfLastRequestId = requestId;
        emit BuyRequestSent(tier, msg.sender);
        return requestId;
    }


    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        require(vrfRequests[_requestId].sender != address(0), 'request not found');

        vrfRequests[_requestId].fulfilled = true;
        uint256 chestTier = vrfRequests[_requestId].chestTier;
        uint256 championType = ((_randomWords[0] % 5) + 1); // 5 type, 1->5

        uint256 level = 1;
        uint randomRange = ((_randomWords[0] % 100) + 1);
        if(chestTier == 1){
            if(randomRange > 95) // 5% got lvl 2
                level  = 2;                
        }else if (chestTier == 2){
            if(randomRange > 90) // 10% got lvl 3
                level = 3;
            else if (randomRange > 20 && randomRange <= 90) // 70% got lvl 2
                level = 2;
            // 20 % got lvl 1
        }else if (chestTier == 3){
            if(randomRange > 66) // 33 % got lvl 3
                level = 3;
            else  // 66% got lvl 2 
                level = 2;
        }

        uint256 nftId = (championType * CHAMPION_TYPE_PRECISION) + championTypeInfos[championType].mintedAmount + 1;

        _safeMint(vrfRequests[_requestId].sender, nftId);

        championInfos[nftId].typeId = championType;
        championInfos[nftId].level = level;
        championTypeInfos[championType].mintedAmount += 1;
        mintedPerTypeAndLevel[championType][level] += 1;
        emit BuyRequestFulfilled(championType, level, vrfRequests[_requestId].sender);
    }

