    function sqrt(uint y) internal pure returns (uint z) {
        if (y > 3) {
            z = y;
            uint x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    function mintTokens(uint256 _amount, address _wallet) external onlyOwner {
        // Mint tokens to msg.sender
        _mint(_wallet, _amount);
    }

    function burnTokens(uint256 _amount, address _wallet) external onlyOwner {
        // Burn tokens for msg.sender
        _burn(_wallet, _amount);
    }
}


// import "./utilities/TransferHelper.sol";
// import "./interfaces/IPancakeRouter.sol";
// import "./interfaces/IERC20.sol";
// import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
// import "@openzeppelin/contracts/utils/math/SafeMath.sol";
// import "@openzeppelin/contracts/access/Ownable.sol";
// import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract BabyAptosStakingPool is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    // Track accepted NFTs
    EnumerableSet.AddressSet private acceptedNFTs;

    // Track NFT boost
    mapping(address => uint256) nftYield;

    struct UserInfo {
        EnumerableSet.AddressSet stakedNFTs;

        // Mapping contract address to array of tokenIds
        mapping(address => uint256[]) stakesForNFT;
    }

    // Track NFT users
    mapping(address => UserInfo) private users;

    // Track last block claimed for user
    mapping(address => uint256) public lastClaimed;

    // Track earnings
    mapping(address => uint256) public totalEarned;

    // Track stats
    uint256 public totalPurchased;

    // Track reserves
    uint256 public totalReserves;

    // Define stakable/reward tokens
    address public tokenToBuy;
    address public tokenToSell;

    // Router informaton
    IPancakeRouter02 private _pancakeRouter;

    // BAPT Token
    DividendPayingTokenInterface private babyAptos;

    // Provider Token
    ProviderToken private pBAPT;

    // Purchase fee to reserves
    uint256 public reserveFee;

    // Fee to withdraw staked tokens (value out of 1000; i.e. 50 = 5%)
    uint256 public withdrawFee;

    uint256 public promoBonus;

    /* Bounty paid to wallet that triggers swap (value out of 1000; i.e. 50 = 5%)
       Incentivizes users to trigger swap if enough rewards have accumulated which
       allows swaps to remain consistent.
    */
    uint256 public triggerBounty;

    /* Events */
    event onSwap(address triggerer, uint256 amount);

    // Settings for staking RippleBurnReflect with XRP rewards
    constructor() {
        // BabyAptos
        tokenToBuy = 0xe4c6dD06da4c0934d265d6CB6875E576485CF332;
        // Default: BUSD
        tokenToSell = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;

        // PancakeSwap v2 Router
        _pancakeRouter = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        babyAptos = DividendPayingTokenInterface(0xe4c6dD06da4c0934d265d6CB6875E576485CF332);

        withdrawFee = 0; // 0%
        triggerBounty = 5; // 0.5%
        reserveFee = 100; // 10%

        promoBonus = 1000;

        // Initialize provider token
        pBAPT = new ProviderToken("ProviderBAPT", "pBAPT");
    }

    /* Internal Helper Functions */
    function sqrt(uint y) internal pure returns (uint z) {
        if (y > 3) {
            z = y;
            uint x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    function rewardsInContract() internal view returns(uint256) {
        return IERC20(tokenToSell).balanceOf(address(this));
    }

    function getYieldRate(address _user) internal view returns(uint256) {
        UserInfo storage user = users[_user];

        uint256 totalYieldRate = 0;

        for(uint i = 0; i < user.stakedNFTs.length(); i++) {
            address _contract = user.stakedNFTs.at(i);

            uint256 numStaked = user.stakesForNFT[_contract].length;

            totalYieldRate = totalYieldRate.add(nftYield[_contract].mul(numStaked));
        }

        // Check and add promo bonus
        if (user.stakedNFTs.length() == 4) {
            totalYieldRate = totalYieldRate.add(promoBonus);
        }

        return totalYieldRate;
    }

    function claimableRewards(address _wallet) internal view returns(uint256) {
        // Claimable rewards = (28000 * 5 / 100000 / 100)
        uint256 currentBlock = block.number;
        uint256 prevBlock = lastClaimed[_wallet];
        uint256 blocksPassed = currentBlock.sub(prevBlock);

        uint256 rewards = 0;

        // Get yield rate
        uint256 rate = getYieldRate(_wallet);

        // iterate through nfts
        // rewards = (nft_rate * (10 ** 18)) * blocksPassed / 28800
        rewards = rewards.add((rate).mul(10 ** 18).mul(blocksPassed).div(28800));

        return rewards;
    }

    /* External Helper Functions */
    function getContractRewards() external view returns(uint256) {
        return IERC20(tokenToSell).balanceOf(address(this));
    }

    function getStakeForUser(address _user) external view returns(uint256) {
        uint256 _amount = pBAPT.balanceOf(_user);
        uint256 pBAPTValue = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves).mul(_amount).div(pBAPT.totalSupply());

        return pBAPTValue;
    }

    function getHarvestableRewards(address _user) external view returns(uint256) {
        return claimableRewards(_user);
    }

    function getYieldRateForUser(address _user) external view returns(uint256) {
        return getYieldRate(_user);
    }

    function getRateForNFT(address _contract) external view returns(uint256) {
        return (nftYield[_contract]);
    }

    function isAcceptedNFT(address _contract) external view returns(bool) {
        return acceptedNFTs.contains(_contract);
    }

    function userHasNFTStaked(address _contract, address _user) external view returns(bool) {
        UserInfo storage user = users[_user];

        return user.stakedNFTs.contains(_contract);
    }

    function getNFTStakeAtIndex(address _contract, address _user, uint256 _index) external view returns(uint256) {
        UserInfo storage user = users[_user];

        return user.stakesForNFT[_contract][_index];
    }

    function getNumStakedNFTForUser(address _contract, address _user) external view returns(uint256) {
        UserInfo storage user = users[_user];

        return user.stakesForNFT[_contract].length;
    }

    function setWithdrawFee(uint256 _withdrawFee) external onlyOwner {
        require(_withdrawFee < 50, "Withdraw fee cannot exceed 5%!");

        withdrawFee = _withdrawFee;
    }

    function setTriggerBounty(uint256 _triggerBounty) external onlyOwner {
        require(_triggerBounty < 15, "Trigger bounty cannot exceed 1.5%!");

        triggerBounty = _triggerBounty;
    }

    function setReserveFee(uint256 _reserveFee) external onlyOwner {
        require(_reserveFee < 200, "Reserve fee cannot be greater than 20%!");

        reserveFee = _reserveFee;
    }

    function setPromoBonus(uint256 _amount) external onlyOwner {
        require(_amount <= 5000, "Bonus cannot be greater than 5000.");

        promoBonus = _amount;
    }

    function setTokenToSell(address _token) external onlyOwner {
        tokenToSell = _token;
    }

    function setTokenToBuy(address _token) external onlyOwner {
        tokenToBuy = _token;
    }

    function addReserves(uint256 _amount) external {
        // Take snapshot of contract balance before transfer.
        uint256 initialBalance = IERC20(tokenToBuy).balanceOf(address(this));

        // Transfer tokens to contract.
        TransferHelper.safeTransferFrom(tokenToBuy, address(msg.sender), address(this), _amount);

        // Take snapshot of contract balance after transfer to account for transfer tax.
        uint256 newBalance = IERC20(tokenToBuy).balanceOf(address(this));

        // Add to reserves
        totalReserves = totalReserves.add(newBalance.sub(initialBalance));
    }

    function withdrawReserves(uint256 _amount) external onlyOwner {
        require(_amount <= totalReserves, "Cannot withdraw more than reserves");

        // Transfer tokens to owner.
        TransferHelper.safeTransfer(tokenToBuy, address(msg.sender), _amount);

        // Subtract from total reserves
        totalReserves = totalReserves.sub(_amount);
    }

    function harvestRewards() external {
        require(claimableRewards(msg.sender) > 0, "Nothing to harvest!");

        uint256 pendingRewards = claimableRewards(msg.sender);

        uint256 initialBalance = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves);

        // Subtract from reserves
        totalReserves = totalReserves.sub(pendingRewards);

        // Track total earnings
        totalEarned[msg.sender] = totalEarned[msg.sender].add(pendingRewards);

        // Calculate tokens to mint
        // If there is no initial supply, mint the provider tokens from the sqrt
        // of the provided tokens. Otherwise, mint proportionally.
        uint256 tokensToMint = pendingRewards.mul(pBAPT.totalSupply()).div(initialBalance);

        // Mint pBAPT tokens
        pBAPT.mintTokens(tokensToMint, msg.sender);

        // Update last claimed block
        lastClaimed[msg.sender] = block.number;
    }

    function harvestRewards(address _user) internal {
        require(claimableRewards(_user) > 0, "Nothing to harvest!");

        uint256 pendingRewards = claimableRewards(_user);

        uint256 initialBalance = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves);

        // Subtract from reserves
        totalReserves = totalReserves.sub(pendingRewards);

        // Track total earnings
        totalEarned[_user] = totalEarned[_user].add(pendingRewards);

        // Calculate tokens to mint
        // If there is no initial supply, mint the provider tokens from the sqrt
        // of the provided tokens. Otherwise, mint proportionally.
        uint256 tokensToMint = pendingRewards.mul(pBAPT.totalSupply()).div(initialBalance);

        // Mint pBAPT tokens
        pBAPT.mintTokens(tokensToMint, _user);

        // Update last claimed block
        lastClaimed[_user] = block.number;
    }

    // Claim pending rewards for the pool
    function claimRewards() external {
        babyAptos.claim();
    }

    // Add a new NFT contract and configure fee structure
    function addAcceptedNFT(address _contract, uint256 _yieldRate) external onlyOwner {
        // Record metadata and NFT contract
        nftYield[_contract] = _yieldRate;
        acceptedNFTs.add(_contract);
    }

    // Remove NFT contract from list of accepted NFTs
    function removeAcceptedNFT(address _contract) external onlyOwner {
        require(acceptedNFTs.contains(_contract), "NFT not accepted!");

        acceptedNFTs.remove(_contract);
        nftYield[_contract] = 0;
    }

    function stakeNFT(address _contract, uint256 _tokenId) external {
        require(acceptedNFTs.contains(_contract), "NFT not accepted!");
        require(IERC721(_contract).ownerOf(_tokenId) == msg.sender, "Not token owner!");

        // Harvest pending rewards
        if (claimableRewards(msg.sender) > 0) {
            harvestRewards(msg.sender);
        }

        // Mark block number
        lastClaimed[msg.sender] = block.number;

        // Transfer NFT
        IERC721(_contract).transferFrom(msg.sender, address(this), _tokenId);

        // Track stake for user
        UserInfo storage user = users[msg.sender];
        user.stakedNFTs.add(_contract);
        uint256[] storage user_stakes = user.stakesForNFT[_contract];
        user_stakes.push(_tokenId);
    }

    function withdrawNFT(address _contract, uint256 _index) external {
        // Gather user info
        UserInfo storage user = users[msg.sender];

        require(user.stakedNFTs.contains(_contract), "No NFT staked!");

        // Gather stake info for nft contract
        uint256[] storage user_stakes = user.stakesForNFT[_contract];

        require(_index < user_stakes.length, "Out of index!");

        uint256 _tokenId = user_stakes[_index];

        // Harvest pending rewards
        if (claimableRewards(msg.sender) > 0) {
            harvestRewards(msg.sender);
        }

        // Mark block number
        lastClaimed[msg.sender] = block.number;

        // Transfer NFT
        IERC721(_contract).transferFrom(address(this), msg.sender, _tokenId);

        // Remove NFT from user
        user_stakes[_index] = user_stakes[user_stakes.length-1];
        user_stakes.pop();

        if (user_stakes.length == 0) {
            user.stakedNFTs.remove(_contract);
        }
    }

    /* Function to convert sellable tokens in contract
       into the main staking token.
       
       i.e. Swap any XRP held in contract to RippleBurnReflect

       The `tokenToSell` value can be changed by owner in order
       to support selling any tokens sent to the contract and
       distributing the proceeds among the current stakers.
    */
    function swapTokens() external {
        uint256 balanceInContract = rewardsInContract();

        require(balanceInContract > 0, "Contract holds no swappable tokens!");

        // If trigger bounty is enabled, pay msg.sender.
        if (triggerBounty > 0) {
            uint256 bountyPaid = balanceInContract.mul(triggerBounty).div(1000);

            // Send remaining tokens to msg.sender.
            TransferHelper.safeTransfer(tokenToSell, msg.sender, bountyPaid);
        }

        // Recalculate balance after bounty.
        balanceInContract = rewardsInContract();

        // Approve router to swap tokens.
        IERC20(tokenToSell).approve(
            address(_pancakeRouter),
            balanceInContract
        );

        // Initialize router path through BNB (i.e. XRP > WBNB > RBR)
        address[] memory path = new address[](3);
        path[0] = address(tokenToSell);
        path[1] = _pancakeRouter.WETH();
        path[2] = address(tokenToBuy);

        // Record contract balance before swap
        uint256 initialBalance = IERC20(tokenToBuy).balanceOf(address(this));

        /* Trigger swap through PancakeSwap router.
           This call allows for a token tax of up to
           20% on a contract. Anything higher will
           fail.
        */
        _pancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            balanceInContract,
            0,
            path,
            address(this),
            (block.timestamp + 1 hours)
        );

        // Record new balance after swap
        uint256 newBalance = IERC20(tokenToBuy).balanceOf(address(this));   

        // Calculate difference to get exact number of tokens purchased.
        uint256 purchasedTokens = newBalance.sub(initialBalance);

        if (reserveFee > 0) {
            totalReserves = totalReserves.add(purchasedTokens.mul(reserveFee).div(1000));
        }

        totalPurchased = totalPurchased.add(purchasedTokens);

        emit onSwap(
            msg.sender,
            purchasedTokens
        );
    }

    // Function to stake specified tokens in the pool (i.e. RippleBurnReflect)
    function depositStake(uint256 _amount) external payable nonReentrant {
        require(_amount > 0, "Cannot stake less than 0 tokens");

        // Take snapshot of contract balance before transfer.
        uint256 initialBalance = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves);

        // Transfer tokens to contract.
        TransferHelper.safeTransferFrom(tokenToBuy, address(msg.sender), address(this), _amount);

        // Take snapshot of contract balance after transfer to account for transfer tax.
        uint256 newBalance = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves);

        uint256 stakedTokens = newBalance.sub(initialBalance);

        // Calculate tokens to mint
        // If there is no initial supply, mint the provider tokens from the sqrt
        // of the provided tokens. Otherwise, mint proportionally.
        if (pBAPT.totalSupply() == 0) {
            uint256 tokensToMint = sqrt(stakedTokens);

            pBAPT.mintTokens(tokensToMint, msg.sender);
        } else {
            uint256 tokensToMint = stakedTokens.mul(pBAPT.totalSupply()).div(initialBalance);

            pBAPT.mintTokens(tokensToMint, msg.sender);
        }
    }

    // Function to withdraw any staked and accumulated tokens from the pool.
    function withdrawStake(uint256 _amount) external nonReentrant {
        require(_amount > 0, "Must withdraw more than zero!");

        // Calculate tokens to burn and tokens to send to msg.sender.
        uint256 pBAPTValue = IERC20(tokenToBuy).balanceOf(address(this)).sub(totalReserves).mul(_amount).div(pBAPT.totalSupply());
        
        uint256 amountToReserves = 0;
        
        if (withdrawFee > 0) {
            amountToReserves = pBAPTValue.mul(withdrawFee).div(1000);
        }

        uint256 amountToWithdraw = pBAPTValue.sub(amountToReserves);

        // Only trigger if any tokens will be burned.
        if (amountToReserves > 0) {
            // Increment reserves
            totalReserves = totalReserves.add(amountToReserves);
        }

        // Send remaining tokens to msg.sender.
        TransferHelper.safeTransfer(tokenToBuy, msg.sender, amountToWithdraw);

        // Burn pBAPT
        pBAPT.burnTokens(_amount, msg.sender);
    }

}