        return _name;
    }

    function symbol() public pure returns (string memory) {
        return _symbol;
    }

    function decimals() public pure returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }


    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    function setCooldownEnabled(bool onoff) external onlyOwner {
        cooldownEnabled = onoff;
    }

    function setBurnEnabled(bool onoff) external onlyOwner {
        burnEnabled = onoff;
    }

    function setMaxTxAmount(uint256 amount) external onlyOwner {
        _maxTxAmount = amount;
    }

    function setBurnExcludeList(address[] memory accounts_) public onlyOwner {
        for (uint i = 0; i < accounts_.length; i++) {
            if (_isExcludedFromBurn[accounts_[i]] == false) _isExcludedFromBurn[accounts_[i]] = true;
        }
    }

    function excludeAccountFromBurn(address account) external onlyOwner {
        require(!_isExcludedFromBurn[account], "Account is already excluded from Burns");
        _isExcludedFromBurn[account] = true;
    }

    function includeAccountInBurn(address account) external onlyOwner {
        require(_isExcludedFromBurn[account], "Account is already included for Burns");
        _isExcludedFromBurn[account] = false;
    }

    function setBurnPercent(uint256 _burnPercent) external onlyOwner {
        burnPercent = _burnPercent;
    }


