// File: PriceOracle.sol

contract PriceOracle {
    using SafeMath for uint256;

    /* ========== STATE VARIABLES ========== */

    // uniswap
    address public token0;
    address public token1;
    IUniswapV2Pair public pair;

    constructor(
        IUniswapV2Pair _pair
    ) public {
        pair = _pair;
        token0 = pair.token0();
        token1 = pair.token1();
    }

    function consult(address _token, uint256 _amountIn) external view returns (uint144 amountOut) {
        require(_amountIn > 0, 'PriceOracle: INSUFFICIENT_AMOUNT');

        (uint reserve0, uint reserve1,) = pair.getReserves();
        require(reserve0 > 0 && reserve1 > 0, 'PriceOracle: INSUFFICIENT_LIQUIDITY');


        if (_token == token0) {
            amountOut = uint144(_amountIn.mul(reserve1).div(reserve0));
        } else {
            require(_token == token1, "PriceOracle: INVALID_TOKEN");
            amountOut = uint144(_amountIn.mul(reserve0).div(reserve1));
        }
    }

}