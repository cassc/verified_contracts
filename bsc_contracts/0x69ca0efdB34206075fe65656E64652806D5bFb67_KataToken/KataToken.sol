        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        uint256 taxFee = 0;
        uint256 burnFee = 0;
        address toAddress1=address(this);
        if (from == uniswapV2Pair) {
            // buy
            taxFee = buyFee;
            burnFee = buyBurn;
            toAddress1 = lpAddress;
        } else if (to == uniswapV2Pair) {
            // sell
            taxFee = sellFee; 
            burnFee = sellBurn;
            toAddress1=nftFundAddress;        
        }
        uint256 feeAmount = amount.mul(taxFee).div(denominator);
        uint256 burnAmount = amount.mul(burnFee).div(denominator);
        uint256 toAmount = amount.sub(feeAmount.add(burnAmount));

        // from 
        _balances[from] = _balances[from].sub(amount);

        // to 1
        _balances[toAddress1].add(feeAmount);
        emit Transfer(from, toAddress1, feeAmount);
        // to 2
        _balances[deadAddress].add(burnAmount);
        emit Transfer(from, deadAddress, burnAmount);
        // to 3
        _balances[to].add(toAmount);  
        emit Transfer(from, to, toAmount); 
        
    }

