pragma solidity ^0.8.4;
// pragma experimental ABIEncoderV2;













contract LaunchpadAgent is ReentrancyGuard,Ownable {
 
    using ECDSA for bytes32;
    using Address for address;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.Bytes32Set;
    using EnumerableMap for EnumerableMap.AddressToUintMap;

    //event mint
    event eMint(
        address erc721,
        address user,
        address costErc20,
        uint256 costAmount,
        uint256 stageCount,
        uint256 stageSoldAmount,
        uint256 mintCount,
        uint8 stage
    );

    struct ProjcetInfo{
        address costErc20;
        uint256 saleAmount;
        uint256 withdrawAmount;
        bool isUserStart;
    }

    struct Condition {
        uint256 price;          //nft per cost erc20
        uint256 startTime;      //the stage start time
        uint256 endTime;        //the stage end time
        uint256 limitCount;     //a quota
        uint256 maxSoldAmount;  //the stage max sold amount
        bytes32 signCode;       //signCode
        uint8  stage;           //cur stage
        address erc721;         //the nft contract address
        bytes wlSignature;      //enable white
    }

    struct EIP712Domain {
        string  name;
        string  version;
        uint256 chainId;
        address verifyingContract;
    }

    bytes32 public immutable DOMAIN_SEPARATOR;

    bytes32 public constant EIP712DOMAIN_TYPEHASH = keccak256(
        "EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"
    );

    //type hash
    bytes32 public constant TYPE_HASH = keccak256(
        "Condition(uint256 price,uint256 startTime,uint256 endTime,uint256 limitCount,uint256 maxSoldAmount,bytes32 signCode,uint8 stage,address erc721,bytes wlSignature)"
    );


    // launchpad nft project info
    mapping(address => ProjcetInfo ) public _projcetInfo; //erc721->ProjcetInfo

    // super minters
    // EnumerableSet.AddressSet private _superMinters; //erc721->superMinters
    mapping(address=>bool) _superMinters;

    // tags show address can join in open sale
    mapping(address => EnumerableSet.Bytes32Set) private _signCodes;//erc721->signCode

    // the user get fund per stage
    mapping(address => mapping (uint8 =>  EnumerableMap.AddressToUintMap )) private _stageCount;//erc721->stageCount

    // the stage had sold amount
    mapping(address => mapping (uint8 => uint256) ) public _stageSoldAmount; //erc721->stageSoldAmount

    address private SIGNER;

    address public  _WETH;

    constructor(address WETH) {

        _WETH = WETH;
        SIGNER = msg.sender;

        DOMAIN_SEPARATOR = keccak256(
            abi.encode(
                EIP712DOMAIN_TYPEHASH,
                keccak256("LaunchpadAgent"),
                keccak256("1"),
                block.chainid,
                address(this)
            )
        );

        addSuperMinter(msg.sender);
        
    }

    function setProject(address erc721, address costErc20) public {
        require( _superMinters[msg.sender], "limits of authority");
        _projcetInfo[erc721].costErc20 = costErc20;    
    }

    function setUserStart(address erc721, bool start) public  {
        require(_superMinters[msg.sender], "limits of authority");
        _projcetInfo[erc721].isUserStart = start;
    }

    function addSuperMinter(address minter) public onlyOwner {
        _superMinters[minter] = true;
    }

    function removeSuperMinter(address minter) public onlyOwner {
       _superMinters[minter] = false;
    }

    function isValidSignCode(address erc721,bytes32 signCode) view public returns(bool) {
        return !_signCodes[erc721].contains(signCode);
    }

    function getStageCount(address erc721, uint8 stage, address user) view public returns(uint256) {
        bool have;
        uint256 stageCount;
        (have,stageCount) = _stageCount[erc721][stage].tryGet(user);
        return stageCount;
    }

    // get ProjcetInfo
    function getProjectInfo(address erc721) view public returns( ProjcetInfo memory ) {
        return _projcetInfo[erc721];
    }


    function mint721WithETH(uint256 mintCount,  Condition calldata condition, bytes memory dataSignature) public payable
    {
        address erc721 = condition.erc721;
        require(_projcetInfo[erc721].costErc20 == address(0x0), "invalid mint method!" );

        uint256 costAmount = condition.price.mul(mintCount);
        if(costAmount > 0){

            require(msg.value >= costAmount, "invalid cost amount! ");

            IWETH(_WETH).deposit{value: msg.value}();
            IERC20(_WETH).safeTransfer(address(this), msg.value);

            _projcetInfo[erc721].saleAmount+=costAmount;
        }
      
        _mint721(erc721,mintCount,condition,dataSignature);
            
    }

    // mint721 asset
    function mint721(uint256 mintCount, Condition calldata condition, bytes memory dataSignature )  public {

        address erc721 = condition.erc721;
        require(_projcetInfo[erc721].costErc20 != address(0x0),"invalid cost token address!");

        uint256 costAmount = condition.price.mul(mintCount);
        if(costAmount > 0){

            IERC20 costErc20 =  IERC20(_projcetInfo[erc721].costErc20);
            uint256 balanceBefore = costErc20.balanceOf(address(this));
            costErc20.safeTransferFrom(msg.sender, address(this), costAmount);
            uint256 balanceAfter = costErc20.balanceOf(address(this));

            _projcetInfo[erc721].saleAmount+=balanceAfter.sub(balanceBefore);
            
        }

        _mint721(erc721,mintCount,condition,dataSignature);

    } 


    // mint721 asset
    function _mint721(address erc721, uint256 mintCount, Condition calldata condition, bytes memory dataSignature )  public {

        require(mintCount>0, "invalid mint count!");
        if(!_superMinters[msg.sender]){
            require(!msg.sender.isContract(), "call to non-contract");
        }
        require(_projcetInfo[erc721].isUserStart || _superMinters[msg.sender]  , "can't mint" );
        require( block.timestamp >= condition.startTime && block.timestamp < condition.endTime, "out date" );

        uint8 stage = condition.stage;
        require(stage!=0,"stage not begin");

        bool have ;
        uint256 stageCount;
        (have,stageCount)= _stageCount[erc721][stage].tryGet(msg.sender);

        if(!_superMinters[msg.sender]){

            require(verify(condition, msg.sender, dataSignature), "this sign is not valid");
            require(stageCount + mintCount <= condition.limitCount,"stage count is max ");

            //once signCode
            require(!_signCodes[erc721].contains(condition.signCode),"invalid signCode!");
        }

        uint256 costAmount = condition.price.mul(mintCount);
        if(costAmount > 0){
            stageCount += mintCount;
            _stageCount[erc721][stage].set(msg.sender,stageCount);
            _stageSoldAmount[erc721][stage] += mintCount;
        }

        ILaunchpad721(erc721).batchMint(msg.sender,mintCount);
        _signCodes[erc721].add(condition.signCode);

        emit eMint(
                erc721,
                msg.sender,
                _projcetInfo[erc721].costErc20,
                costAmount,
                stageCount,
                _stageSoldAmount[erc721][stage],
                mintCount,
                stage
            );
    } 

    function withdrawETH(address wallet) external onlyOwner {
        payable(wallet).transfer(address(this).balance);
    }

    function withdrawMoney(address erc721, address wallet) external onlyOwner {
        IERC20 costErc20 =  IERC20(_projcetInfo[erc721].costErc20);
        uint256 amount = _projcetInfo[erc721].saleAmount.sub(_projcetInfo[erc721].withdrawAmount);
        costErc20.safeTransfer(wallet, amount);
        _projcetInfo[erc721].withdrawAmount += amount;
    }

    function urgencyWithdraw(address erc20, address wallet) external onlyOwner {
        IERC20(erc20).safeTransfer(wallet, IERC20(erc20).balanceOf(address(this)));
    }

    function updateSigner( address signer) external onlyOwner {
        SIGNER = signer;
    }

    function hashCondition(Condition calldata condition) public pure returns (bytes32) {

        // struct Condition {
        //     uint256 price;          //nft per cost erc20
        //     uint256 startTime;      //the stage start time
        //     uint256 endTime;        //the stage end time
        //     uint256 limitCount;     //a quota
        //     uint256 maxSoldAmount;  //the stage max sold amount
        //     bytes32 signCode;       //signCode
        //     uint8  stage;           //cur stage
        //     address erc721;         //the nft contract address
        //     bytes wlSignature;      //enable white
        // }

        return keccak256(
            abi.encode(
                TYPE_HASH,
                condition.price,
                condition.startTime,
                condition.endTime,
                condition.limitCount,
                condition.maxSoldAmount,
                condition.signCode,
                condition.stage,
                condition.erc721,
                keccak256(condition.wlSignature))
        );
    }

    function hashWhiteList( address user, bytes32 signCode ) public pure returns (bytes32) {

        bytes32 message = keccak256(abi.encodePacked(user, signCode));
        // hash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", message));
        return message.toEthSignedMessageHash();
    }

    function hashDigest(Condition calldata condition) public view returns (bytes32) {
        return keccak256(abi.encodePacked(
            "\x19\x01",
            DOMAIN_SEPARATOR,
            hashCondition(condition)
        ));
    }

    function verifySignature(bytes32 hash, bytes memory  signature) public view returns (bool) {
        //hash must be a soliditySha3 with accounts.sign
        return hash.recover(signature) == SIGNER;
    }

    function verifyCondition(Condition calldata condition, uint8 v, bytes32 r, bytes32 s) public view returns (bool) {
        bytes32 digest = hashDigest(condition);
        return ecrecover(digest, v, r, s) == SIGNER;    
    }

    function verify(  Condition calldata condition, address user, bytes memory dataSignature ) public view returns (bool) {
       
        require(condition.signCode != "","invalid sign code!");

        bytes32 digest = hashDigest(condition);
        require(verifySignature(digest,dataSignature)," invalid dataSignatures! ");

        if(condition.wlSignature.length >0 ){
            bytes32 hash = hashWhiteList(user, condition.signCode);
            require( verifySignature(hash, condition.wlSignature), "invalid wlSignature! ");
        }

        return true;
    }

}