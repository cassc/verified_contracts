        bytes memory returndata = address(token).functionCall(data, "SafeERC20: low-level call failed");
        if (returndata.length > 0) {
            // Return data is optional
            require(abi.decode(returndata, (bool)), "SafeERC20: ERC20 operation did not succeed");
        }
    }
}


interface IUniswapV2Factory {

    function createPair(address tokenA, address tokenB) external returns (address pair);
}

interface IUniswapV2Pair {
  
    function factory() external view returns (address);
}

interface IUniswapV2Router {
    function factory() external pure returns (address);

    function WETH() external pure returns (address);

    function getAmountsOut(uint amountIn, address[] memory path) external view returns (uint[] memory amounts);

    function addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    )
        external
        returns (
            uint256 amountA,
            uint256 amountB,
            uint256 liquidity
        );

    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external;

}

interface INeverFail{
    function buy(uint256 amountU) external returns(uint256);
}

contract PairTempAddress{
    address token;
    address target;
    constructor(address _token,address _target){
        token = _token;
        target = _target;
        IERC20(token).approve(target, type(uint256).max);
    }
        
    function reApprove() public{
        IERC20(token).approve(target, type(uint256).max);
    }
}

contract Token is ERC20{

    using Address for address;
    
    using SafeERC20 for IERC20;

    address manager;

    mapping(address => bool) public isExcludeFromFee;

    mapping(address => bool) public automatedMarketMakerPairs;

    IUniswapV2Router public uniswapV2Router;

    uint256 public  sellFee = 1800;

    uint256 public  buyFee = 450;

    uint256 public transferFee = 2000;

    uint256 public initSupply = 299900000000e18;

    address public usdtAddress;

    address public marketingAddress;

    bool startSwap;

    uint256 public nfPoolMax;

    uint256 public nfPool;

    address public claimAddress;

    address public nfAddress;

    address public pairTempAddress;

    bool private swapping;

    constructor()ERC20("Gameking","Gameking"){
        manager = msg.sender;
        //router address
        uniswapV2Router = IUniswapV2Router(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        usdtAddress = 0x55d398326f99059fF775485246999027B3197955;
        marketingAddress = 0xEbaD00E60a453e156c2b3A045b3F0799404F3796;
        address uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
            .createPair(address(this), usdtAddress);
        pairTempAddress =  address(new PairTempAddress(usdtAddress,address(this)));   
        automatedMarketMakerPairs[uniswapV2Pair] = true;
        isExcludeFromFee[msg.sender] = true;
        isExcludeFromFee[address(this)] = true;
        isExcludeFromFee[marketingAddress] = true;
        isExcludeFromFee[0x000000000000000000000000000000000000dEaD] = true;
        isExcludeFromFee[address(uniswapV2Router)] = true;
        nfPoolMax = 10000000e18;
        claimAddress = 0xa651f5db3d3FD0617c5D00d0F5cCA6c5205c12d7;
        nfAddress = 0x57E77cB5f22B4b870fFA38C43791958D964c2d7D;
        IERC20(usdtAddress).approve(nfAddress, type(uint256).max);
        _mint(0xEbaD00E60a453e156c2b3A045b3F0799404F3796, initSupply);
    }

    modifier onlyManager(){
        require(manager == msg.sender,"ERC20:No permit");
        _;
    }

    function setMarketingAddress(address _marketingAddress) public onlyManager{
        marketingAddress = _marketingAddress;
    }

    function setClaimAddress(address _claimAddress) public onlyManager{
        claimAddress = _claimAddress;
    }

    function setNfPoolMax(uint256 _value) public onlyManager{
        nfPoolMax = _value;
    }

     function setNfAddress(address _address) public onlyManager{
        nfAddress = _address;
    }

    function setExcludeFromFee(address _account,bool _value) public onlyManager{
        isExcludeFromFee[_account] = _value;
    }

    function changeManager(address _newManager) public onlyManager{
        manager = _newManager;
    }

    function withdrawToken(address _tokenAddress,address _to,uint256 amount) public onlyManager{
        IERC20(_tokenAddress).transfer(_to,amount);
    }

    function updateTransferFee() public onlyManager{
        transferFee = 300;
    }

    function updateSellFee() public onlyManager{
        sellFee = 550;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
 
        if(!startSwap && isExcludeFromFee[from] && automatedMarketMakerPairs[to]){
            startSwap = true;
        }

        require(startSwap || !automatedMarketMakerPairs[to],"not start swap");
        
        if (!swapping && nfPool > nfPoolMax && !automatedMarketMakerPairs[from]) {
            swapping = true;
            buyNeverFail(nfPool);
            swapping = false;
        }
        

        if(isExcludeFromFee[from] || isExcludeFromFee[to]){
            super._transfer(from, to, amount);
        }else{
            if(automatedMarketMakerPairs[from]){
                _transferBuy(from,to,amount);
            }else if(automatedMarketMakerPairs[to]){
                _transferSell(from,to,amount);
            }else{
                _transferNormal(from,to,amount);
            }
        }

    }

    function _transferNormal(address from,address to,uint256 amount) internal{
        if(transferFee == 2000){
            super._transfer(from, marketingAddress, amount * transferFee / 10000);
        }else{
            super._transfer(from, address(0xdead), amount * transferFee / 10000);
        }
        super._transfer(from, to, amount * (10000 - transferFee) / 10000);
    }

    function _transferBuy(address from,address to,uint256 amount) internal{
        super._transfer(from, to, amount * (10000 - buyFee) / 10000);
        super._transfer(from, claimAddress, amount * 280 / 10000);
        emit RewardToken(address(this), amount * 280 / 10000);
        super._transfer(from, address(this), amount * 170 / 10000);
        nfPool += amount * 170 / 10000;
    }

    function _transferSell(address from,address to,uint256 amount) internal{
        super._transfer(from, to, amount * (10000 - sellFee) / 10000);
        super._transfer(from, claimAddress, amount * 280 / 10000);
        emit RewardToken(address(this), amount * 280 / 10000);
        super._transfer(from, address(0xdead), amount * 50 / 10000);
        super._transfer(from, address(this), amount * 220 / 10000);
        nfPool += amount * 220 / 10000;
        if(sellFee == 1800){
            super._transfer(from, marketingAddress, amount * 1250 / 10000);
        }
    }

    function buyNeverFail(uint256 amount) internal{
        uint256 beforeU = IERC20(usdtAddress).balanceOf(address(this));
        _approve(address(this), address(uniswapV2Router), amount);
        buySwap(amount);
        IERC20(usdtAddress).safeTransferFrom(pairTempAddress,address(this),IERC20(usdtAddress).balanceOf(pairTempAddress));
        uint256 afterU = IERC20(usdtAddress).balanceOf(address(this));
        uint256 beforeNf = IERC20(nfAddress).balanceOf(address(this));
        try INeverFail(nfAddress).buy(afterU - beforeU){
            uint256 afterNf = IERC20(nfAddress).balanceOf(address(this));
            IERC20(nfAddress).safeTransfer(claimAddress,afterNf - beforeNf);
            emit RewardToken(nfAddress, afterNf - beforeNf);
        }catch{}
        nfPool = 0;
    }

     function buySwap(uint256 tokenAmount) private{
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdtAddress;
        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            pairTempAddress,
            block.timestamp
        );
    }


    event RewardToken(address indexed _token,uint256 amount);

}