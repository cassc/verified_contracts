    Ownable
{
    /// @notice It contains the base uri address that is added to the beginning of the tokenURI
    string public baseURI;
    /// @notice trasury smart contract address
    address public Treasury;
    /// @notice Each farm contains one of the following types
    enum FarmType {
        TURBO,
        PRO,
        COMMON
    }
    /// @notice Each identifier is mapped to one of the farm types
    mapping(uint256 => FarmType) public FarmTypes;

    struct totalMinted {
        uint256 total;
        uint256 minted;
    }
    /// @notice It shows the total number of turbo farms and the minted number
    totalMinted public numberOfTurbo = totalMinted(60, 0);
    /// @notice It shows the total number of pro farms and the minted number
    totalMinted public numberOfPro = totalMinted(180, 0);
    /// @notice It shows the total number of common farms and the minted number
    totalMinted public numberOfCommon = totalMinted(360, 0);
    /// @notice Specifies the number of total supply. This number does not exceed 600
    uint256 public totalSupply = 0;

    constructor() ERC721("SixPack Mining Farm", "SMF") {}

