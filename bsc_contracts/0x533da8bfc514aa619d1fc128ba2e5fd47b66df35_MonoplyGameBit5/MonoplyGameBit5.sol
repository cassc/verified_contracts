    event ChanceToUser(address user, uint256 level);
    event NothingToUser(address user, uint256 level);
    event UnmintedCompany(address user, uint256 level);
    event MyOwnCompany(address user, uint256 level);
    event RentPaid(address owner,address user, uint256 tokenId, uint256 rent);

    constructor(
        address _bit5,
        address _authenticator,
        address _bit5Dice,
        address _bit5Companies
    ) public {
        BIT5 = _bit5;
        authenticator = _authenticator;
        bit5Dice = _bit5Dice;
        bit5Co = IBit5Companies(_bit5Companies);
    }
    function deposit(uint256 _amount) external nonReentrant {
        require(_amount > 0, "Deposit Should be Greater then zero");
        IERC20(BIT5).safeTransferFrom(address(_msgSender()), address(this), _amount);
        balance[_msgSender()] = balance[_msgSender()].add(_amount);
        deposits[_msgSender()] = deposits[_msgSender()].add(_amount);
    }

    function withdraw(uint256 _amount) external nonReentrant {
        require(
            balance[_msgSender()] >= _amount,
            "withdraw Amount Should be less then or equal to"
        );
        IERC20(BIT5).safeTransfer(address(_msgSender()), _amount);
        balance[_msgSender()] = balance[_msgSender()].sub(_amount);
        withdrawals[_msgSender()] = withdrawals[_msgSender()].add(_amount);
    }

    /**
    _data vars
     if contain 361 = punishment 
     if contain 362 = chance
     if contain 363 = empty  / nothing
     if contain 50000 = unminted company
     if contain any number from 1 to 360 means company id
     */
    function rollDice(
        uint256[] calldata _data,
        uint256[] calldata _tokenIds,
        bytes calldata signature,
        uint256 level
    ) external nonReentrant {
        require(Bit5Dices(bit5Dice).balanceOf(_msgSender()) >= 1, "Please mint dices");
        require(Bit5Dices(bit5Dice).ownerOf(_tokenIds[0]) == _msgSender() &&
                Bit5Dices(bit5Dice).ownerOf(_tokenIds[1]) == _msgSender() &&
                Bit5Dices(bit5Dice).ownerOf(_tokenIds[2]) == _msgSender(),"Please mint dices");
        require(recoverSigner(_data, level, signature) == authenticator, "wrong signature");
        uint256 DoBet = processBet();
        uint256 getCell = _data[DoBet - 1];
        if (getCell == 361) {
            // punishment
            punishment(level);
            burningTokens(_tokenIds, level);
        }
        if (getCell == 363) {
            // empty
            burningTokens(_tokenIds, level);
            emit NothingToUser(_msgSender(), level);
        }
        if (getCell <= 360) {
            // rent to company
            uint256 CompRent; 
            address CompOwner;
                (CompOwner, , CompRent, , ) = bit5Co.getCompanyInfo(getCell);
            if(CompOwner != msg.sender){
                IERC20(BIT5).safeTransfer(
                address(CompOwner),
               CompRent* 1e18
            );
            balance[_msgSender()] = balance[_msgSender()].sub(CompRent*1e18);
            withdrawals[_msgSender()] = withdrawals[_msgSender()].add(CompRent*1e18);
            emit RentPaid(address(CompOwner) ,_msgSender(), getCell, CompRent);
            } else {
                 emit MyOwnCompany(_msgSender(), level);
            }
          
            burningTokens(_tokenIds, level);
        }
        if (getCell == 50000) {
            // unminted company
            burningTokens(_tokenIds, level);
            emit UnmintedCompany(_msgSender(), level);
        }
        if (getCell == 362) {
            // on chance nothing will happen
            emit ChanceToUser(_msgSender(), level);
        }
    }

    function burningTokens(uint256[] calldata _tokenIds, uint256 level) internal {
        if (level == 1) {
            Bit5Dices(bit5Dice).burnDices(_tokenIds[0]);
        }
        if (level == 2) {
            Bit5Dices(bit5Dice).burnDices(_tokenIds[0]);
            Bit5Dices(bit5Dice).burnDices(_tokenIds[1]);
        }
        if (level == 3) {
            Bit5Dices(bit5Dice).burnDices(_tokenIds[0]);
            Bit5Dices(bit5Dice).burnDices(_tokenIds[1]);
            Bit5Dices(bit5Dice).burnDices(_tokenIds[2]);
        }
    }

    function punishment(uint256 level) internal {
        uint256 transferAmount =  level == 2 ? punishmentLevel2 : level == 3 ? punishmentLevel3 : punishmentLevel1;
        IERC20(BIT5).safeTransfer(address(owner()),transferAmount);
        balance[_msgSender()] = balance[_msgSender()].sub(transferAmount);
        withdrawals[_msgSender()] = withdrawals[_msgSender()].add(transferAmount);
        emit PunshimentToUser(_msgSender(), level);
    }

    function processBet() internal returns (uint256) {
        uint256 diceNumber_1 = generateRandomNumber(1);
        uint256 diceNumber_2 = generateRandomNumber(2);
        uint256 result = diceNumber_1.add(diceNumber_2);
        emit DiceRolled(rollId, diceNumber_1, diceNumber_2, result);
        rollId++;
        return result;
    }

    function recoverSigner(
        uint256[] calldata _data,
        uint256 _level,
        bytes calldata signature
    ) internal pure returns (address) {
        bytes32 hash = keccak256(abi.encodePacked(_data, _level));
        return hash.toEthSignedMessageHash().recover(signature);
    }

    function Random() external nonReentrant returns (uint256) {
        uint256 diceNumber_1 = generateRandomNumber(1);
        uint256 diceNumber_2 = generateRandomNumber(2);
        uint256 result = diceNumber_1.add(diceNumber_2);
        emit DiceRolled(rollId, diceNumber_1, diceNumber_2, result);
        return result;
    }

    function generateRandomNumber(uint256 seed) private view returns (uint256) {
        uint256 result = uint256(
            (uint256(
                keccak256(
                    abi.encodePacked(tx.origin, blockhash(block.number - 1), block.timestamp, seed)
                )
            ) % 6) + 1
        );
        return result;
    }

    function setTokenAddresses(address _token, address _bit5Dice) public onlyOwner {
        BIT5 = _token;
        bit5Dice = _bit5Dice;
    }

    function updateAuthenticator(address _authenticator) external virtual onlyOwner {
        authenticator = _authenticator;
    }
}